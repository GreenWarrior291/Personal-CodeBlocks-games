         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
