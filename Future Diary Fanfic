    cout << Diary[5] << " and " << Protagonist << " team up to attack " << attacked << " with a powerful attack." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << attacked << " managed to dodge the attack from both " << Diary[5] << " and " << Protagonist << "." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
        allypoints = allypoints - 15;
    }
    else if(attack != dodge)
    {
        superattack = strength * allystrength;
        cout << Protagonist << " and " << Diary[5] << " land a powerful love attack at " << attacked << "." << endl;
        cout << superattack << " damage!" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
        allypoints = allypoints - 15;
    }
}
else if(choice == 3 && choice == pat2)
{
    int object = 1, heal, direction;
    cout << " " << endl;
    cout << "Choose an item." << endl;
