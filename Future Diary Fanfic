int TFight (int& lvl, int armor, int& strength, int& points,string Protagonist)
{
        int enemy=100;
    int exp = 1;
    int attack, dodge, attack2, dodge2, action, action2;
    int going, going2;
    int turn = 0;
    const int MIN_VALUE = 1,
              MAX_VALUE = 10;

    unsigned seed = time(0);

    srand(seed);
cout << " " << endl;
cout << "Father: Alright son, remember that you must keep an eye on your endurance.  You don't want to have it too low or" << endl;
cout << "you'll be beaten badly." << endl;
cout << " " << endl;
cout << " " << endl;
cout << Protagonist << ": " << "Understood." << endl;
cout << " " << endl;
system("pause");
cout << " " << endl;
cout << "Father: Also you can skip your turn in a fight if you want but not really recommended." << endl;
cout << " " << endl;
cout << " " << endl;
cout << Protagonist << ": " << "Alright dad, I'll keep that in mind." << endl;
cout << "\n\n\n\n\n\n" << endl;
system("pause");
cout << "\n\n\n\n\n" << endl;
    do
    {
    cout << "\n\n" << endl;
    cout << Protagonist << " health: " << armor << endl;
    cout << "Father's health: " << enemy << endl;
    cout << " " << endl;
    cout << " " << endl;
int turning;
turning = rand() % 2;
going = 0;
going2 = 1;
        if (turning == going2)
        {
            cout << Protagonist << " turn." << endl;
            cout << " " << endl;
            cout << "1. Attack" << endl;
            cout << "2. skip" << endl;
            do
            {
                cin >> action;
            } while (action > 2 || action < 1);

            if (action = 1)
            {
                cout << Protagonist << " choose to attacks." << endl;
                    attack = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;
                dodge= (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;

                if(attack != dodge)
                {
                    attack = 2;
                    attack = attack*(strength/10);
                    enemy = enemy - attack;
                    points ++;
                    exp= exp +8;
                    cout << Protagonist << " managed to hit." << endl;
                    cout << attack << " damage" << endl;
                    cout << " " << endl;
                }
                else if (attack == dodge)
                {
                    cout << Protagonist << " missed." << endl;
                    cout << "No damage." << endl;
                    cout << " " << endl;
                }
            }
            else if (action == 2)
            {
                cout << Protagonist << " choose to skip his turn." << endl;
                cout << " " << endl;
            }
        }
        else if (going == turning)
 {
            cout << "Father" << " turn." << endl;
            cout << " " << endl;
            action2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;
            if (action2 > 5 || action2 < 7)
            {
                cout << "Father" << " choose to attack." << endl;
                    attack2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;
                dodge2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;

                if(attack2 != dodge2)
                {
                    attack2 = 5;
                    cout << "Father" << " managed to hit." << endl;
                    armor = armor-attack2;
                    cout << attack2 << " damage." << endl;
                    cout << " " << endl;
                }
                else if (attack2 == dodge2)
                {
                    cout << "Father" << " missed." << endl;
                    cout << "No damage." << endl;
                    cout << " " << endl;
                    points++;
                    exp++;
                }
            }
             else if (action2 > 7)
            {
                cout << "Father" << " choose to heavy attack." << endl;
                    attack2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;
                dodge2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;

                if(attack2 != dodge2)
                {
                    attack2 = 10;
                    cout << "Father" << " managed to hit." << endl;
                    armor = armor-attack2;
                    cout << attack2 << " damage."<< endl;
                    cout << " " << endl;
                }
                else if (attack2 == dodge2)
                {
                    cout << "Father" << " missed." << endl;
                    cout << "No damage." << endl;
                    cout << " " << endl;
                    exp = exp *2;
                }
            }
                else if (action2 < 5)
            {
                cout << "Father" << " choose to skip his turn." << endl;
                cout << " " << endl;
            }
        else if (going == going2)
        {
            cout << "Father and " << Protagonist << "attack each other but end" << endl;
            cout << "up countering each other." << endl;
            cout << " " << endl;
        }
    }
cout << " " << endl;
system("pause");
if (exp >= 100)
{
    exp = 1;
    lvl++;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;

}
cout << " " << endl;
system("pause");
cout << "\n\n\n\n\n\n" << endl;
turn++;
    } while (turn < 20);
    armor = 100;
    return armor;
}
int Fight (int& lvl, int armor, int& strength, int& points,string Protagonist)
{
    int enemy=200;
    int exp = 1;
    armor = armor + 100;
    int attack, dodge, attack2, dodge2, action, action2;
    int going, going2, turn;
    const int MIN_VALUE = 1,
              MAX_VALUE = 10;

    unsigned seed = time(0);

    srand(seed);

do
{
    cout << " " << endl;
    cout << Protagonist << " health: " << armor << endl;
    cout << "Yuno Gasai's health: " << enemy << endl;
    cout << " " << endl;
    cout << " " << endl;
        turn = rand()% 2;
        going2 = 1;
        going = 0;
        if (turn == going2)
        {
            cout << Protagonist << " turn." << endl;
            cout << " " << endl;
            cout << "1. Attack" << endl;
            cout << "2. skip" << endl;
            do
            {
                cin >> action;
            } while (action > 2 || action < 1);

            if (action = 1)
            {
                cout << Protagonist << " choose to attacks." << endl;
                    attack = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;
                dodge= (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;

                if(attack != dodge)
                {
                    attack = 2;
                    attack = attack*(strength/10);
                    enemy = enemy - attack;
                    points ++;
                    exp= exp +8;
                    cout << Protagonist << " managed to hit." << endl;
                    cout << attack << " damage" << endl;
                    cout << " " << endl;
                }
                else if (attack == dodge)
                {
                    cout << Protagonist << " missed." << endl;
                    cout << "No damage." << endl;
                    cout << " " << endl;
                }
            }
            else if (action == 2)
            {
                cout << Protagonist << " choose to skip his turn." << endl;
                cout << " " << endl;
            }
        }
        else if (going == turn)
 {
            cout << "Yuno Gasai" << " turn." << endl;
            cout << " " << endl;
            action2 = rand() % 2;
            if (action2 == 0)
            {
                cout << "Yuno Gasai" << " choose to attack." << endl;
                    attack2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;
                dodge2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;

                if(attack2 != dodge2)
                {
                    attack2 = 5;
                    cout << "Yuno Gasai" << " managed to hit." << endl;
                    armor = armor-attack2;
                    cout << attack2 << " damage." << endl;
                    cout << " " << endl;
                }
                else if (attack2 == dodge2)
                {
                    cout << "Yuno Gasai" << " missed." << endl;
                    cout << "No damage." << endl;
                    cout << " " << endl;
                    points++;
                    exp++;
                }
            }
             else if (action2 == 1)
            {
                cout << "Yuno Gasai" << " choose to heavy attack." << endl;
                    attack2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;
                dodge2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;

                if(attack2 != dodge2)
                {
                    attack2 = 10;
                    cout << "Yuno Gasai" << " managed to hit." << endl;
                    armor = armor-attack2;
                    cout << attack2 << " damage."<< endl;
                    cout << " " << endl;
                }
                else if (attack2 == dodge2)
                {
                    cout << "Yuno Gasai" << " missed." << endl;
                    cout << "No damage." << endl;
                    cout << " " << endl;
                    exp = exp *2;
                }
            }

    }
cout << " " << endl;
system("pause");
if (exp >= 100)
{
    exp = 1;
    lvl++;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;

}
cout << " " << endl;
if (enemy <= 50)
{
    armor = 100;
    return armor;
}
else if (armor <= 45)
{
    armor = 50;
    return armor;
}
} while(armor > 0);
}
int Fight2 (int& lvl, int& strength, int& armor, int& points, int& item, string Protagonist)
{
    int enemyteam;
    int ball = 0;
    int SuperAttack;
    int attack, dodge, attack2, dodge2, vic = 0,action, action2;
    int team1, team2;
    int health = armor * lvl;
    int exp = 1*lvl;
    int turnbase = 0;
    int enemies[10] = {50,60,25,75,50,30,20,70,70,70};
    const int MIN_VALUE = 1,
              MAX_VALUE = 10;

    unsigned seed = time(0);

    srand(seed);
    do
    {
            enemyteam = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4] + enemies[5] + enemies[6] + enemies[7] + enemies[8] + enemies[9];
    cout << "\n\n\n" << endl;
    cout << "Team 1: " << endl;
    cout << Protagonist << "'s health:\t\t" << health << "\tPoints:\t" << points << endl;
cout << " " << endl;
    cout << "Team 2: " << endl;
    for (int x = 0; x < 10; x++)
    {
    cout << "Cult person " << x << "'s health:\t" << enemies[x] << endl;
    }
    cout << " " << endl;
    cout << " " << endl;
    system("pause");
    cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;

                    team1 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 4)) +
