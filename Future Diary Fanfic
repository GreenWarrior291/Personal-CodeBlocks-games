        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << Diary[3] << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
