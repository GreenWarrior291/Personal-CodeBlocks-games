    cout << "\n\n\n\n" << endl;
    cout << " " << endl;
    system("pause");
    cout << "\n\n\n\n\n\n\n";
    cout << "Mirai Nikki: Another Path" << endl;
    cout << "Fanmade story by: GreenWarrior291" << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << " " << endl;
    do
 {
    cout << "1. Start New Game" << endl;
    cout << "2. Load game" << endl;
    cout << "3. Quit" << endl;
    cin >> game;
} while (game > 3 || game < 1);
if (game == 3)
{
return 0;
}
else if( game == 2)
{
loadfile(Checkpoint,Trust,load,enemyH,chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist,C1,C2,C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
}
else
lvl = 0;
chapter=0;
do
{
system("pause");
Chapter0(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
do
{
GaMe = 1;
if(chapter == 1)
{
Chapter1(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
for (int x = 0; x < 12; x++)
{
    if (DT[x]> 200)
    {
        DT[x] = 200;
    }
    else if (DT[x]<0)
    {
        DT[x] = 0;
    }
    else
    {
        DT[x];
    }
}
for(int y = 0; y < 5; y++)
{
    if(ST[y]> 200)
