int Fight1 (int& lvl, int& strength, int& armor, int& points, int& item, string Protagonist)
{
    int enemyteam;
    int ball = 0;
    int SuperAttack;
    int attack, dodge, attack2, dodge2, vic = 0,action, action2;
    int team1, team2;
    int health = armor * lvl;
    int exp = 1*lvl;
    int turnbase = 0;
    int enemies[10] = {50,60,25,75,50,30,20,70,70,70};
    const int MIN_VALUE = 1,
              MAX_VALUE = 10;

    unsigned seed = time(0);

    srand(seed);
    do
    {
            enemyteam = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4] + enemies[5] + enemies[6] + enemies[7] + enemies[8] + enemies[9];
