 cout << " " << endl;
 do
 {
     cin >> choice;
 } while(choice > maxi || choice < 1);
int superattack;
cout << "\n\n\n" << endl;

 if (choice == 1)
 {
     superattack = 2;
     cout << Protagonist << " chose to attack Cop " << choices+1 << "." << endl;
     cout << " " << endl;
