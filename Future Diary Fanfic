        cout << " " << endl;
        cout << " " << endl;

        if(choice == 1)
        {
            superattack = 6;
            cout << Supportive[4] << " goes to attack " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack != dodge)
            {
                cout << Supportive[4] << " landed a strong punch to " << attacked << "'s jaw." << endl;
                superattack = superattack * allystrength;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                allypoints++;
            }
            else if(dodge == attack)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
        }
        else if(choice == 2)
        {
            cout << Supportive[4] << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(choice == pat)
        {
            bullets--;
            superattack = 25;
            cout << Supportive[4] << " shoots his gun directly at " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand()%3;
            dodge = rand()%3;
            if(attack == dodge)
            {
                cout << Supportive[4] << " missed his shot." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack * allystrength;
                cout << Supportive[4] << " " << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
            }
        }
        else if(choice == pat1)
        {
            cout << Supportive[4] << " reloads his gun." << endl;
            cout << " " << endl;
            bullets = 6;
        }


            if(attacking == 0)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 1)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 2)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 4)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 5)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 6)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 7)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 8)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 9)
        {
            enemies[attacking] = enemy;
        }

    }



      }
      else if(going == 1)
      {
        do
        {
            going2 = rand() % fighting;
            if(going2 == Fighter1 && health > 0)
            {
                target = Protagonist;
                Ally = health;
                turning++;
            }
            else if(going2 == Fighter2 && Nishijima > 0)
            {
                target = Supportive[4];
                Ally = Nishijima;
                turning++;
            }
            else if(going2 == Fighter3 && Minene > 0)
            {
                target = Diary[9];
                Ally = Minene;
                turning++;
            }
            else if(going2 == Fighter4 && Mother > 0)
            {
                target = "Takeo's Mother";
                Ally = Mother;
                turning++;
            }
            else if(going2 == Fighter5 && Tsubaki > 0)
            {
                target = Diary[5];
                Ally = Tsubaki;
                turning++;
            }

        }while(turning == 1);
          turning = 1;

int random;
do
{
    random = rand() % 10;
    if(enemies[random] > 0)
    {
        if(random == 0)
        {
            attacked = "Enemy 1";
        }
        else if(random == 1)
        {
            attacked = "Enemy 2";
        }
        else if(random == 2)
        {
            attacked = "Enemy 3";
        }
        else if(random == 3)
        {
            attacked = "Enemy 4";
        }
        else if(random == 4)
        {
            attacked = "Enemy 5";
        }
        else if(random == 5)
        {
            attacked = "Enemy 6";
        }
        else if(random == 6)
        {
            attacked = "Enemy 7";
        }
        else if(random == 7)
        {
            attacked = "Enemy 8";
        }
        else if(random == 8)
        {
            attacked = "Enemy 9";
        }
        else if(random == 9)
        {
            attacked = "Enemy 10";
        }
    turning++;
    }
}while (turning == 1);
turning = 1;

cout << " " << endl;
cout << " " << endl;
cout << attacked << " points a gun at " << target << "." << endl;
cout << " " << endl;
attack = rand() % 3;
dodge = rand() % 3;
if(attack == dodge)
{
    cout << target << " managed to dodge " << attacked << " bullet." << endl;
    cout << "0 damage" << endl;
    cout << " " << endl;
}
else if(attack != dodge)
{
    cout << attacked << " landed a hit on " << target << endl;
    cout << "100 damage!" << endl;
    cout << " " << endl;
    Ally = Ally - 100;
}

            if(going2 == Fighter1)
            {
                health = Ally;
            }
            else if(going2 == Fighter2)
            {
                Nishijima = Ally;
            }
            else if(going2 == Fighter3)
            {
                Minene = Ally;
            }
            else if(going2 == Fighter4)
            {
                Mother = Ally;
            }
            else if(going2 == Fighter5)
            {
                Tsubaki = Ally;
            }

      }

         if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp;
    exp = 1;
    health = 100 * lvl/2;
    strength = strength + 8;
    allystrength = allystrength + 5;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
if(fighting == 5)
{
    allyteam = health + Tsubaki + Mother + Minene + Nishijima;
}
else if(fighting == 4)
{
    allyteam = health + Mother + Minene + Nishijima;
}
else if(fighting == 3)
{
    allyteam = health + Tsubaki + Nishijima;
}
else if(fighting == 2)
{
    allyteam = health + Nishijima;
}
enemyteam = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4] + enemies[5] + enemies[6] + enemies[7] + enemies[8] + enemies[9];
cout << "\n\n\n\n" << endl;
system("pause");
cout << "\n\n\n\n\n" << endl;
    }while(allyteam > 0 && enemyteam > 0);
    if(enemyteam <= 0)
    {
        armor = 200;
    }
    else if(allyteam <= 0)
    {
        armor = 25;
    }
    lvl = States(strength, Protagonist, lvl, armor, points);
    cout << "\n\n\n" << endl;
    return armor;
}

int DiaryFight7(int& armor, int& points, int& strength, int& item, int& injury, int& lvl, string Protagonist, string Diary[13])
{
    int health = armor * (lvl/2), Minene = injury, superattack, enemy, increase, timer = 0, attack, dodge, going, exp = 1, maxexp = 100;
    string target = Protagonist, attacked = Diary[9];

do
{
    going = rand() % 2;

        if(going == 0)
        {
        enemy = Minene;
        int choice, attacking, continue1 = 1;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    pat2 = x;
     cout << x << "." << " Items" << endl;
 }
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> choice;
        } while(choice > y || choice < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(choice == 1)
        {
            superattack = 2;
            cout << Protagonist << " goes to attack " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 4;
            dodge = rand() % 4;
            if(attack == dodge)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/4);
                cout << Protagonist << " lands a hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp++;
                points++;
            }
        }
        else if(choice == 2)
        {
            cout << Protagonist << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(choice == 3 && pat == choice)
        {
            superattack = 6;
            cout << Protagonist << " charges for a super strike on " <<  attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << attacked << " dodged the super attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                points = points - 5;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/2);
                cout << Protagonist << " lands a deadly hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp = exp + 5;
                points = points - 5;
            }
        }
        else if(choice == 3 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", she dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on  " << attacked << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7 * increase;
        cout << Protagonist << " goes to give a strong kick to " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << attacked << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20 * increase;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
        cout << Protagonist << " misses the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(attack != dodge)
    {
        superattack = superattack * (strength/2);
        cout << Protagonist << " lands a very deadly blow to " << attacked << "." << endl;
        cout << superattack << " damage" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 4)
{
    cout << Protagonist << " gains his ancestors strength before charging directly at " << attacked << "." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses and ends up getting fatigue." << endl;
        cout << " " << endl;
        health = health - 50;
        if(health <= 0)
        {
            health = 10;
        }
        points = points - 100;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " landed one hell of an attack." << endl;
        superattack = 10 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 5)
{
    cout << Protagonist << " goes and charges " << attacked << " with everything he got." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses his attack and gets countered by " << attacked << "." << endl;
        cout << "50 damage to " << Protagonist << "." << endl;
        cout << " " << endl;
        health = health - 50;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " land one deadly attack." << endl;
        superattack = 15 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " "<< endl;
        enemy = enemy - superattack;
    }
}
}
        else if(choice == 4 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
