            points = points - 500;
            exp = exp + 20;
        }
    }

     cout << " " << endl;
     if(timer <= 5 || timer >= 1)
     {
         timer--;
         increase = 1;

     }
     else if(timer > 5)
     {
         increase = 5;
     }


    }
    else if(going == 1)
    {
            cout << " " << endl;
