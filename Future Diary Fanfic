    int ST[5]; //Trust points for supportive casts
    int DT[13]; //The trust points, this regards to how each diary owner treats you.
    int strength=0; //The main characters strength
    int armor = 100; //The main characters endurance and if it goes to 0, it's a dead end
    int lvl; //Level of player (affects the strength of MC and allies.)
    int path; //gives player a choice
    int GaMe; //End of game if player wants to restart the game.
    string Protagonist = "Takeo Aiwaka"; //Main Character Male
srand(seed);
    cout << "Disclaimer: " << endl;
    cout << " " << endl;
    cout << "Mirai Nikki (also known as Future Diary) is created and" << endl;
    cout << "Licensed by: AUS Madman EntertainmentNA Funimation, UK KazÃ© UK." << endl;
