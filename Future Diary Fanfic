    {
                    Yuno = (rand() %
                 (MAX_VALUE - MIN_VALUE)) +
                  MIN_VALUE;
                      Takeo = (rand() %
                 (MAX_VALUE - MIN_VALUE)) +
                  MIN_VALUE;
            if(Yuno == Takeo)
            {
        cout << "Cult person " << persons << " chooses to attack with their weapon at both " << Protagonist << " and " <<  Diary[1] << endl;
        cout << " " << endl;
                          dodge = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
