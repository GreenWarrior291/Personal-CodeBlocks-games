    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5;
        cout << Protagonist << " goes to strike down " << Diary[4] << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << Diary[4] << ", he dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[4] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 exp = exp + lvl;
                 DiaryDestroyer = DiaryDestroyer + 20;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << Diary[4] << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit " << Diary[4] << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit " << Diary[4] << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7;
        cout << Protagonist << " goes to give a strong kick to " << Diary[4] << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << Diary[4] << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }

}

else if (choice == 3 && chosen2 == choice)
{
    cout << Protagonist << " chooses to use an item." << endl;
    cout << " " << endl;
    cout << "He heals himself and gains more breath." << endl;
    cout << " " << endl;
    breath = breath + 20;
    health = health + 50;

}

else if (choice == 4 && chosen2 == choice)
{
    cout << Protagonist << " chooses to use an item." << endl;
    cout << " " << endl;
    cout << "He heals himself and gains more breath." << endl;
    cout << " " << endl;
    breath = breath + 20;
    health = health + 50;

}

else if (choice == 5 && chosen2 == choice)
{
    cout << Protagonist << " chooses to use an item." << endl;
    cout << " " << endl;
    cout << "He heals himself and gains more breath." << endl;
    cout << " " << endl;
    breath = breath + 20;
    health = health + 50;

}
else if (choice == 3 && chosen3 == choice)
{
    cout << Protagonist << " goes to destroy " << Diary[4] << "'s diary." << endl;
    cout << " " << endl;
    attack = rand() % 2;
    dodge = rand() % 2;
    if (attack == dodge)
    {
        cout << Diary[4] << " managed to get his diary to safety!" << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
        DiaryDestroyer = DiaryDestroyer - 1000;
    }
    else if (attack != dodge)
    {
        cout << Protagonist << " manages to damage the diary." << endl;
        cout << "10000000000000000000000000000000000000 damage!" << endl;
        cout << " " << endl;
        exp = exp * 100;
        points = points * 10;
        item = item + 1;
        enemy = 0;
    }
}
}
else if (going == 0)
{
cout << Diary[4] << "'s turn" << endl;
cout << " " << endl;
int action;
action = rand() % 2;
if (action == 0)
{
    cout << Diary[4] << " chooses to attack " << Protagonist << "." << endl;
    cout << " " << endl;
    attack = rand() % 4;
    dodge = rand() % 4;
    if (attack == dodge)
    {
        cout << Diary[4] << " misses his attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
        exp++;
        points++;
        DiaryDestroyer = DiaryDestroyer + 10;
    }
    else if (attack != dodge)
    {
        cout << Diary[4] << " manages to land a hit on " << Protagonist << "." << endl;
        if (breath > 0)
        {
            cout << "10 damage!" << endl;
            health = health - 10;
        }
        else if (breath <= 0)
        {
            cout << "20 damage!" << endl;
            health = health - 20;
        }
        cout << " " << endl;
    }
}
else if (action == 1)
{
 cout << Diary[4] << " chooses to harm " << Protagonist << " with his knife." << endl;
 cout << " " << endl;
 attack = rand() % 3;
 dodge = rand() % 3;
 if (attack == dodge)
 {
     cout << Protagonist << " manages to dodge the attack and kick " << Diary[4] << " back." << endl;
     cout << "25 damage to " << Diary[4] << endl;
     cout << " " << endl;
     enemy = enemy - 25;
     exp = exp + 20;
     DiaryDestroyer = DiaryDestroyer + 30;
     points = points + 10;
 }
 else if (attack != dodge)
 {
     cout << Diary[4] << " does a serious blow to " << Protagonist << "." << endl;
     cout << "39 damage!" << endl;
     cout << " " << endl;
     health = health - 39;
 }
}

}

breath--;
if (breath == 1)
{
    cout << " " << endl;
    cout << " " << endl;
    system("pause");
    cout << " " << endl;
    cout << Protagonist << ": " << "(Crap!  I'm running out of air to hold!)" << endl;
    cout << " " << endl;
    cout << Diary[4] << ": "<< "Hahahaha!  How will you survive once you've run out of air." << endl;
    cout << " " << endl;
    cout << Protagonist << ": " << "(I better finish this, quick!)" << endl;
    cout << " " << endl;
}
else if (breath <= 0)
{
    breath = 0;
    health = health - 2;
}

if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp * lvl;
    exp = 1 * lvl;
    health = 100;
    strength = strength + 5;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
cout << " " << endl;
system("pause");
if (health <= 0)
{
    armor = health;
    return armor;
}
else if (enemy <=0)
{
    armor = health;
    return armor;
}
}while (victor == 1);

}

int DiaryFight4(int& strength, int& armor, int& lvl, int& item, int& points, int& victor, string Protagonist, string Diary[13])
{
    int health = armor;
    int exp = 1;
    int enemy = 550;
    int maxexp = 100;
    int gun = 6, reload = 0, bullets = 20;
    int attack, dodge, going, Takeo = 0, Kurusu = 1;

    do
    {
        cout << "\n\n\n\n\n\n" << endl;
        cout << Protagonist << "'s health:\t" << health << "\tpoints:\t\t" << points << "\titem:\t" << item << "\tgun:\t\t" << gun << endl;
        cout << " " << endl;
        cout << Diary[3] << "'s health:\t" << enemy << endl;
        cout << "\n\n\n\n\n" << endl;
        going = rand() % 2;
    if (going == Takeo)
    {
int choice, chosen, chosen1, chosen2, chosen3,chosen4;
{
 cout << Protagonist << " turn." << endl;
 cout << "\n\n\n" << endl;
 int maxi = 2, x = 1;
 cout << x << "." << "  Attack." << endl;
 x++;
 cout << x << "." << " Skip turn" << endl;
 if (points >= 5)
 {
     x++;
     chosen = x;
     maxi++;
     cout << x << "." << " Super Attack \t5" << endl;
 }
 else if (lvl >= 6)
 {
     x++;
     chosen1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    chosen2 = x;
     cout << x << "." << " Items\t1" << endl;
 }
 else if(gun >= 1)
 {
     x++;
     maxi++;
     chosen3 = x;
     cout << x << "." << " Shooting the gun\t" << gun << endl;
 }
 else if (gun < 6  && bullets > 0)
 {
     x++;
     maxi++;
     chosen4 = x;
     cout << x << "." << " Reload\t" << reload << endl;
 }
 cout << " " << endl;
 cout << " " << endl;
 do
 {
     cin >> choice;
 } while(choice > maxi || choice < 1);
int superattack;
cout << "\n\n\n" << endl;

 if (choice == 1)
 {
     superattack = 2;
     cout << Protagonist << " chose to attack " << Diary[3] << "." << endl;
     cout << " " << endl;
     attack = rand() % 5;
     dodge = rand() % 5;
     if (attack == dodge)
     {
         cout << "Sadly " << Diary[3] << " dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[3] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 exp = exp + lvl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << Diary[3] << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
     }
     else if (attack != dodge)
     {
         superattack = superattack*(strength/10);
         cout << Protagonist << " lands an attack on " << Diary[3] << "." << endl;
         cout << superattack << " damage!" << endl;
         cout << " " << endl;
         enemy = enemy - superattack;
                 exp = exp + lvl;
                 points++;
     }
      }
else if (choice == 2)
{
    cout << Protagonist << " chooses to skip his turn." << endl;
    cout << " " << endl;
}

else if (choice == 3 && chosen==choice)
{
superattack = 4;
cout << Protagonist << " chooses to do a very powerful attack at " << Diary[3] << endl;
cout << " " << endl;
attack = rand() % 4;
dodge = rand() % 4;
if(attack == dodge)
{
 cout << "Lucky for " << Diary[3] << ", it managed to dodge the attack, barely." << endl;
    cout << "0 damage." << endl;
    points = points - 5;
}
else if (attack != dodge)
{
    superattack = superattack*(strength/5);
    cout << Protagonist << " landed a strong blow at " << Diary[3] << "." << endl;
    cout << superattack << " damage!" << endl;
    cout << " " << endl;
    enemy = enemy - superattack;
    exp = exp + (lvl*2);
    points = points - 5;
}

}
else if (choice == 3 && chosen1 == choice)
{
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5;
        cout << Protagonist << " goes to strike down " << Diary[3] << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << Diary[3] << ", he dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[3] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 exp = exp + lvl;
                 points++;
             }
