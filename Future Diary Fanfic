    }while (battle > 0);



    }

int DogFight2A(int& lvl, int& armor, int& strength, int& points, int& item, string Protagonist, string Supportive[5], string Diary[13])
{
    int exp = 1;
    int enemies[20] = {50,50,30,20,70,70,70,100,85,31,50,50,30,20,70,70,70,100,85,31};
    int Ouij = 100;
    int Tsubaki = 50;
    int Akise = 500;
    int health = armor;
    int multi;
    int battle = 1;
    int teamhealth;
    int enemyteam;
    int allypoints = 250;
    int AI1 = 7;
    int AI2 = 3;
    int AI3 = 10;
    int attack, dodge, TH1=0, TH2 = 0, OH = 0, AH = 0,shield = 0, action, action2, SuperAttack;
    int going, teamturn, team1 = 0, team2 = 1, turn;
    const int MIN_VALUE = 1,
              MAX_VALUE = 10;

    unsigned seed = time(0);


    srand(seed);
   do
    {
    teamhealth = health + Akise + Ouij + Tsubaki;
    enemyteam = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4] + enemies[5] + enemies[6] + enemies[7] + enemies[8] + enemies[9] + enemies[10] + enemies[11] + enemies[12] + enemies[13] + enemies[14] + enemies[15] + enemies[16] + enemies[17] + enemies[18] + enemies[19];
    cout << "\n\n\n" << endl;
    cout << "Team 1: " << endl;
    cout << Protagonist << "'s health:\t\t" << health << "\t\tPoints:\t" << points << endl;
    cout << Supportive[0] << "'s health:\t\t" << Akise << "\t\tPoints:\t" << allypoints << "\tItems:\t" << AI1 << endl;
    cout << Supportive[3] << "'s health:\t\t" << Ouij << "\t\tPoints:\t" << allypoints << "\tItems:\t" << AI2 << endl;
    cout << Diary[5] << "'s health:\t\t" << Tsubaki << "\t\tPoints:\t" << allypoints << "\tItems:\t" << AI3 << endl;
    cout << " " << endl;
    cout << "Team 2: " << endl;
    for (int x = 0; x < 20; x++)
    {
        if(enemies[x] > 0)
    {
    cout << "Dog " << x+1 << "'s health:\t" << enemies[x] << endl;
    }
    }
    cout << " " << endl;
    cout << " " << endl;
    system("pause");
    cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;

    teamturn = rand() % 2;


    if (team2 == teamturn)
 {
     cout << " " << endl;
     int Takeoh = 1, Akiseh = 2, Ouijh = 3, Tsubakih = 4, attacking, attacker, continu1, continu2;
     continu1 = 0;
     continu2 = 0;
     do
     {
         attacking = rand() % 4;
        if(attacking == 0 && health > 0)
        {
            continu1 = 1;
        }
        else if(attacking == 1 && Akise > 0)
        {
            continu1 = 2;
        }
        else if(attacking == 2 && Ouij > 0)
        {
            continu1 = 3;
        }
        else if(attacking == 3 && Tsubaki > 0)
        {
            continu1 = 4;
        }
     } while (continu1 == 0);
     cout << " " << endl;
     do
     {
         attacker = rand() % 20;
         if(enemies[attacker] > 0)
         {
             continu2 = attacker+1;
         }
     } while (continu2 == 0);
     cout << "Dog " << continu2 << " goes off to attack ";
     if(continu1 == Takeoh)
     {
         cout << Protagonist;
     }
     else if(continu1 == Ouijh)
     {
         cout << Supportive[3];
     }
     else if(continu1 == Akiseh)
     {
         cout << Supportive[0];
     }
     else if(continu1 == Tsubakih)
     {
         cout << Diary[5];
     }
     cout << "." << endl;
     attack = rand() % 5;
     dodge = rand() % 5;
     if(attack == dodge)
     {
         cout << "Dog " << continu2 << " misses." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         if(continu1 == Takeoh)
         {
             points++;
             exp++;
         }
         else if(Akiseh == continu1 || continu1 == Ouijh || continu1 == Tsubakih)
         {
             allypoints = allypoints + 2;
         }
     }
     else if(attack != dodge)
     {
         cout << "Dog " << continu2 << " takes a bite on ";
              if(continu1 == Takeoh)
     {
         cout << Protagonist;
     }
     else if(continu1 == Ouijh)
     {
         cout << Supportive[3];
     }
     else if(continu1 == Akiseh)
     {
         cout << Supportive[0];
     }
     else if(continu1 == Tsubakih)
     {
         cout << Diary[5];
     }
     cout << "." << endl;

    if(continu1 == Takeoh)
     {
         if(shield < TH1)
         {
             cout << "Yet a shield is protecting " << Protagonist << endl;
             cout << "0 Damage." << endl;
             cout << " " << endl;
             TH1 = TH1 - 1;
         }
         else if (TH1 == 0)
         {
                     health = health - 10;
             cout << "10 damage." << endl;
             cout << " " << endl;
         }
     }
     else if(continu1 == Ouijh)
     {
         if(shield < OH)
         {
             cout << "Yet a shield is protecting " << Supportive[3] << endl;
             cout << "0 Damage." << endl;
             cout << " " << endl;
             OH = OH - 1;
         }
         else if (OH == 0)
         {
            Ouij = Ouij - 10;
             cout << "10 damage." << endl;
             cout << " " << endl;
         }
     }
     else if(continu1 == Akiseh)
     {
         if(shield < AH)
         {
             cout << "Yet a shield is protecting " << Supportive[0] << endl;
