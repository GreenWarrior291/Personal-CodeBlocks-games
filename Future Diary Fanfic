     else if(choice == 3 && choice == pat2)
     {
         int let;
         if(timer == 0 && item >= 5)
         {
             cout << " " << endl;
             cout << "What would you like to do?" << endl;
             cout << " " << endl;
             cout << "1. Heal.       1" << endl;
             cout << "2. Strength.   5" << endl;
             cout << "\n\n" << endl;
             do
             {
                 cin >> let;
             } while(let > 2 || let < 1);
         }
         else if(timer != 0 || item < 5)
         {
             let = 1;
         }
         cout << "\n\n" << endl;
         if(let == 1)
         {
             item--;
             cout << Protagonist << " heals himself." << endl;
             cout << "His health was " << health;
             health = health + 95;
             cout << ", now it is " << health << "." << endl;
             cout << " " << endl;
         }
         else if(let == 2)
         {
             item = item - 5;
             cout << Protagonist << " increased his strength!" << endl;
             cout << " " << endl;
             increase = 5;
             timer = 21;
         }
     }
