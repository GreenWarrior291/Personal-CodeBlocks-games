int Chapter4(int Trust,std::string& load, int enemyH,int& chapter, int& friends, int& love, int& item, int& points, int allyH, string Gods[2], string Diary[13], string Supportive[5], string Protagonist,int C1[4], int C2[5], int C3[5], int C4[5], int C5[5], int C6[2], int DT[13], int ST[5], int& strength, int& armor, int& lvl, int path)
{
        love2 = 0;
        saved = 0;
        saved1 = 0;
        saved2 = 2;
if(Checkpoint == 0)
{
do
{
    cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;
    cout << "Chapter 4: Struggle of love" << endl;
    cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
    cout << Gods[0] << ": " << "(Murmur, you are foolish for trying to go behind my back.  That is why I had Aiwaka Takeo created to assure things go right...speaking of that, let us see what you've done so far.)" << endl;
    cout << " " << endl;
    if(C3[0]==1)
    {
     cout << Gods[0] << ": " << "(The fourth betrayed you and pointed you as an enemy.)" << endl;
     if(love == 1 || friends == 1)
     {
         cout << " " << endl;
         cout << Gods[0] << ": " << "(And sadly for you my creation, the sixth was killed.)" << endl;
     }
     else if(love == 2 || friends == 2)
     {
         cout << " " << endl;
         cout << Gods[0] << ": " << "(Interesting enough, you managed to still keep the sixth alive.  I'm not unsure on how to react to this put I feel like this might play out an interesting path.)" << endl;
     }
    }
    else if(C3[0]==2)
    {
        cout << Gods[0] << ": " << "(The fourth saw you as an ally despite knowing that you would turn against him.)" << endl;
        if(love == 2 || friends == 2)
        {
            cout << " " << endl;
            cout << Gods[0] << ": " << "(Although this might have saved the sixth, seeing that she might play an important role later on in your journey.)" << endl;
        }
    }
    cout << " " << endl;
    system("pause");
    cout << "\n\n\n" << endl;
    if(C3[1]==1)
    {
     cout << Gods[0] << ": " << "(You volunteered to go and scout which eventually lead the First to stop the Fourth from harming the Second.)" << endl;
    }
    else if(C3[1]==2)
    {
        cout << Gods[0] << ": " << "(You let the first go on his own which lead to you being the one to stop the fourth from harming your ally.)" << endl;
    }
    cout << "\n\n\n" << endl;
    system("pause");
    cout << "\n\n\n\n" << endl;
    if(C3[2]==1)
    {
        cout << Gods[0] << ": " << "(You attempted to deliver the message to the first yet the second figured it out what you were doing.)" << endl;
    }
    else if(C3[2]==2)
    {
        cout << Gods[0] << ": " << "(You alerted the Second of what was going on, which increased your friendship with her but at a cost.)" << endl;
    }
    cout << " " << endl;
    cout << " " << endl;
    system("pause");
    cout << "\n\n\n" << endl;
    if(C3[3]==1)
    {
        cout << Gods[0] << ": " << "(You decided to take on the second after having followed the message with the group.)" << endl;
    }
    else if(C3[3]==2)
    {
        cout << Gods[0] << ": " << "(You allowed " << Supportive[3] << " to go save you all knowing that the second could've killed him.)" << endl;
    }
    else if(C3[3]==3)
    {
        cout << Gods[0] << ": " << "(You were captured by the second yet you still challenged her to a fight.)" << endl;
    }
    else if(C3[3]==4)
    {
        cout << Gods[0] << ": " << "(You were captured by the second and didn't choose to fight her.)" << endl;
    }
    cout << "\n\n\n" << endl;
    cout << Gods[0] << ": " << "(And in the end you";
    if(C3[4]==1)
    {
        cout << " leave the second behind.)" << endl;
    }
    else if(C3[4]==2)
    {
        cout << " bring the second along.)" << endl;
    }
    cout << " " << endl;
    cout << Gods[0] << ": " << "(You've kept the game balance from what MurMur is doing but be careful, things will not be easy from here on out.)" << endl;
    cout << " " << endl;
    cout << " " << endl;
    system("pause");
    Checkpoint++;
} while(Checkpoint == 0);

}
