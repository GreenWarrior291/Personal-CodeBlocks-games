cout << "\n\n\n\n\n\n" << endl;
cout << "Choose either 1 or 2." << endl;
cout << "\n\n" << endl;
do
{
    cin >> attacking;
} while(attacking > 2 || attacking < 1);
attacking--;
random = rand() % 2;
    if(random == attacking)
    {
cout << "Takeo and Yuno both attempted to attack the robot yet the robot escaped" << endl;
cout << "0 damage!" << endl;
cout << " " << endl;
cout << " " << endl;
