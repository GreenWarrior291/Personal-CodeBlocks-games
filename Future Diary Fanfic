chapter = 3;
savefile(Checkpoint,Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
