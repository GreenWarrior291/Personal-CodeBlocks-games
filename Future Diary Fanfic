int strong = 0, stronger = 1, skills = 2, choosing;
if(enemypoints >= 10)
{
    choosing = rand() % 3;
}
else if(enemypoints <= 9)
{
    choosing = rand() % 2;
}
cout << " " << endl;
if(choosing == strong)
{
    SuperAttack = 2;
    cout << Diary[1] << " chooses to attack " << Protagonist << "." << endl;
    cout << " " << endl;
    attack = rand() % 6;
    dodge = rand() % 6;
    if(dodge == attack)
    {
        cout << Protagonist << " managed to dodge the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(attack != dodge)
    {
        cout << Diary[1] << " managed to hit " << Protagonist << "." << endl;
        SuperAttack = SuperAttack * (Yuki * 5);
        cout << SuperAttack << " damage!" << endl;
        cout << " " << endl;
        health = health - SuperAttack;
        enemypoints++;
    }

}
else if (choosing == stronger)
{
    SuperAttack = 4;
    cout << Diary[1] << " chooses to give a deadly attack towards " << Protagonist << "." << endl;
    cout << " " << endl;
    attack = rand() % 5;
    dodge = rand() % 5;
    if(dodge == attack)
    {
        cout << Diary[1] << " misses" << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(dodge != attack)
    {
        cout << Diary[1] << " lands a deadly blow to " << Protagonist << "." << endl;
        SuperAttack = SuperAttack * (Yuki * 7);
        cout << SuperAttack << " damage!" << endl;
        cout << " " << endl;
        enemypoints = enemypoints + 5;
        health = health - SuperAttack;
    }
}
else if(choosing == skills)
{
    int chance = 0, chance2 = 1, chance3 = 2, choices;
    if(strength2 > 0)
    {
        choices = rand() % 2;
    }
    else if(strength2 < 1)
    {
        choices = rand() % 3;
    }
    cout << " " << endl;
    if(choices == chance)
    {
        int damage;
        cout << Diary[1] << " charges at " << Protagonist << " with her axe." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if(attack == dodge)
        {
            cout << Diary[1] << " misses her axe attack." << endl;
            cout << "0 damage." << endl;
            cout << " " << endl;
            enemypoints = enemypoints - 45;
        }
        else if(attack != dodge)
        {
            cout << Diary[1] << " lands a horrible blow at " << Protagonist << "." << endl;
            damage = 0;
            damage = damage + 60*Yuki;
            cout << damage << " damage!" << endl;
            cout << " " << endl;
            health = health - damage;
        }
    }
    else if(choices == chance2)
    {
        int damage;
        cout << Diary[1] << " charges towards " << Protagonist << " with a knife." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if(attack == dodge)
        {
            cout << Protagonist << " manages to dodge " << Diary[1] << "'s attack and quickly." << endl;
            cout << "0 damage" << endl;
            cout << " " << endl;
            enemypoints = enemypoints - 75;
        }
        else if(attack != dodge)
        {
            cout << Diary[1] << " lands a jab at " << Protagonist << "." << endl;
            damage = 0;
            damage = damage + 75*Yuki;
            cout << damage << " damage!" << endl;
            cout << " " << endl;
            enemypoints = enemypoints - 75;
        }
    }
    else if(choices == chance3)
    {
        cout << Diary[1] << " increases her strength for her Yukki." << endl;
        cout << " " << endl;
        Yuki = 3;
        strength2 = 7;
    }
}
            if(strength2 > 0)
        {
            strength2--;
        }
        else if(strength2 < 1)
        {
            Yuki = 1;
        }
    }
    if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp * lvl;
    exp = 1 * lvl;
    health = 25 * lvl;
    strength = strength + 5;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
cout << " " << endl;
system("pause");
    if(health < 1)
    {
        armor = health;
        return armor;
    }
    } while(enemy > 1);

armor = health;
lvl = States(strength, Protagonist, lvl, armor, points);
return armor;

}

int PoliceFight(int& strength, int& armor, int& lvl, int& item, int& points, int& victor, string Protagonist)
{
    int health = armor;
    int exp = 1;
    int maxexp = 100;
    int enemy, team;
    int enemies[5] = {100,105,150,100,125};
    int gun = 6, reload = 0, bullets = 20;
    int attack, dodge, going, Takeo = 0, cop = 1;
        do
    {
        cout << "\n\n\n\n\n\n" << endl;
        cout << Protagonist << "'s health:\t" << health << "\tpoints:\t\t" << points << "\titem:\t" << item << "\tgun:\t\t" << gun << endl;
        cout << " " << endl;
for(int c = 0; c < 5; c++)
{
    cout << "Cop " << c+1 << "'s health:\t" << enemies[c] << endl;
}
        cout << "\n\n\n\n\n" << endl;
        going = rand() % 2;
    if (going == Takeo)
    {
int choice, choices, chosen, chosen1, chosen2, chosen3,chosen4, MAX = 5;
{
    cout << " " << endl;
    cout << " " << endl;
for (int x = 0; x < 20; x++)
{
    enemies[x];
    if (enemies[x] > 0)
    {
        cout << x << "." << "Cop " << x+1 << " " << "health:\t\t\t" << enemies[x] << endl;
    }
    else if (enemies[x] < 1)
    {
        cout << x << "." << "Cop " << x+1 << " " << "health:\t\t\t" << "0" << endl;
    }
}
cout << " " << endl;
cout << " " << endl;
do
{
    cin >> choices;
    if (enemies[choices] < 1)
{
cout << "Choose an opponent that is available." << endl;
cout << " " << endl;
    cin >> choices;
}
} while (choices < 0 && choices > MAX);
choices = choices - 1;
enemy = enemies[choices];
 cout << Protagonist << " turn." << endl;
 cout << "\n\n\n" << endl;
 int maxi = 2, x = 1;
 cout << x << "." << "  Attack." << endl;
