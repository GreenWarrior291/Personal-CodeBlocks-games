    cout << "The destination was..." << endl;
    cout << "\n\n\n" << endl;
    system("pause");
}



}while(victor==0 && turn <= 50);

return armor;


}


int GodFight(int& maximumturns, int& strength, int& lvl, int& item, int& points, int& armor, string Protagonist, string Diary[13], string Gods[2])
{
      int health = armor * lvl, Yuno = 100000, MurMur = 10000000, enemyteam, superattack, enemy, increase, timer = 0, attack, dodge, going, turn = 0, exp = 1, maxexp = 100;
    string target = Protagonist, attacked;

    do
    {
        cout << "\n\n\n\n" << endl;
        cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titems:\t" << item << endl;
        cout << "\n\n\n" << endl;
        cout << Diary[1] << "'s health:\t" << Yuno << "." << endl;
        cout << Gods[1] << "'s health:\t" << MurMur << "." << endl;
        cout << "\n\n\n" << endl;
        going = rand() %2;

        if(going == 0)
        {
        int after;
        cout << " " << endl;
        if(Yuno > 0 && MurMur > 0)
        {
            cout << "Who do you choose to attack?" << endl;
            cout << " " << endl;
            cout << "1. " << Diary[1] << "." << endl;
            cout << "2. " << Gods[1] << "." << endl;
            cout << "\n\n" << endl;
            do
            {
                cin >> after;
            } while(after > 2 || after < 1);
            cout << " " << endl;
            after--;
        }
        else if(Yuno <= 0)
        {
            after = 1;
        }
        else if(MurMur <= 0)
        {
            after = 0;
        }
        cout << " " << endl;
        if(after == 0)
        {
            enemy = Yuno;
            attacked = Diary[1];
        }
        else if(after == 1)
        {
            enemy = MurMur;
            attacked = Gods[1];
        }
        int choice, attacking, continue1 = 1;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, God, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if(points >= 500)
 {
     x++;
     God = x;
     maxi++;
     cout << x << "." << "Divine attack.\t500" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    pat2 = x;
     cout << x << "." << " Items" << endl;
 }
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> choice;
        } while(choice > y || choice < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(choice == 1)
        {
            superattack = 2;
            cout << Protagonist << " goes to attack " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 4;
            dodge = rand() % 4;
            if(attack == dodge)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/4);
                cout << Protagonist << " lands a hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp++;
                points++;
            }
        }
        else if(choice == 2)
        {
            cout << Protagonist << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(pat == choice)
        {
            superattack = 6;
            cout << Protagonist << " charges for a super strike on " <<  attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << attacked << " dodged the super attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                points = points - 5;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/2);
                cout << Protagonist << " lands a deadly hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp = exp + 5;
                points = points - 5;
            }
        }
        else if(choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", she dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on  " << attacked << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7 * increase;
        cout << Protagonist << " goes to give a strong kick to " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << attacked << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20 * increase;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
        cout << Protagonist << " misses the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(attack != dodge)
    {
        superattack = superattack * (strength/2);
        cout << Protagonist << " lands a very deadly blow to " << attacked << "." << endl;
        cout << superattack << " damage" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 4)
{
    cout << Protagonist << " gains his ancestors strength before charging directly at " << attacked << "." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses and ends up getting fatigue." << endl;
        cout << " " << endl;
        health = health - 50;
        if(health <= 0)
        {
            health = 10;
        }
        points = points - 100;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " landed one hell of an attack." << endl;
        superattack = 10 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 5)
{
    cout << Protagonist << " goes and charges " << attacked << " with everything he got." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses his attack and gets countered by " << attacked << "." << endl;
        cout << "50 damage to " << Protagonist << "." << endl;
        cout << " " << endl;
        health = health - 50;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " land one deadly attack." << endl;
        superattack = 15 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " "<< endl;
        enemy = enemy - superattack;
    }
}
}
        else if(choice == pat2)
     {
         int let;
         if(timer == 0 && item >= 5)
         {
             cout << " " << endl;
             cout << "What would you like to do?" << endl;
             cout << " " << endl;
             cout << "1. Heal.       1" << endl;
             cout << "2. Strength.   5" << endl;
             cout << "\n\n" << endl;
             do
             {
                 cin >> let;
             } while(let > 2 || let < 1);
         }
         else if(timer != 0 || item < 5)
         {
             let = 1;
         }
         cout << "\n\n" << endl;
         if(let == 1)
         {
             item--;
             cout << Protagonist << " heals himself." << endl;
             cout << "His health was " << health;
             health = health + 200;
             cout << ", now it is " << health << "." << endl;
             cout << " " << endl;
         }
         else if(let == 2)
         {
             item = item - 5;
             cout << Protagonist << " increased his strength!" << endl;
             cout << " " << endl;
             increase = 5;
             timer = 21;
         }
     }
    else if(choice == God)
    {
        cout << " " << endl;
        cout << Protagonist << " with the power of " << Gods[0] << " charges at " << attacked << " with divine power." << endl;
        cout << " " << endl;
        attack = rand() % 2;
        dodge = rand() % 2;
        if(attack == dodge)
        {
            cout << Protagonist << " misses his attack and loses some of his strength." << endl;
            cout << "250 damage to " << Protagonist << "." << endl;
            cout << " " << endl;
            points = points - 500;
            health = health - 250;
        }
        else if(attack != dodge)
        {
            cout << Protagonist << " lands a godly damaging blow to " << attacked << "." << endl;
            superattack = (strength*strength) * lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - strength;
            points = points - 500;
            exp = exp + 20;
        }
    }

     cout << " " << endl;
     if(timer <= 5 || timer >= 1)
     {
         timer--;
         increase = 1;

     }
     else if(timer > 5)
     {
         increase = 5;
     }


if(after == 0)
{
    Yuno = enemy;
}
else if(after == 1)
{
    MurMur = enemy;
}
    }

        else if(going == 1)
        {
            int going3;
            going3 = rand()%2;
            if(going3 == 0)
                {
    cout << "\n\n" << endl;
    cout << Diary[1] << " turn." << endl;
    cout << " " << endl;
    int going2;
    if(Yuno == 100000)
    {
        going2 = rand() % 2;
    }
    else if(Yuno < 100000)
    {
        going2 = rand() % 3;
    }
    cout << " " << endl;
    if(going2 == 0)
    {
        cout << Diary[1] << " goes to strike down " << Protagonist << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if(attack == dodge)
        {
            cout << Diary[1] << " missed her attack." << endl;
            cout << "0 damage." << endl;
            cout << " " << endl;
            exp = exp + 4;
            points = points + 5;
        }
        else if(attack != dodge)
        {
            cout << Diary[1] << " lands a deadly blow to " << Protagonist << "." << endl;
            cout << "300 damage!" << endl;
            cout << " " << endl;
            health = health - 300;
        }
    }
    else if(going2 == 1)
{
    cout << Diary[1] << " attempts to go for a deadly blow to " << Protagonist << "." << endl;
    cout << " " << endl;
    attack = rand()%2;
    dodge = rand()%2;
    if(attack == dodge)
    {
        cout << Diary[1] << " missed her deadly attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
        exp = exp + 10;
        points = points + 15;
    }
    else if(attack != dodge)
    {
        cout << Diary[1] << " landed a destructive blow to " << Protagonist << "." << endl;
    cout << "550 damage!" << endl;
    cout << " " << endl;
    health = health - 550;
    }
}
    else if(going2 == 2)
    {
    cout << Diary[1] << " heals herself.  Her health was " << Yuno << "," << endl;
    cout << "now it it ";
    Yuno = 100000;
    cout << Yuno << "." << endl;
    cout << " " << endl;
    }
}
        else if(going3 == 1)
        {
            cout << " " << endl;
            cout << Gods[1] << " turn." << endl;
            cout << "\n\n" << endl;
            cout << Gods[1] << " goes to give a powerful MurMur attack!" << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << Gods[1] << " misses her attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                exp = exp + 10;
                points = points + 10;
                health++;
            }
            else if(attack != dodge)
            {
                cout << Gods[1] << " landed a destructive blow to " << target << "." << endl;
                cout << "500 damage!" << endl;
                cout << " " << endl;
                health = health - 500;
            }
        }
        }

        enemyteam = Yuno + MurMur;
        health = health + 100;
         if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp;
    exp = 1;
    points = points + 200;
    health = 300 * lvl;
    strength = strength + 15;

    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
turn++;
cout << " " << endl;
cout << " " << endl;
system("pause");
    }while(turn <= maximumturns && enemyteam>=1);

lvl = States(strength, Protagonist, lvl, armor, points);

}

int minigame6(int& strength, int& armor, int& lvl, string Protagonist, string Diary[13], string Gods[2])
{
    int dodge, attack, hits = 0, misses = 0, Yuki = 1000, target, health = armor * lvl, stage = 1;
    cout << " " << endl;
    cout << " " << endl;
    cout << Gods[1] << ": " << "You fools!  Now die by I, Queen " << Gods[1] << "." << endl;
    cout << " " << endl;
    cout << Protagonist << ": " << "You won't win " << Gods[1] << "." << endl;
    cout << " " << endl;
    cout << Diary[0] << ": " << "Be careful, Takeo!" << endl;
    cout << "\n\n\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
    do
    {
        attack = rand() % 3;
        target = rand() % 2;
        if(target == 0)
        {
            cout << Gods[1] << " throws the sphere at " << Protagonist << "'s direction." << endl;
            cout << "\n\n\n" << endl;
            cout << "Choose a number between 1 and 3. "<< endl;
            do
            {
                cin >> dodge;
            }while(dodge > 3 || dodge < 1);
            cout << " " << endl;
            dodge--;
            cout << " " << endl;
            if(dodge == attack)
            {
                cout << Protagonist << " dodges the attack." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                cout << Gods[1] << "'s sphere manages to hit Takeo, giving him some damage." << endl;
                cout << "25 damage!" << endl;
                cout << " " << endl;
                health = health - 25;
            }
        }
        else if(target == 1)
        {
            cout << Gods[1] << " throws the sphere at " << Diary[0] << "'s direction, forcing Takeo to move out of the way to avoid" << endl;
            cout << "getting his ally get hurt." << endl;
            cout << "\n\n\n" << endl;
            cout << "Choose a number between 1 and 3. "<< endl;
            do
            {
                cin >> dodge;
            }while(dodge > 3 || dodge < 1);
            cout << " " << endl;
            dodge--;
            cout << " " << endl;
            if(dodge == attack)
            {
                cout << Protagonist << "helped" << Diary[0] << " dodge the attack." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                cout << Gods[1] << "'s sphere manages to hit Yukiteru and Takeo, giving him some damage." << endl;
                cout << "25 damage to Takeo!  55 damage to Yukiteru!" << endl;
                cout << " " << endl;
                Yuki = Yuki - 55;
                health = health - 25;
            }
        }

    if(health <= 0 || Yuki <= 0)
    {
     armor = 0;
     stage = 1000;
    }
    else if(stage == 10)
    {
        health = health + 75;
        Yuki = Yuki + 110;
    }
    stage++;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n" << endl;
    }while(stage <= 20);

}

int GodFight2(int& strength, int& lvl, int& item, int& points, int& armor, string Protagonist, string Diary[13], string Gods[2])
{
      int health = armor * lvl, ally, Minene = 90000, enemy = 10000000, allyteam, AI = 10, allypoints = 20000, superattack, increase, timer = 0, attack, dodge, going, turn = 0, exp = 1, maxexp = 100;
    string target, attacked = Gods[1];

do
{
  going = rand()% 2;
  cout << "\n\n\n\n" << endl;
        cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titems:\t" << item << endl;
        cout << Diary[9] << "'s health:\t" << Minene << "\tpoints:\t" << allypoints << "\titems:\t" << AI << endl;
        cout << "\n\n\n" << endl;
        cout << Gods[1] << "'s health:\t" << enemy << endl;
        cout << "\n\n" << endl;

    if(going == 0)
    {
        int going2;
        going2 = rand() %2;

        if(going2 == 0)
        {
        int choice, attacking;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, God, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
