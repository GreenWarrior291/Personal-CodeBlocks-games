    cout << "Past " << Diary[1] << ": " << "Then would you like to go see the stars with me?" << endl;
    cout << " " << endl;
    cout << Diary[0] << ": " << "You're kidding." << endl;
    cout << " " << endl;
    cout << "MurMur: She had no dreams for the future.  And you were the one who gave her hope." << endl;
    cout << " " << endl;
    cout << Diary[0] << ": " << "I did?" << endl;
    cout << " " << endl;
    cout << "Past " << Diary[1] << ": " << "I'll be your bride in the future." << endl;
    cout << " " << endl;
    cout << "Past " << Diary[0] << ": " << "Sure, when we grow up." << endl;
    cout << "\n\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
    cout << Diary[0] << ": " << "You're joking." << endl;
    cout << " " << endl;
    cout << "MurMur: Follow me." << endl;
    cout << " " << endl;
    cout << Diary[0] << ": " << "To where?" << endl;
    cout << " " << endl;
    cout << "MurMur: You want to know, don't you?  Why the Second is out to kill you." << endl;
    cout << " " << endl;
    cout << "MurMur begins to create a blue portal to which leads towards the truth that Yukiteru is looking for." << endl;
    cout << "\n\n\n" << endl;
    system("pause");
}
else if(turn == 26)
{
    cout << "As Takeo and Yuno continue their fight, Yukiteru is flying alongside MurMur in some odd looking place with green squares." << endl;
    cout << "As he is being taken by MurMur to some unknown area, he begins to question her," << endl;
