}

int Chapter3(int Trust,std::string& load, int enemyH,int& chapter, int& friends, int& love, int& item, int& points, int allyH, string Gods[2], string Diary[13], string Supportive[5], string Protagonist,int C1[4], int C2[5], int C3[5], int C4[5], int C5[5], int C6[2], int DT[13], int ST[5], int& strength, int& armor, int& lvl, int path)
{

        love2 = 0;
        saved = 0;
        saved1 = 0;
        saved2 = 2;
    Checkpoint = loading;
if(Checkpoint == 0)
{
do
{
        cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;
