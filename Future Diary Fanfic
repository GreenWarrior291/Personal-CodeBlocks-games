     {
         superattack = superattack*(strength/10);
         cout << Protagonist << " lands an attack on Cop " << choices+1 << "." << endl;
         cout << superattack << " damage!" << endl;
         cout << " " << endl;
         enemy = enemy - superattack;
                 exp = exp + lvl;
                 points++;
     }
