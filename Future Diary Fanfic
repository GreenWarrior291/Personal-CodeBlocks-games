    {
        if (enemies[x]< 1)
        {
            enemies[x]=0;
        }
        else
            enemies[x];
    }
    if (health < 1)
    {
        health = 0;
    }
    else if (health > 100)
    {
        health = 100;
    }

    if (ally2 < 1)
    {
        ally2 = 0;
    }
    else if (ally2 > 200)
    {
        ally2 = 200;
    }
    teamhealth = health + ally2;
    enemyteam = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4] + enemies[5] + enemies[6];
        if (exp >= 100)
{
    exp = 1;
    lvl++;
    health = 100;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
}
    system("pause");
    if (teamhealth <= 0)
    {
cout << " " << endl;
cout << " " << endl;
armor = health;
system("pause");
return armor;
    }
    else if (enemyteam <= 0)
    {
cout << " " << endl;
cout << " " << endl;
armor = health;
system("pause");
return armor;
    }

    }while (battle > 0);



    }

int fighting (string Protagonist,int& points, int& armor, int& strength,int& item,int& lvl,string Supportive[5])
    {
    int health = armor;
    int attackers = 1;
    int SuperAttack = strength;
    int students[12] = {50, 50, 50, 50, 50, 50, 25, 25, 40, 10, 25, 100};
    int action, attack, dodge, maxattack = 12, minattack = 0 , choose;
    int team1 = 0, team2 = 1, going;
    int exp = 1;
    int expmax = 100;
    int turn = 0, turns = 0, nextturn = 1;

    cout << "\n\n\n\n\n\n\n\n\n\n" << endl;
    do
    {
        cout << " " << endl;
        cout << "Team 1: " << endl;
        cout << Protagonist << " Health:\t" << health << "\t\t" << points << endl;
        cout << "\n\n" << endl;
        cout << "Team 2: " << endl;
        cout << Supportive[3] << " Health:\t" << students[11] << endl;
        for (int y = 0; y < 11; y++)
        {
        if (students[y]> 0)
        {
            cout << "Student " << y+1 << " Health:\t" << students[y] << endl;
        }
        else if (students[y]<=0)
        {
            continue;
        }
        }
        system("pause");
        cout << "\n\n\n\n\n" << endl;

                              going = rand() % 2;

        if (team1 == going)
        {
            cout << " " << endl;
    cout << Protagonist <<"'s turn." << endl;
            cout << " " << endl;
            cout << "\n\n" << endl;
            cout << "Choose a student to attack?" << endl;
            cout << " " << endl;
                for (int y = 0; y < 11; y++)
        {
        if (students[y]> 0)
        {
            cout << "Student " << y+1 << endl;
        }

        }
        if (students[11] > 0)
        {
            cout << Supportive[3] << endl;
        }
        cout << " " << endl;
        do
        {
            cin >> choose;
            choose = choose - 1;
            if (students[choose] > 0 )
            {
                attackers++;
            }
            else if (students[choose] <= 0)
            {
                cout << "choose another opponent." << endl;
                cout << " " << endl;
            }
        }while (attackers == 1);


        attackers = 1;


        cout << " " << endl;
        cout << " " << endl;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n\n" << endl;
        int options,path,path2, maximumchoice = 2, x = 1;
          cout << x << ".\t Attack." << endl;
          x++;
          cout << x << ".\tskip." << endl;
          path = 0;
          path2 = 0;
          if (points >= 5)
          {
              x++;
              maximumchoice++;
              path = x;
              cout << x << ".\tSuper Attack." << endl;
          }


          if (item > 0)
          {
              maximumchoice++;
              x++;
              path2 = x;
              cout << x << ".\t Healing." << endl;
          }
        cout << "\n\n\n\n" << endl;
        do
        {
            cin >> options;
        } while (options < 0 || options > maximumchoice);

        if (options == 1)
        {
            SuperAttack = 2;
            cout << " " << endl;
            cout << Protagonist << " gives an attack at ";
            if (choose == 11)
            {
            cout << Supportive[3] << "." << endl;
            }
            else if (choose < 11 || choose >= 0)
            {
            cout << "student " << choose+1 << "." << endl;
            }
            cout << " " << endl;
            cout << " " << endl;
            attack = rand() % 15;
            dodge = rand() % 15;
            if (attack == dodge)
            {
                cout << Protagonist << " misses." << endl;
                cout << "No damage!" << endl;
                cout << " " << endl;
            }
            else if (attack != dodge)
            {
                SuperAttack = SuperAttack*(strength/10);
                students[choose] = students[choose] - SuperAttack;
                cout << Protagonist << " manages to land a blow at ";
                            if (choose == 11)
            {
            cout << Supportive[3] << "." << endl;
            }
            else if (choose < 11 || choose >= 0)
            {
            cout << "student " << choose+1 << "." << endl;
            }
                cout << SuperAttack << " damage!" << endl;
                points++;
                exp++;
            }
        }
        else if (options == 2 )
        {
    cout << " " << endl;
    cout << Protagonist << " skips his turn." << endl;
    cout << " " << endl;
        }
                else if (options == path)
        {
            SuperAttack = 4;
            cout << " " << endl;
            cout << Protagonist << " gives a strong attack at ";
            if (choose == 11)
            {
            cout << Supportive[3] << "." << endl;
            }
            else if (choose < 11 || choose >= 0)
            {
            cout << "student " << choose+1 << "." << endl;
            }
            cout << " " << endl;
            cout << " " << endl;
            attack = rand() % 10;
            dodge = rand() % 10;
            if (attack == dodge)
            {
                cout << Protagonist << " misses." << endl;
                cout << "No damage!" << endl;
                cout << " " << endl;
            }
                    else if (attack != dodge)
            {
                cout << Protagonist << " manages to land a deadly blow at ";
                SuperAttack = SuperAttack*(strength/5);
                students[choose] = students[choose] - SuperAttack;
            if (choose == 11)
            {
            cout << Supportive[3] << "." << endl;
            }
            else if (choose < 11 || choose >= 0)
            {
            cout << "student " << choose+1 << "." << endl;
            }
                cout << SuperAttack << " damage!" << endl;
                points = points - 5;
                exp = exp + lvl;
            }






        turns++;
        }
        else if (options == path2)
        {
            cout << " " << endl;
            cout << " " << endl;
            cout << "Who do you wish to heal?" << endl;
            cout << "1. " << Protagonist << endl;
            cout << " " << endl;
            int heal;
            do
            {
                cin >> heal;
            } while (heal != 1);
            cout << " " << endl;
            cout << " " << endl;
            cout << "Takeo heal himself." << endl;
            health =+ 50;
            cout << "Health now: " << health << endl;
            cout << " " << endl;
        }

        }
        else if (team2 == going)
        {
        do
        {
        int person;
        person = rand() % 12;
        if (person == 11)
        {
        cout << " " << endl;
        cout << "Team 2's turn." << endl;
        cout << " " << endl;
        cout << " " << endl;
        cout << Supportive[3] << " goes off to attack " << Protagonist << "." << endl;
        cout << " " << endl;
        system("pause");
        cout << "\n\n\n" << endl;
        attack = rand() % 10;
        dodge = rand() % 10;

        if (attack == dodge)
        {
            cout << Supportive[3] << " land a hit on " << Protagonist << "." << endl;
            cout << "10 damage!" << endl;
            cout << " " << endl;
            health = health - 10;
        }
        else if (attack != dodge)
        {
            cout << Supportive[3] << " missed." << endl;
            cout << "No damage." << endl;
            cout << " " << endl;
            points++;
            exp =+ 5;
        }
        turn++;
        }
        else if (students[person] > 0)
        {
            cout << " " << endl;
            cout << "Team 2's turn." << endl;
            cout << " " << endl;
            cout << " " << endl;
            cout << "Student " << person+1 << " goes off to attack " << Protagonist << "." << endl;
            cout << " " << endl;
        system("pause");
        cout << "\n\n\n" << endl;
                attack = rand() % 10;
        dodge = rand() % 10;

        if (attack == dodge)
        {
            cout << "Student " << person+1 << " land a hit on " << Protagonist << "." << endl;
            cout << "5 damage!" << endl;
            cout << " " << endl;
            health = health - 5;
        }
        else if (attack != dodge)
        {
            cout << "Student " << person+1 << " missed." << endl;
            cout << "No damage." << endl;
            cout << " " << endl;
            points++;
            exp++;
        }
        turn++;
        }

        }while (turn < nextturn);
        turns++;
        }



        nextturn++;
        if (health > 100)
        {
            health = 100;
        }

        for(int u = 0; u < 11; u++)
        {
         if (students[u] <= 0)
         {
             students[u] = 20 + lvl;
         }
         else if (students[u]> 0)
         {
             students[u];
         }
         else if (students[11] <= 0)
         {
             students[11] = 0;
         }
        }
        cout << " " << endl;
        if (exp >= expmax)
{
    exp = 1;
    lvl++;
    health = 100;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
}
if (points == itemachieve)
{
    itemachieve =+ 200;
    item =+ 1;
}
        }while (health > 25 && turns < 25);
    system("pause");
cout << " " << endl;
cout << " " << endl;
cout << " " << endl;
cout << " " << endl;
lvl = States(strength, Protagonist, lvl, armor, points);
cout << " " << endl;
cout << " " << endl;
armor = 100;
return armor;
    }



int minigame1(int& armor, int& lvl, int& strength, string Protagonist, string Diary[13])
{
    armor = 100;
    int mines = 1, location, choice, maxi = 3, missed, hit, maxturns = 10;
    int item = 3, increase = 5;
    cout << "\n\n\n\n\n\n" << endl;
    cout << Protagonist << " looks at " << Diary[9] << " with a cocky looking look before beginning his run. "<< endl;
    cout << " " << endl;
    cout << Protagonist << " looks around and can tell that there is one mine either on his left side, right side, or his front side." << endl;
    cout << " " << endl;
    cout << "He must be very caution since he can't take too much damage himself either." << endl;
    cout << "\n\n" << endl;
    system("pause");
    cout << " " << endl;
    do
    {
        maxi = 3;
        cout << " " << endl;
        cout << " " << endl;
        cout << "1. head left." << endl;
        cout << "2. head right." << endl;
        cout << "3. head forward." << endl;
        if (item > 0)
        {
            cout << "4. Throw rock." << " " << " " << item << endl;
        maxi++;
        }
    cout << " " << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << " " << endl;
    do
    {
        cin >> choice;
    } while (choice > maxi || choice < 1);

    location = rand() % 3 + 1;

    cout << " " << endl;
    if (choice == 1)
    {
        cout << Protagonist << " went to the left." << endl;
    }
    else if (choice == 2)
    {
        cout << Protagonist << " went to the right." << endl;
    }
    else if (choice == 3)
    {
        cout << Protagonist << " went forward." << endl;
    }


    if (choice == location)
    {
            cout << " " << endl;
        cout << "Sadly for Takeo, he hit a mine and got caught on the explosion." << endl;
        cout << " " << endl;
        hit++;
        armor = armor - (35+ increase);
        system("pause");
    }
    else if (choice != location)
    {
            cout << " " << endl;
        cout << "Lucky for Takeo, he managed to dodge the mine." << endl;
        cout << " " << endl;
        missed++;
        system("pause");
    }
    else if (choice  == 4)
    {
        item = item - 1;
            cout << " " << endl;
        cout << "Takeo grabs his rock and manages to escape another mine, annoying " << Diary[9] << "." << endl;
        cout << " " << endl;
        missed++;
        system("pause");
    }

    if (mines == 3)
    {
        increase = 10;
    if(missed < hit)
    {
        cout << " " << endl;
        cout << " " << endl;
        cout << Diary[9] << " begins to laugh as she finds " << Protagonist << " as a fool." << endl;
        cout << " " << endl;
        cout << Diary[9] << ": your a damn fool kid!" << endl;
        cout << " " << endl;
        cout << Protagonist << " knows that he can't keep taking damage or he will die." << endl;
    }
    else if (missed>hit)
    {
        cout << " " << endl;
        cout << " " << endl;
        if (hit == 0)
        {
        cout << Diary[9] << ":(Damn, this asshole.  I don't know how he hasn't hit a single mine but he will make a mistake.)" << endl;
        }
        else if (hit > 0)
        {
        cout << Diary[9] << ":(Damn this asshole, although he hit a mine, he somehow is surviving this.)" << endl;
        }

        cout << " " << endl;
        cout << Protagonist << " looks at the Ninth and begins to say in confident tone of voice, " << endl;
        cout << " " << endl;
        cout << Protagonist << ": Don't worry, I won't disappoint." << endl;
    }
    system("pause");
    }
    else if (mines == 6)
    {
        increase = 15;
        cout << " " << endl;
        cout << " " << endl;
        if (hit == missed)
        {
        cout << " " << endl;
        cout << Diary[9] << ": " << "Although you somehow survived my mines, you will not survive with the next three." << endl;
        cout << " " << endl;
        }
        else if (missed > hit)
        {
            if (hit == 0)
            {
            cout << Diary[9] << ": (How?!  How is he this good?!  Is he a diary owner?!  No, he can't be!  He isn't using his diary.)" << endl;
            }
            else if (hit > 0)
            {
            cout << Diary[9] << ": (This kid is good, although he hit some of my mines, he somehow is holding his own somehow.)" << endl;
            }
        cout << " " << endl;
        cout << Diary[9] << ": " << "You will not survive the next three mines, kid!!" << endl;
        cout << " " << endl;
        }
    }

    if (armor <= 0)
    {
        armor = 0;
        return armor;
    }

    if (missed == 6)
    {
    lvl++;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
    }
    armor = armor + 25;
    mines++;
    } while (mines < maxturns);
item = 0;
cout << " " << endl;
cout << " " << endl;
if (hit == 0)
{
    cout << Diary[9] << ": " << "B-But how...?  How could you dodge them all?!" << endl;
    cout << " " << endl;
    cout << Protagonist << " smiles as he continues to make his way towards her, seeing the shock expression" << endl;
    cout << "in the ninth's face." << endl;
    cout << " " << endl;
    system("pause");
    cout << " " << endl;
        lvl++;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
}

cout << " " << endl;
armor = 100;
return armor;
}

int minigame2 (int& lvl, int& armor, int& strength, int training, string Protagonist, string Diary[13])
{
    int shooting, bottles = 20, direction, miss, hit;
    cout << " " << endl;
     if (training == 1)
    {
        cout << Diary[3] << ": " << "Considering that you didn't kill the twelfth, I'll assure that you can detect your target." << endl;
    }
    else if (training == 2)
    {
    cout << Diary[3] << ": " << "Considering that you managed to kill the twelfth, I'll be challenging your skills." << endl;
    }
cout << " " << endl;
cout << Diary[3] << ": I'm going to throw a few bottles up, you'll have to guess which direction I'm throwing it towards, right or left." << endl;
cout << " " << endl;
if (training == 1)
{
    do
    {
        direction = rand() % 2;
        if (direction == 1)
        {
        cout << " " << endl;
        cout << Diary[3] << ": " << "Okay, I'm going to throw this bottle towards your left side." << endl;
        cout << " " << endl;
        }
        else if (direction == 2)
        {
        cout << " " << endl;
        cout << Diary[3] << ": " << "Okay, I'm going to throw this bottle towards your right side." << endl;
        cout << " " << endl;
        }
        cout << " " << endl;
        cout << " " << endl;
        cout << "0. left" << endl;
        cout << "1. right" << endl;
        cout << " " << endl;
        do
        {
            cin >> shooting;
        } while (shooting > 1 || shooting < 0);

        if (direction == shooting)
        {
            cout << " " << endl;
            cout << Diary[3] << ": " << "There we go! Good job." << endl;
            cout << " " << endl;
            hit++;
        }
        else if (direction != shooting)
        {
            cout << " " << endl;
            cout << Diary[3] << ": " << "Come on, focus Aiwaka." << endl;
            cout << " " << endl;
            miss++;
        }
        bottles--;
        system("pause");
        cout << "\n\n\n\n\n\n\n" << endl;
    } while (bottles > 15);
cout << " " << endl;
cout << " " << endl;
cout << Diary[3] << ": " << "Okay, I'm going to truly test your skills now." << endl;
if (hit > miss)
{
    cout << "I don't think you should have much trouble." << endl;
    cout << " " << endl;
}
else if (hit < miss)
{
    cout << "Please try to stay focus. " << endl;
    cout << " " << endl;
    cout << Protagonist << ": " << "Okay, I won't let you down." << endl;
    cout << " " << endl;
}
cout << " " << endl;
system("pause");
cout << "\n\n\n\n\n\n\n\n" << endl;
do
{
    direction = rand() % 2;
    cout << " " << endl;
    cout << Diary[3] << ": " << "Alright kid, here I go!" << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << "0. left" << endl;
    cout << "1. right" << endl;
    cout << " " << endl;
    do
    {
        cin >> shooting;
    } while (shooting > 1 || shooting < 0);

            if (direction == shooting)
        {
            cout << " " << endl;
            cout << Diary[3] << ": " << "Impressive, very impressive Aiwaka" << endl;
            cout << " " << endl;
            hit++;
        }
        else if (direction != shooting)
        {
            cout << " " << endl;
            cout << Diary[3] << ": " << "Stay focus kid." << endl;
            cout << " " << endl;
            miss++;
        }
        bottles--;
        system("pause");
        cout << "\n\n\n\n\n\n\n" << endl;


}while (bottles > 0);
}
else if (training == 2)
{
    bottles = 15;
    do
    {
         direction = rand() % 2;
    cout << " " << endl;
    cout << Diary[3] << ": " << "Alright kid, here I go!" << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << "0. left" << endl;
    cout << "1. right" << endl;
    cout << " " << endl;
    do
    {
        cin >> shooting;
    } while (shooting > 1 || shooting < 0);

            if (direction == shooting)
        {
            cout << " " << endl;
            cout << Diary[3] << ": " << "Impressive, very impressive Aiwaka" << endl;
            cout << " " << endl;
            hit++;
        }
        else if (direction != shooting)
        {
            cout << " " << endl;
            cout << Diary[3] << ": " << "Stay focus kid." << endl;
            cout << " " << endl;
            miss++;
        }
        bottles--;
        system("pause");
        cout << "\n\n\n\n\n\n\n" << endl;


    } while (bottles > 0);
}
cout << " " << endl;
cout << " " << endl;
if (miss == 0 || hit > 15)
{
    cout << Diary[3] << ": " << "You've impressed me, good job kid." << endl;
    cout << "You have what it takes to use a gun." << endl;
    cout << " " << endl;
    cout << Protagonist << ": " << "Thank you Kurusu-San." << endl;
    cout << " " << endl;
        system("pause");
    cout << " " << endl;
        lvl = lvl + 3;
    strength = strength + 15;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
}
else if (hit == 10 || hit == miss)
{
    cout << Diary[3] << ": " << "you did good, your getting the hang of it." << endl;
    cout << " " << endl;
    cout << Protagonist << ": " << "Thank you, Kurusu-San." << endl;
    cout << " " << endl;
        system("pause");
    cout << " " << endl;
        lvl=lvl +2;
    strength = strength + 10;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
}
else if (miss > hit || hit == 0)
{
    cout << Diary[3] << ": " << "Hmm, I'll have to train you some more but at least you can shoot a gun." << endl;
    cout << " " << endl;
    cout << Protagonist << ": " << "...Sorry..." << endl;
    cout << " " << endl;
    cout << Diary[3] << ": " << "No, it's alright.  I just now know where exactly to train you." << endl;
    cout << " " << endl;
    if (hit > 5)
    {
            system("pause");
    cout << " " << endl;
        lvl++;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    }
}
system("pause");
}

int DogFight2(int& lvl, int& armor, int& strength, int& points, int& item, string Protagonist, string Supportive[5])
{
        int exp = 1;
    int enemies[20] = {50,50,30,20,70,70,70,100,85,31,50,50,30,20,70,70,70,100,85,31};
    int Ouij = 100;
    int Akise = 500;
    int health = armor;
    int multi;
    int battle = 1;
    int teamhealth;
    int enemyteam;
    int allypoints = 250;
    int AI1 = 7;
    int AI2 = 3;
    int attack, dodge, action, action2, SuperAttack;
    int going, teamturn, team1 = 0, team2 = 1, turn;
    const int MIN_VALUE = 1,
              MAX_VALUE = 10;

    unsigned seed = time(0);


    srand(seed);
   do
    {
    teamhealth = health + Akise + Ouij;
    enemyteam = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4] + enemies[5] + enemies[6] + enemies[7] + enemies[8] + enemies[9] + enemies[10] + enemies[11] + enemies[12] + enemies[13] + enemies[14] + enemies[15] + enemies[16] + enemies[17] + enemies[18] + enemies[19];
    cout << "\n\n\n" << endl;
    cout << "Team 1: " << endl;
    cout << Protagonist << "'s health:\t\t" << health << "\t\tPoints:\t" << points << endl;
    cout << Supportive[0] << "'s health:\t\t" << Akise << "\t\tPoints:\t" << allypoints << "\tItems:\t" << AI1 << endl;
    cout << Supportive[3] << "'s health:\t\t" << Ouij << "\t\tPoints:\t" << allypoints << "\tItems:\t" << AI2 << endl;
    cout << " " << endl;
    cout << "Team 2: " << endl;
    for (int x = 0; x < 20; x++)
    {
        if(enemies[x] > 0)
    {
    cout << "Dog " << x+1 << "'s health:\t" << enemies[x] << endl;
    }
    }
    cout << " " << endl;
    cout << " " << endl;
    system("pause");
    cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;

    teamturn = rand() % 2;


    if (team2 == teamturn)
 {
     cout << " " << endl;
     int Takeoh = 1, Akiseh = 2, Ouijh = 3, attacking, attacker, continu1, continu2;
     continu1 = 0;
     continu2 = 0;
     do
     {
        attacking = rand() % 3;
        if(attacking == 0 && health > 0)
        {
            continu1 = 1;
        }
        else if(attacking == 1 && Akise > 0)
        {
            continu1 = 2;
        }
        else if(attacking == 2 && Ouij > 0)
        {
            continu1 = 3;
        }
     } while (continu1 == 0);
     cout << " " << endl;
     do
     {
         attacker = rand() % 20;
         if(enemies[attacker] > 0)
         {
             continu2 = attacker+1;
         }
     } while (continu2 == 0);
     cout << "Dog " << continu2 << " goes off to attack ";
     if(continu1 == Takeoh)
     {
         cout << Protagonist;
     }
     else if(continu1 == Ouijh)
     {
         cout << Supportive[3];
     }
