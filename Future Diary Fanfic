            attack = rand() % 2;
            dodge = rand() % 2;
            if(dodge == attack)
            {
                cout << " " << endl;
            }
            else if(dodge != attack)
            {
                cout << Diary[7] << " manages to land an extra hit." << endl;
                cout << "25 damage!" << endl;
                cout << " " << endl;
                health = health - 25;
            }
        }
    }
    cout << " " << endl;
    if (exp >= maxexp)
{
    lvl++;
    maxexp = 100;
    exp = 1;
    health = 100 * lvl/2;
    strength = strength + 5;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n\n\n" << endl;
}
    system("pause");
    cout << " " << endl;
    cout << " " << endl;
    turn++;
if(health <= 0)
{
    armor = 0;
    return armor;
}
else if(enemy <= 0)
{
    turn = 30;
}
}while(turn < 20);
if(health >= enemy)
{
    victor = 1;
}
else if(health < enemy)
{
    victor = 2;
}
lvl = States(strength, Protagonist, lvl, armor, points);

}

int EnemyFight(int& armor, int& points, int& item, int& strength, int& victor, int& lvl, string Protagonist)
{
    victor = 0;
    int attack, dodge, going, killers = 1, Takeo = 0, health = armor * (lvl/2), enemyteam, enemies[5]={1500, 500, 750, 1000, 500}, superattack, enemy, increase = 1, timer = 0, exp = 0, maxexp = 100;
    cout << " " << endl;
    do
    {
        cout << "\n\n\n\n" << endl;
        cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titems:\t" << item << "." << endl;
        cout << " " << endl;
        cout << " " << endl;
        for(int x = 0; x < 5; x++)
        {
            cout << x+1 << ". Enemy " << x+1 << "'s health:\t" << enemies[x] << endl;
        }
        cout << " " << endl;
        cout << " " << endl;
        going = rand() % 2;
        if(going == Takeo)
    {
        int choice, attacking, continue1 = 1;
        string attacked;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        cout << "Who do you wish to attack?" << endl;
        cout << "\n\n\n\n" << endl;
        for(int t = 0; t < 5; t++)
        {
            if(enemies[t] >= 1)
            {
                cout << t+1 << ".  Enemy " << t+1 << endl;
            }
            else if(enemies[t] <= 0)
            {
                cout << " " << endl;
            }
        }
        cout << " " << endl;
        do
        {
            cin >> attacking;
            cout << " " << endl;
            attacking--;
            if(enemies[attacking] >= 1)
            {
                continue1++;
            }
            else if(enemies[attacking] < 1)
            {
                cout << " " << endl;
            }
        }while(continue1 == 1);
        continue1 = 1;
        if(attacking == 0)
        {
            attacked = "Enemy 1";
            enemy = enemies[attacking];
        }
        else if(attacking == 1)
        {
            attacked = "Enemy 2";
            enemy = enemies[attacking];
        }
        else if(attacking == 2)
        {
            attacked = "Enemy 3";
            enemy = enemies[attacking];
        }
        else if(attacking == 3)
        {
            attacked = "Enemy 4";
            enemy = enemies[attacking];
        }
        else if(attacking == 4)
        {
            attacked = "Enemy 5";
            enemy = enemies[attacking];
        }
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    pat2 = x;
     cout << x << "." << " Items" << endl;
 }
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> choice;
        } while(choice > y || choice < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(choice == 1)
        {
            superattack = 2;
            cout << Protagonist << " goes to attack " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 4;
            dodge = rand() % 4;
            if(attack == dodge)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/4);
                cout << Protagonist << " lands a hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp++;
                points++;
            }
        }
        else if(choice == 2)
        {
            cout << Protagonist << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(choice == 3 && pat == choice)
        {
            superattack = 6;
            cout << Protagonist << " charges for a super strike on " <<  attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << attacked << " dodged the super attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                points = points - 5;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/2);
                cout << Protagonist << " lands a deadly hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp = exp + 5;
                points = points - 5;
            }
        }
        else if(choice == 3 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", she dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on  " << attacked << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7 * increase;
        cout << Protagonist << " goes to give a strong kick to " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << attacked << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20 * increase;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
