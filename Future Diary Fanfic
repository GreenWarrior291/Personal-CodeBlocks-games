int TFight2 (int& lvl, int& armor, int& points, int& strength, int& item, int& packs, string Protagonist, string Supportive[5], string Diary[13])
{
    int victory, turns = 0, group1 = 0, group2 = 1, AI1 = 5, AI2 =12, Ally = 1000, Ally2 = 550, allypoints = 1000, enemy2 = 1000, enemy1 = 950, enemyteam, superattack, allystrength = 20, allystrength2 = 5, allyteam, going, going1, attack, health = armor *(lvl/2), dodge, Takeo = 0, Yuno = 1, Tsubaki = 2, charge, attacking, random, choose, choice, maximum = 2;
    cout << " " << endl;
    cout << " " << endl;
    cout << " " << endl;
    do
    {
        cout << "Team 1: " << endl;
        cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titems:\t" << item << "." << endl;
        cout << Diary[1] << "'s health:\t" << Ally << "\tpoints:\t" << allypoints << "\titems:\t" << AI1 << "." << endl;
        if(packs == 2)
        {
            cout << Diary[5] << "'s health:\t" << Ally2 << "\tpoints:\t" << allypoints << "\titems:\t" << AI2 << "." << endl;
        }
        cout << "\n\n" << endl;
        cout << "Team 2: " << endl;
        cout << Supportive[4] << "'s health:\t" << enemy1 << endl;
        cout << Supportive[0] << "'s health:\t" << enemy2 << endl;
        cout << "\n\n\n" << endl;
        going = rand() % 2;
        if(group1 == going)
        {
            if(packs == 2)
            {
                maximum = 3;
                random = rand() % 3;
            }
            else if(packs == 1)
            {
             random = rand() % 2;
             Ally2 = 0;
            }

            if(random == Takeo && health > 0)
            {
                cout << " " << endl;
                cout << " " << endl;
                cout << Protagonist << " turn" << endl;
            if(enemy1 > 0 && enemy2 > 0)
            {

                cout << " " << endl;
                cout << " " << endl;
                cout << "Who to attack?" << endl;
                cout << " " << endl;
                cout << "1. " << Supportive[0] << "." << endl;
                cout << "2. " << Supportive[4] << "." << endl;
                cout << "\n\n" << endl;
                do
                {
                    cin >> choose;
                } while(choose > 2 || choose < 1);
                cout << " " << endl;
            }
            else if(enemy1 <= 0)
            {
                choose = 1;
            }
            else if(enemy2 <= 0)
            {
                choose = 2;
            }
                cout << " " << endl;

                cout << " " << endl;
                int x = 1, y = 2, pat,pat1, pat2, pat3 ,choose1, enemy;
                string attacked;
                if(choose == 1)
                {
                    enemy = enemy2;
                    attacked = Supportive[0];
                }
                else if(choose == 2)
                {
                    enemy = enemy1;
                    attacked = Supportive[4];
                }
                cout << x << "." << " Attack." << endl;
                x++;
                cout << " " << endl;
                cout << x << "." << " Skip." << endl;
                if(points > 0 && allypoints > 0)
                {
                if(Ally > 0 || Ally2 > 0)
                {
                    x++;
                    pat = x;
                    y++;
                    cout << " " << endl;
                    cout << " Team attack!" << endl;
                }
                }
                else if(points >= 5)
                {
                    x++;
                    pat1 = x;
                    y++;
                    cout << " " << endl;
                    cout << " Super Attack! \t5" << endl;
                }
                else if(item >= 1)
                {
                    x++;
                    y++;
                    pat2 = x;
                    cout << " " << endl;
                    cout << " Healing \t1" << endl;
                }
                else if(lvl >= 10)
                {
                    x++;
                    y++;
                    pat3 = x;
                    cout << " " << endl;
                    cout << " Combos" << endl;
                }
                cout << " " << endl;
                cout << " " << endl;
                do
                {
                    cin >> choice;
                }while (choice > y || choice < 1);
                cout << " " << endl;
                cout << " " << endl;
                if(choice == 1)
                {
                    superattack = 2;
                    cout << Protagonist << " goes to attack ";
                    if(choose == 1)
                    {
                        cout << Supportive[0] << "." << endl;
                    }
                    else if(choose == 2)
                    {
                        cout << Supportive[4] << "." << endl;
                    }
                    cout << " " << endl;
                    attack = rand() % 5;
                    dodge = rand() % 5;
                    if(attack == dodge)
                    {
                        cout << attacked << " manages to dodge " << Protagonist << "'s attack." << endl;
                        cout << "0 damage" << endl;
                        cout << " " << endl;
                    }
                    else if(attack != dodge)
                    {
                        cout << Protagonist << " manages to land a hit on " << attacked << "." << endl;
                        superattack = superattack * (strength/4);
                        cout << superattack << " damage! "<< endl;
                        cout << " " << endl;
                        points = points + 2;
                        enemy = enemy - superattack;
                    }
                cout << " " << endl;
        if(choose == 1)
    {
        enemy = enemy2;
    }
    else if(choose == 2)
    {
        enemy = enemy1;
    }
                }
                else if(choice == 2)
                {
                cout << Protagonist << " skips his turn." << endl;
                cout << " " << endl;
                }
                else if(choice == 3 && choice == pat)
{
    cout << " " << endl;
if(packs == 1)
{
    choose1 = 2;
}
else if(packs == 2)
{
if(Ally > 0 && Ally2 > 0)
{
    cout << "\n\n\n" << endl;
    cout << "Choose who you want to team up with." << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << "1. Tsubaki." << endl;
    cout << "2. Yuno" << endl;
    cout << " " << endl;
    cout << " " << endl;
    do
    {
        cin >> choose1;
    } while (choose1 > 2 || choose1 < 1);
}
else if(Ally <= 0)
{
    choose1 = 1;
}
else if(Ally2 <= 0)
{
    choose1 = 2;
}
}
    cout << " " << endl;
    cout << " " << endl;
    if(choose1 == 1)
    {
        cout << Protagonist << ": " << "You and me, Tsubaki." << endl;
        cout << " " << endl;
        cout << Diary[5] << ": " << "Alright, cool!" << endl;
        cout << " " << endl;
    }
    else if(choose1 == 2)
    {
        cout << Protagonist << ": " << "Let's do this Yuno." << endl;
        cout << " " << endl;
        cout << Diary[1] << ": " << "Okay Takeo.  Let's do this!" << endl;
        cout << " " << endl;
    }
        else if(choice == 3 && choice == pat)
        {
                cout << "\n\n\n" << endl;
    cout << "Choose either 1 or 2" << endl;
    cout << "\n\n" << endl;
 do
{
    cin >> attacking;
} while(attacking > 2 || attacking < 1);
attacking--;
random = rand() % 2;
cout << "\n\n" << endl;
if(choose1 == 1)
{
    if(random == attacking)
    {
cout << "Takeo and Tsubaki both attempted to attack " << attacked << " yet he dodged the attack." << endl;
cout << "0 damage!" << endl;
