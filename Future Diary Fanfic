        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if(points >= 500)
 {
     x++;
     God = x;
     maxi++;
