            cout << "0 damage." << endl;
            cout << " " << endl;
            }
            else if(attack != dodge)
            {
            cout << Protagonist << " lands a strong hit at " << attacked << "." << endl;
            superattack = superattack * (strength/4);
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy =  enemy - superattack;
            points = points - 5;
            }
                }
                else if(choice == 3 && choice == pat2)
                {
                cout << " " << endl;
                cout << "Who do you wish to heal? " << endl;
                cout << "\n\n" << endl;
                int healer, heal = 2;
                cout << "1. " << Protagonist << endl;
                cout << "2. " << Diary[1] << endl;
                if(packs == 2)
                {
                    heal++;
                    cout << "3. " << Diary[5] << endl;
                }
                cout << "\n\n" << endl;
                do
                {
                    cin >> healer;
                } while (healer > heal || heal < 1);
                cout << " " << endl;
                cout << " " << endl;

               if(healer == 1)
               {
                   cout << Protagonist << " heals himself." << endl;
                   cout << "He had " << health;
                   health = health + 55;
                   cout << ", now his health is " << health << "." << endl;
                   cout << " " << endl;
               }
               else if(healer == 2)
               {
                   cout << Protagonist << " heals " << Diary[1] << "." << endl;
                   cout << "She had " << Ally;
                   Ally = Ally + 55;
                   cout << ", now her health is " << Ally << "." << endl;
                   cout << " " << endl;
               }
                else if(healer == 3)
                {
                 cout << Protagonist << " heals " << Diary[5] << "." << endl;
                 cout << "She had " << Ally2;
                 Ally2 = Ally2 + 65;
                 cout << ", now her health is " << Ally2 << "." << endl;
                 cout << " " << endl;
                }

                }
                else if(choice == 4 && choice == pat2)
                {
                cout << " " << endl;
                cout << "Who do you wish to heal? " << endl;
                cout << "\n\n" << endl;
                int healer, heal = 2;
                cout << "1. " << Protagonist << endl;
                cout << "2. " << Diary[1] << endl;
                if(packs == 2)
                {
                    heal++;
                    cout << "3. " << Diary[5] << endl;
                }
                cout << "\n\n" << endl;
                do
                {
                    cin >> healer;
                } while (healer > heal || heal < 1);
                cout << " " << endl;
                cout << " " << endl;

               if(healer == 1)
               {
                   cout << Protagonist << " heals himself." << endl;
                   cout << "He had " << health;
