{
    cout << target << " dodged " << killer << "'s knife attack." << endl;
    cout << "0 damage." << endl;
    cout << " " << endl;
    exp = exp + 5;
    points = points + 3;
    allypoints = allypoints + 5;
    if(points >= 1000)
    {
        item = item + 10;
    }
}
else if(attack != dodge)
{
    cout << killer << " stabbed " << target << "." << endl;
    cout << "40 damage!" << endl;
    cout << " " << endl;
    attacking = attacking - 40;
    cout << " " << endl;
    attack = rand() % 2;
    dodge = rand() % 2;
    if(attack != dodge)
    {
        cout << killer << " goes for another stab at " << target << " and causes some serious damage." << endl;
        cout << "55 damage!" << endl;
        cout << " " << endl;
        attacking = attacking - 55;
    }
    else if(dodge == attack)
    {
        item++;
        AI++;
        points  = points + 10;
        allypoints = allypoints + 25;
        exp = exp + 8;
    }
}


        if(going2 == 0)
        {
            health = attacking;
        }
        else if(going2 == 1)
        {
            Ally = attacking;
        }
        }
cout << " " << endl;
    if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp;
    exp = 1;
    health = 100 * lvl/2;
    strength = strength + 8;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
        system("pause");
        if(team == 2)
        {
            teamally = health + Ally;
        }
        else if(team == 1)
        {
            teamally = health;
        }

        enemyteam = enemies[0] + enemies[1] + enemies[2];
        if(teamally <= 0)
        {
            armor = 0;
            return armor;
        }
    } while(enemyteam > 0);

    lvl = States(strength, Protagonist, lvl, armor, points);
    return armor;
}

int minigame4(int& ammo, int& ammo2, int& victor, string Protagonist, string Diary[13])
{
    int shoot, hit = 0, miss = 0, aim;
    cout << " " << endl;
    cout << "Yuno shoots her gun exactly as the diary of her Yukki's says she does yet the eleventh's vehicle dodged the shot.  This surprised Yuno, causing the group to think that the eleventh as a similar" << endl;
    cout << "Diary to Yukiteru's which caused Yuno to try again, only aiming for the eleventh yet he dodged the attack." << endl;
    cout << " " << endl;
    ammo--;
    cout << Diary[0] << ": " << "He dodged it...!" << endl;
    cout << " " << endl;
    cout << Protagonist << ": " << "His diary is called the watcher, my theory is that he can spy on our diary, meaning-" << endl;
    cout << " " << endl;
    cout << Diary[1] << ": " << "We can't hit him easily." << endl;
    cout << " " << endl;
    cout << Protagonist << ": " << "Hmm maybe we still can, one way to find out." << endl;
    cout << "\n\n\n" << endl;
    ammo2--;
    do
    {
        cout << "Choose a number between 1 and 5" << endl;
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> shoot;
        } while (shoot > 5 || shoot < 1);
        cout << " " << endl;
        shoot--;
        cout << "\n\n" << endl;
        aim = rand() % 5;
        if(shoot == aim)
        {
            hit++;
    if(hit == 1)
    {
            cout << Protagonist << ": " << "Yes! Direct hit!" << endl;
            cout << " " << endl;
            cout << Diary[0] << ": " << "Impressive!  Now we need to hit" << endl;
            cout << " " << endl;
            cout << Diary[1] << ": " << "Allow me to finish this." << endl;
            cout << " " << endl;
    }
    else if(hit == 2)
    {
        cout << Protagonist << ": " << "Got it!" << endl;
        cout << " " << endl;
        cout << Diary[0] << ": " << "You did it Takeo, awesome!" << endl;
        cout << " " << endl;
        cout << Diary[1] << ": " << "Good job Takeo!" << endl;
        cout << " " << endl;
        victor = 1;
    }
        }
    else if(shoot != aim)
    {
        miss++;
    if(ammo > 0)
    {
        cout << " " << endl;
        cout << Protagonist << ": "<< "Crap! I missed." << endl;
        cout << " " << endl;
        cout << Diary[1] << ": " << "Allow me to try!" << endl;
        cout << " " << endl;
    }
    else if(ammo <= 0)
    {
      cout << " " << endl;
      cout << Protagonist << ": " << "Fuck!  I'm out of ammo." << endl;
      cout << " " << endl;
      if(hit == 1)
      {
          cout << Diary[0] << ": " << "It's all up to you, Yuno." << endl;
          cout << " " << endl;
          cout << Diary[1] << ": " << "I can do it." << endl;
          cout << " " << endl;
      }
      else if(hit == 0)
      {
          cout << Diary[1] << ": " << "I can do it." << endl;
          cout << " " << endl;
          cout << Diary[0] << ": " << "Save the ammo, it's not worth it." << endl;
          cout << " " << endl;
          cout << Protagonist << ": " << "He has a point, we needed to hit both tires." << endl;
          cout << " " << endl;
          cout << Diary[0] << ": " << "Yeah but don't worry, I got an idea." << endl;
          cout << " " << endl;
          victor = 2;
      }
    }
    }
    ammo--;
    cout << "\n\n" << endl;
        system("pause");
        cout << "\n\n\n" << endl;
        cout << "Choose a number between 1 and 5" << endl;
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> shoot;
        } while (shoot > 5 || shoot < 1);
        cout << " " << endl;
        shoot--;
        cout << "\n\n" << endl;
        aim = rand() % 5;
            if(shoot == aim)
        {
            hit++;
    if(hit == 1)
    {
            cout << Diary[1] << ": " << "Yes!" << endl;
            cout << " " << endl;
            cout << Diary[0] << ": " << "Amazing Yuno!  Now we need to hit" << endl;
            cout << " " << endl;
            cout << Protagonist << ": " << "Let me finish this." << endl;
            cout << " " << endl;
    }
    else if(hit == 2)
    {
        cout << Diary[1] << ": " << "I did it!" << endl;
        cout << " " << endl;
        cout << Diary[0] << ": " << "Amazing shooting Yuno!" << endl;
        cout << " " << endl;
        cout << Protagonist << ": " << "Great job Yuno!" << endl;
        cout << " " << endl;
        victor = 1;
    }
        }
    else if(shoot != aim)
    {
        miss++;
    if(ammo2 > 0)
    {
        cout << " " << endl;
        cout << Diary[1] << ": "<< "Crap! I missed." << endl;
        cout << " " << endl;
        cout << Protagonist << ": " << "Allow me to try!" << endl;
        cout << " " << endl;
    }
    else if(ammo2 <= 0)
    {
      cout << " " << endl;
      cout << Diary[1] << ": " << "Darn it!  I missed." << endl;
      cout << " " << endl;
      cout << Diary[0] << ": " << "It's okay Yuno, I have an idea." << endl;
      cout << " " << endl;
      cout << " " << endl;
      victor = 2;
    }
    }
    ammo2--;
    cout << "\n\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
    }while(victor == 0);

    return victor;
}

int PoliceFight2(int& armor, int& points, int& item, int& strength, int& assistance, int& assistance2, int& assistance3, int& lvl, string Protagonist, string Diary[13], string Supportive[5])
{
    int attack, Ally, dodge, Fighter1 = 0, turning = 1, allypoints = 10000, allystrength = 10, Fighter2 = 1, fighting = 1, Fighter3, Fighter4, Fighter5, exp = 0, maxexp = 100, Tsubaki = 2000, health = armor * (lvl/2), Mother = 1000, Nishijima = 2500, Minene = 9000, superattack, going, going2, allyteam, AI = 15, AI2 = 20, AI3 = 7, timer = 0, increase, enemyteam, enemy, enemies[10] = {4000,4000,4000,4000,4000,4000,4000,4000,4000,4000};
    string attacked, target;
    if(assistance == 2)
    {
        fighting++;
        Fighter3 = fighting;
    }
    else if(assistance2 == 1)
    {
        fighting++;
        Fighter4 = fighting;
    }
    else if(assistance3 == 1)
    {
        fighting++;
        Fighter5 = fighting;
    }
    cout << " " << endl;
    fighting++;
    do
    {
        cout << "\n\n\n\n" << endl;
        cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titems:\t" << item << "." << endl;
        cout << Supportive[4] << "'s health:\t" << Nishijima << "." << endl;
        if(assistance == 2)
        {
            cout << Diary[9] << "'s health:\t" << Minene << "\tpoints:\t" << allypoints << "\titems:\t" << AI3 << "." << endl;
        }
        else if(assistance2 == 1)
        {
            cout << "Mother's health:\t" << Mother << "\tpoints:\t" << allypoints << "\titems:\t" << AI2 << "." << endl;
        }

      going = rand() % 2;
      if(going == 0)
      {
        do
        {
            going2 = rand() % fighting;
            if(going2 == Fighter1 && health > 0)
            {
                turning++;
            }
            else if(going2 == Fighter2 && Nishijima > 0)
            {
                turning++;
            }
            else if(going2 == Fighter3 && Minene > 0)
            {
                turning++;
            }
            else if(going2 == Fighter4 && Mother > 0)
            {
                turning++;
            }
            else if(going2 == Fighter5 && Tsubaki > 0)
            {
                turning++;
            }

        }while(turning == 1);
          turning = 1;
          cout << " " << endl;
          if(going2 == Fighter1)
    {
        int choice, attacking, continue1 = 1;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        cout << "Who do you wish to attack?" << endl;
        cout << "\n\n\n\n" << endl;
        for(int t = 0; t < 10; t++)
        {
            if(enemies[t] >= 1)
            {
                cout << t+1 << ".  Enemy " << t+1 << endl;
            }
            else if(enemies[t] <= 0)
            {
                cout << " " << endl;
            }
        }
        cout << " " << endl;
        do
        {
            cin >> attacking;
            cout << " " << endl;
            attacking--;
            if(enemies[attacking] >= 1)
            {
                continue1++;
            }
            else if(enemies[attacking] < 1)
            {
                cout << " " << endl;
            }
        }while(continue1 == 1);
        continue1 = 1;
        if(attacking == 0)
        {
            attacked = "Enemy 1";
            enemy = enemies[attacking];
        }
        else if(attacking == 1)
        {
            attacked = "Enemy 2";
            enemy = enemies[attacking];
        }
        else if(attacking == 2)
        {
            attacked = "Enemy 3";
            enemy = enemies[attacking];
        }
        else if(attacking == 3)
        {
            attacked = "Enemy 4";
            enemy = enemies[attacking];
        }
        else if(attacking == 4)
        {
            attacked = "Enemy 5";
            enemy = enemies[attacking];
        }
        else if(attacking == 5)
        {
            attacked = "Enemy 6";
            enemy = enemies[attacking];
        }
        else if(attacking == 6)
        {
            attacked = "Enemy 7";
            enemy = enemies[attacking];
        }
        else if(attacking == 7)
        {
            attacked = "Enemy 8";
            enemy = enemies[attacking];
        }
        else if(attacking == 8)
        {
            attacked = "Enemy 9";
            enemy = enemies[attacking];
        }
        else if(attacking == 9)
        {
            attacked = "Enemy 10";
            enemy = enemies[attacking];
        }
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    pat2 = x;
     cout << x << "." << " Items" << endl;
 }
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> choice;
        } while(choice > y || choice < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(choice == 1)
        {
            superattack = 2;
            cout << Protagonist << " goes to attack " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 4;
            dodge = rand() % 4;
            if(attack == dodge)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/4);
                cout << Protagonist << " lands a hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp++;
                points++;
            }
        }
        else if(choice == 2)
        {
            cout << Protagonist << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(choice == 3 && pat == choice)
        {
            superattack = 6;
            cout << Protagonist << " charges for a super strike on " <<  attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << attacked << " dodged the super attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                points = points - 5;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/2);
                cout << Protagonist << " lands a deadly hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp = exp + 5;
                points = points - 5;
            }
        }
        else if(choice == 3 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", she dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on  " << attacked << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7 * increase;
        cout << Protagonist << " goes to give a strong kick to " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << attacked << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20 * increase;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
        cout << Protagonist << " misses the attack." << endl;
        cout << "0 damage." << endl;
