        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", she dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
