    int C3[5] = {0,0,0,0,0}; //keeps progress of important choices in chapter 3
    int C4[5] = {0, 0, 0, 0, 0}; //keeps progress of important choices in chapter 4
    int C5[5] = {0, 0, 0, 0, 0}; //keeps progress of important choices in chapter 5
    int C6[2] = {0,0}; //keeps progress of important choices in chapter 6
    int chapter=0; //Chapter the player is in.
    int friends=0; //should the MC reject a love interest by friendzone.
    int love=0; //should the MC gain a love interest, this will be important.
    std::string load;
    int item=0; //ability to heal.
    int points = 0; //able to do abilities
    int allyH; //Ally health
    int enemyH; //enemy health
    string Gods[2]={"Deus", "MurMur"};
    string Diary[13]={"Yukiteru Amano","Yuno Gasai","Takao Hiyama","Keigo Kurusu","Reisuke Houjou","Tsubaki Kasugano","Marco Ikusaba","Ai Mikami","Kamado Ueshita","Minene Uryu","Karyuudo Tsukishima","John Becchus","Yomotsu Hirasaka"};
