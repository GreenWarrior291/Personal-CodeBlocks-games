    cout << " " << endl;
    cout << " " << endl;
    }

}
system("pause");
do
{
    cout << "\n\n\n" << endl;
    cout << "Choose who you want to team up with." << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << "1. Tsubaki." << endl;
    cout << "2. Yuno" << endl;
    cout << " " << endl;
    cout << " " << endl;
    do
    {
        cin >> choose;
    } while (choose > 2 || choose < 1);
    cout << " " << endl;
    cout << " " << endl;
    if(choose == 1)
    {
        cout << Protagonist << ": " << "You and me, Tsubaki." << endl;
        cout << " " << endl;
        cout << Diary[5] << ": " << "Alright, cool!" << endl;
        cout << " " << endl;
    }
    else if(choose == 2)
    {
        cout << Protagonist << ": " << "Let's do this Yuno." << endl;
        cout << " " << endl;
        cout << Diary[1] << ": " << "Okay Takeo.  Let's do this!" << endl;
        cout << " " << endl;
    }
