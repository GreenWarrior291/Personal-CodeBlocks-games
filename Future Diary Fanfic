    {
        bullets = bullets - reload;
        gun = 6;
    }
 reload = 0;
}
else if(choice == 5 && chosen4 == choice)
{
    cout << Protagonist << " decides to reload his gun." << endl;
    cout << " " << endl;
    if(bullets < 6)
    {
        reload = bullets;
        gun = gun + reload;
        if(gun>6)
        {
            gun = 6;
        }
    }
    else if(bullets >= 6)
    {
        bullets = bullets - reload;
        gun = 6;
    }
 reload = 0;
}
else if(choice == 6 && chosen4 == choice)
{
    cout << Protagonist << " decides to reload his gun." << endl;
    cout << " " << endl;
    if(bullets < 6)
    {
        reload = bullets;
        gun = gun + reload;
        if(gun>6)
        {
            gun = 6;
        }
    }
    else if(bullets >= 6)
    {
        bullets = bullets - reload;
        gun = 6;
    }
 reload = 0;
}
else if(choice == 7 && chosen4 == choice)
{
    cout << Protagonist << " decides to reload his gun." << endl;
    cout << " " << endl;
    if(bullets < 6)
    {
        reload = bullets;
        gun = gun + reload;
        if(gun>6)
        {
            gun = 6;
        }
    }
    else if(bullets >= 6)
    {
        bullets = bullets - reload;
        gun = 6;
    }
 reload = 0;
}
}
}
    else if(going == Kurusu)
    {
        cout << " " << endl;
        cout << Diary[3] << " turn." << endl;
        cout << " " << endl;
        attack = rand() % 20;
        if(attack >= 14)
        {
            cout << " " << endl;
            cout << Diary[3] << " shot " << Protagonist << " and damaged him critically." << endl;
            cout << "50 damage." << endl;
            cout << " " << endl;
            health = health - 50;
        }
        else if (attack < 14)
        {
            cout << " " << endl;
            cout << Diary[3] << " missed." << endl;
            cout << "0 damage" << endl;
            cout << " " << endl;
            health = health + 10;
            exp = exp + 8;
        }
    }
    if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp * lvl;
    exp = 1 * lvl;
    health = 100;
    strength = strength + 5;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
cout << " " << endl;
system("pause");
    if(health < 1)
    {
        armor = health;
        return armor;
    }
    } while(enemy <= 200);

armor = health;
return armor;
}

int DiaryFight5(int& strength, int& armor, int& lvl, int& item, int& points, int& victor, string Protagonist, string Diary[13])
{
    int health = (armor/4) * lvl;
    int exp = 1;
    int enemy = 1000;
    int maxexp = 100;
    int enemypoints = 1000;
    int attack, dodge, SuperAttack, Yuki = 1, strength2 = 0, going, Takeo = 0, Yuno = 1;

    do
    {
        cout << "\n\n\n\n\n\n" << endl;
        cout << Protagonist << "'s health:\t" << health << "\tpoints:\t\t" << points << "\titem:\t" << item << endl;
        cout << " " << endl;
        cout << Diary[1] << "'s health:\t" << enemy << endl;
        cout << "\n\n\n\n\n" << endl;
        going = rand() % 2;
    if (going == Takeo)
    {
int choice, chosen, chosen1, chosen2, chosen3,chosen4;
{
 cout << Protagonist << " turn." << endl;
 cout << "\n\n\n" << endl;
 int maxi = 2, x = 1;
 cout << x << "." << "  Attack." << endl;
 x++;
 cout << x << "." << " Skip turn" << endl;
 if (points >= 5)
 {
     x++;
     chosen = x;
     maxi++;
     cout << x << "." << " Super Attack \t5" << endl;
 }
 else if (lvl >= 6)
 {
     x++;
     chosen1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    chosen2 = x;
     cout << x << "." << " Items\t1" << endl;
 }
 cout << " " << endl;
 cout << " " << endl;
 do
 {
     cin >> choice;
 } while(choice > maxi || choice < 1);
int superattack;
cout << "\n\n\n" << endl;

 if (choice == 1)
 {
     superattack = 2;
     cout << Protagonist << " chose to attack " << Diary[1] << "." << endl;
     cout << " " << endl;
     attack = rand() % 5;
     dodge = rand() % 5;
     if (attack == dodge)
     {
         cout << "Sadly " << Diary[1] << " dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[1] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
