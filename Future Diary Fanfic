             {
                 cin >> let;
             } while(let > 2 || let < 1);
         }
         else if(timer != 0 || item < 5)
         {
             let = 1;
         }
         cout << "\n\n" << endl;
         if(let == 1)
         {
             item--;
             cout << Protagonist << " heals himself." << endl;
             cout << "His health was " << health;
             health = health + 95;
             cout << ", now it is " << health << "." << endl;
             cout << " " << endl;
         }
         else if(let == 2)
         {
             item = item - 5;
             cout << Protagonist << " increased his strength!" << endl;
             cout << " " << endl;
             increase = 5;
             timer = 21;
         }
     }
           else if(choice == 4 && choice == pat2)
     {
         int let;
         if(timer == 0 && item >= 5)
