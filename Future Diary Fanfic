        cout << " " << endl;
        strength = strength + 5;
        allystrength = allystrength + 5;
    }
    else if(direction == 2)
    {
        AI = AI - 4;
        cout << " " << endl;
        cout << "Who do you wish to restore full health to?" << endl;
        cout << " " << endl;
        cout << " " << endl;
        cout << "1. " << Protagonist << "." << endl;
        cout << "2. " << Diary[5] << "." << endl;
        cout << "\n\n" << endl;
        do
        {
            cin >> heal;
        } while(heal > 2 || heal < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(heal == 1)
        {
            cout << Diary[5] << " chooses to restore " << Protagonist << "'s health fully." << endl;
            cout << "His health was " << health;
            health = 100 * (lvl/2);
            cout << ", now it is " << health << "." << endl;
            cout << " " << endl;
        }
        else if(heal == 2)
        {
            cout << Diary[5] << " chooses to restore her health fully." << endl;
            cout << "Her health was " << Ally;
            Ally = 2500;
            cout << ", now it is " << Ally << "." << endl;
            cout << " " << endl;
        }
    }
}


            if(attacking == 0)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 1)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 2)
        {
            enemies[attacking] = enemy;
        }
            }


        }
        else if(going == killers)
        {
        cout << " " << endl;
        cout << " " << endl;
        int cont = 1, random, attacking;
        string target, Entering;
        do
        {
            random = rand() % 3;
            if(enemies[random] >= 1)
            {
            if(random == 0)
            {
                killer = "Enemy 1";
            }
            else if(random == 1)
            {
                killer = "Enemy 2";
            }
            else if(random == 2)
            {
                killer = "Enemy 3";
            }
                cont++;
            }
        } while(cont == 1);
        cont = 1;
if(team == 2)
{
        if(health > 0 && Ally > 0)
        {
            going2 = rand() % 2;
        }
        else if(health <= 0)
        {
            going2 = 1;
        }
        else if(Ally <= 0)
        {
            going2 = 0;
        }
}
else if(team == 1)
{
    going2 = 0;
}
        cout << " " << endl;
        if(going2 == 0)
        {
            target = Protagonist;
            attacking = health;
        }
        else if(going2 == 1)
        {
            target = Diary[5];
            attacking = Ally;
        }


cout << killer << " turn." << endl;
cout << " " << endl;
