            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on " << Diary[7] << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7;
        cout << Protagonist << " goes to give a strong kick to " << Diary[7] << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << Diary[7] << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
        cout << Protagonist << " misses the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(attack != dodge)
    {
        superattack = superattack * (strength/2);
        cout << Protagonist << " lands a very deadly blow to " << Diary[7] << "." << endl;
        cout << superattack << " damage" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
        exp = exp + 10;
    }
}

}

else if (choice == 3 && chosen2 == choice)
{
    cout << Protagonist << " chooses to use an item." << endl;
    cout << " " << endl;
    {
        cout << Protagonist << " heals himself." << endl;
        cout << "Health was " << health;
        health = health + 25;
        cout << ", now it is " << health << endl;
        cout << " "  << endl;
        item = item - 1;
    }
}

else if (choice == 4 && chosen2 == choice)
{
    cout << Protagonist << " chooses to use an item." << endl;
    cout << " " << endl;
    {
        cout << Protagonist << " heals himself." << endl;
        cout << "Health was " << health;
        health = health + 25;
        cout << ", now it is " << health << endl;
        cout << " "  << endl;
        item = item - 1;
    }
}

else if (choice == 5 && chosen2 == choice)
{
    cout << Protagonist << " chooses to use an item." << endl;
    cout << " " << endl;
    {
        cout << Protagonist << " heals himself." << endl;
        cout << "Health was " << health;
        health = health + 25;
        cout << ", now it is " << health << endl;
        cout << " "  << endl;
        item = item - 1;
    }
}

}

       }
  else if(going == team2)
       {
    cout << " " << endl;
    cout << Diary[6] << " and " << Diary[7] << " take a turn." << endl;
    cout << " " << endl;
    int turns, guy = 0, girl = 1, go, damages;
    turns = rand() % 2;
    if(guy == turns)
{
    damages = 0;
    go = rand() % 2;
    {
    cout << Diary[6] << " leads the attack towards " << Protagonist << endl;
    cout << " " << endl;
    attack = rand() % 4;
    dodge = rand() % 4;
    if(attack == dodge)
    {
        cout << Protagonist << " dodges the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
        exp = exp + 25;
        points = points + 2;
    }
    else if(attack != dodge)
    {
        cout << Diary[6] << " landed the first hit with " << Diary[7] << " charging for the second attack." << endl;
        cout << " " << endl;
        damages = damages + 20;
        attack = rand() % 3;
        dodge = rand() % 3;
        if(attack == dodge)
        {
            cout << Protagonist << " managed to dodge " << Diary[7] << "'s attack." << endl;
            cout << damages << " damage to " << Protagonist << "." << endl;
            health = health - damages;
            cout << " " << endl;
        }
        else if(attack != dodge)
        {
            cout << Diary[7] << " lands the second hit with " << Diary[6] << " and " << Diary[7] << " preparing to do the finishing blow." << endl;
            cout << " " << endl;
            damages = damages + 15;
            attack = rand() % 2;
            dodge = rand() % 2;
            if(dodge == attack)
            {
                cout << Protagonist << " managed to dodge the attack from the duo while taking the opportunity to counter the two with a strong attack." << endl;
                cout << damages << " damage to " << Protagonist << endl;
                cout << "25 damage to both " << Diary[6] << " and " << Diary[7] << "." << endl;
                cout << " " << endl;
                health = health - damages;
                enemy = enemy - 25;
                enemy1 = enemy1 - 25;
                exp = exp * 2;
                points = points * 2;
            }
            else if(attack != dodge)
            {
                cout << Diary[6] << " and " << Diary[7] << " land the finishing combo blow towards " << Protagonist << "." << endl;
                damages = damages + 25;
                cout << damages << " damage!" << endl;
                cout << " " << endl;
                health = health - damages;
            }
        }
    }
    }
    }

    else if(girl == turns)
    {
        damages = 0;
    cout << " " << endl;
      cout << Diary[7] << " leads the attack towards " << Protagonist << "." << endl;
      cout << " " << endl;
      attack = rand() % 5;
      dodge = rand() % 5;
      if(dodge == attack)
      {
          cout << Protagonist << " manages to dodge the attack." << endl;
          cout << "0 damage." << endl;
          cout << " " << endl;
      }
      else if(dodge != attack)
      {
          cout << Diary[7] << " lands the first light attack with " << Diary[6] << " charging with a stronger attack." << endl;
          cout << " " << endl;
          damages = 10;
          attack = rand() % 4;
          dodge = rand() % 4;
          if(dodge == attack)
          {
              cout << Protagonist << " managed to dodge the attack and counter it with a hit towards " << Diary[6] << endl;
              cout << damages << " damage to " << Protagonist << endl;
              cout << "10 damage to " << Diary[6] << endl;
              cout << " " << endl;
              health = health - damages;
              enemy = enemy - 10;
              exp = exp + 20;
          }
          else if(attack != dodge)
          {
              cout << Diary[6] << " lands a strong blow towards " << Protagonist << " with " << Diary[7] << " preparing to do the finishing blow." << endl;
              cout << " " << endl;
              damages = damages + 20;
              attack = rand() % 4;
              dodge = rand() % 4;
              if(attack != dodge)
              {
                  cout << Diary[7] << " lands the finishing blow towards " << Protagonist << endl;
                  damages = damages + 20;
                  cout << damages << " damage!" << endl;
                  cout << " " << endl;
                  health = health - damages;
              }
              else if(dodge == attack)
              {
                  cout << Diary[7] << " misses with " << Protagonist << " managing to counter attack." << endl;
                  cout << damages << " damage to " << Protagonist << "." << endl;
                  cout << "10 damage to " << Diary[7] << "." << endl;
                  cout << " " << endl;
                  health = health - damages;
                  enemy1 = enemy1 - 10;
                  exp = exp + 20;
              }
           }
      }
    }

       }
    if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp * lvl;
    exp = 1 * lvl;
    health = 100 * lvl/2;
    strength = strength + 5;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
cout << "\n\n\n\n" << endl;
system("pause");
cout << "\n\n\n\n\n" << endl;
enemies = enemy + enemy1;
if(health <= 0)
{
    victor = 2;
}
else if(enemies <= 0)
{
    victor = 1;
}
}while(victor == 0);
if(victor == 1)
{
    armor = 100;
    lvl = States(strength, Protagonist, lvl, armor, points);
    return armor;
}
else if(victor == 2)
{
    armor = 25;
    return armor;
}
}

int DiaryFight2(int& armor, int& lvl, int& item, int& points, int& victor, int& strength, string Protagonist, string Diary[13])
{
    int health = armor;
    int breath = 25;
    int attack, dodge, going, superattack, choice;
    int enemy = 400;
    int DiaryDestroyer = 300;
    int exp = 1, maxexp = 100;

cout << " " << endl;
cout << " " << endl;
cout << Diary[4] << ": " << "Alright, let's see what you got." << endl;
cout << " " << endl;
cout << " " << endl;
system("pause");
do
{
cout << "\n\n\n\n" << endl;
cout << " " << endl;
cout << Protagonist << "\t health:\t" << health << "\t" << points << "\t\t" << item << endl;
cout << " " << endl;
cout << Diary[4] << "\thealth:\t" << enemy << endl;
cout << " " << endl;
system("pause");
cout << "\n\n\n\n\n\n\n\n" << endl;
going = rand() % 2;
int chosen,chosen1, chosen2, chosen3;
if (going == 1)
{
 cout << Protagonist << " turn." << endl;
 cout << "\n\n\n" << endl;
 int maxi = 2, x = 1;
 cout << x << "." << "  Attack." << endl;
 x++;
 cout << x << "." << " Skip turn" << endl;
 if (points >= 5)
 {
     x++;
     chosen = x;
     maxi++;
     cout << x << "." << " Super Attack \t5" << endl;
 }
 else if (lvl >= 6)
 {
     x++;
     chosen1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    chosen2 = x;
     cout << x << "." << " Items\t1" << endl;
 }
 else if (DiaryDestroyer >= 1000)
 {
     x++;
     maxi++;
     chosen3 = x;
     cout << x << "." << "Diary Finish! \t1000" << endl;
 }
 cout << " " << endl;
 cout << " " << endl;
 do
 {
     cin >> choice;
 } while(choice > maxi || choice < 0);

cout << "\n\n\n" << endl;

 if (choice == 1)
 {
     superattack = 2;
     cout << Protagonist << " chose to attack " << Diary[4] << "." << endl;
     cout << " " << endl;
     attack = rand() % 5;
     dodge = rand() % 5;
     if (attack == dodge)
     {
         cout << "Sadly " << Diary[4] << " dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[4] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 exp = exp + lvl;
                 DiaryDestroyer = DiaryDestroyer + 20;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << Diary[4] << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
     }
     else if (attack != dodge)
     {
         superattack = superattack*(strength/10);
         cout << Protagonist << " lands an attack on " << Diary[4] << "." << endl;
         cout << superattack << " damage!" << endl;
         cout << " " << endl;
         enemy = enemy - superattack;
                 exp = exp + lvl;
                 DiaryDestroyer = DiaryDestroyer + 20;
                 points++;
     }
      }
else if (choice == 2)
{
    cout << Protagonist << " chooses to skip his turn." << endl;
    cout << " " << endl;
}

else if (choice == 3 && chosen==choice)
{
superattack = 4;
cout << Protagonist << " chooses to do a very powerful attack at " << Diary[4] << endl;
cout << " " << endl;
attack = rand() % 4;
dodge = rand() % 4;
if(attack == dodge)
{
 cout << "Lucky for " << Diary[4] << ", he managed to dodge the attack, barely." << endl;
    cout << "0 damage." << endl;
    points = points - 5;
}
else if (attack != dodge)
{
    superattack = superattack*(strength/5);
    cout << Protagonist << " landed a strong blow at " << Diary[4] << "." << endl;
    cout << superattack << " damage!" << endl;
    cout << " " << endl;
    exp = exp + (lvl*2);
    points = points - 5;
    DiaryDestroyer = DiaryDestroyer + 25;
}

}
else if (choice == 3 && chosen1 == choice)
{
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5;
        cout << Protagonist << " goes to strike down " << Diary[4] << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << Diary[4] << ", he dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[4] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 exp = exp + lvl;
                 DiaryDestroyer = DiaryDestroyer + 20;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << Diary[4] << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit " << Diary[4] << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
