if (exp >= (100+(lvl*2)))
{
    exp = 1*lvl;
    lvl++;
    health = 100*lvl;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
}
if (points == itemachieve)
{
    itemachieve =+ 200;
    item =+ 1;
}

        if (enemy < 1)
    {
        cout << " " << endl;
        armor = health;
        return armor;
    }
    else if (teamhealth < 1)
{
    cout << " " << endl;
    armor = health;
    return armor;
}
cout << " " << endl;
    }while(battle > 0);
}

int States(int& strength, string Protagonist, int& lvl, int& armor, int& points)
{
    cout << " " << endl;
    cout << " " << endl;
    cout << Protagonist << endl;
    cout << " " << endl;
    cout << "Strength:\t" << strength << endl;
    cout << "Level:   \t" << lvl << endl;
    cout << "Endurance:\t" << armor << endl;
    cout << "Points:   \t" << points << endl;
    cout << "\n\n\n\n" << endl;
    system("pause");
    return lvl;
}

int DiaryFight1(int& lvl, int& strength, int& armor, int& points, int& item, int& victor, string Protagonist, string Diary[13])
{
        int exp = 1;
    int enemies[7] = {50,50,30,20,70,70,70};
    int ally2 = 200;
    int health = armor;
    int protection;
    int battle = 1;
    int teamhealth;
    int enemyteam;
    int YunoStrength = 5;
    int allypoints = 100;
    int YI2 = 1;
    int attack, dodge, action, action2, SuperAttack;
    int going, going2, team1, team2;
    const int MIN_VALUE = 1,
              MAX_VALUE = 10;

    unsigned seed = time(0);


    srand(seed);
   do
    {
    teamhealth = health + ally2;
    enemyteam = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4] + enemies[5] + enemies[6];
    cout << "\n\n\n" << endl;
    cout << "Team 1: " << endl;
    cout << Protagonist << "'s health:\t\t" << health << "\t\tPoints:\t" << points << endl;
    cout << Diary[1] << "'s health:\t\t" << ally2 << "\t\tPoints:\t" << allypoints << "\tItems:\t" << YI2 << endl;
    cout << "Team 2: " << endl;
    for (int x = 0; x < 7; x++)
    {
    cout << "Cult person " << x << "'s health:\t" << enemies[x] << endl;
    }
    cout << " " << endl;
    cout << " " << endl;
    system("pause");
    cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;

                    team1 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;
                      team2 = (rand() %
                 (MAX_VALUE - MIN_VALUE + 1)) +
                  MIN_VALUE;


    if (team1 == team2)
    {
        cout << " " << endl;
        cout << "The two sides look at each other, ready to fight." << endl;
        cout << " " << endl;
    }
    else if (team1 < team2)
    {
    cout << " " << endl;
    int persons;
    int Takeo, Yuno;
    persons = (rand()%(MAX_VALUE - MIN_VALUE - 2))+0;
