cout << " " << endl;
cout << " " << endl;
    }
else if(attacking != random)
    {
    cout << Protagonist << " takes the first lead and lands a punch" << endl;
    cout << "45 damage!" << endl;
    cout << " " << endl;
    cout << Diary[1] << " takes off for the second attack" << endl;
