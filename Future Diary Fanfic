        }

             if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp;
    exp = 1;
    points = points + 500;
    health = 2000 * lvl;
    strength = strength + 25;

    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
cout << "\n\n\n" << endl;
system("pause");
    }while(health > 0 && enemy > 0);

    armor = health;

}

