         {
             item = item - 5;
             cout << Protagonist << " increased his strength!" << endl;
             cout << " " << endl;
             increase = 5;
             timer = 21;
         }
     }


     cout << " " << endl;
     if(timer <= 5 || timer >= 1)
     {
         timer--;
         increase = 1;

     }
     else if(timer > 5)
     {
         increase = 5;
     }

            if(attacking == 0)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 1)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 2)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 3)
        {
            enemies[attacking] = enemy;
        }
        else if(attacking == 4)
        {
            enemies[attacking] = enemy;
        }
    }

else if(going == killers)
{
    int attackers, continue1 = 1;
    string killer;
    do
    {
        attackers = rand() % 5;
        if(enemies[attackers] >= 1)
        {
            if(attackers == 0)
            {
                killer = "Enemy 1";
            }
            else if(attackers == 1)
            {
                killer = "Enemy 2";
            }
            else if(attackers == 2)
            {
                killer = "Enemy 3";
            }
            else if(attackers == 3)
            {
                killer = "Enemy 4";
            }
            else if(attackers == 4)
            {
                killer = "Enemy 5";
            }
            continue1++;
        }
    } while(continue1 == 1);

cout << " " << endl;
cout << killer << " turn." << endl;
cout << " " << endl;
cout << " " << endl;
cout << killer << " charges at " << Protagonist << "." << endl;
cout << " " << endl;
attack = rand() % 3;
dodge = rand() % 3;
if(attack == dodge)
{
    cout << Protagonist << " dodged " << killer << "'s knife attack." << endl;
    cout << "0 damage." << endl;
    cout << " " << endl;
    exp = exp + 5;
    points = points + 3;
    if(points >= 1000)
    {
        item = item + 10;
    }
}
else if(attack != dodge)
{
    cout << killer << " stabbed " << Protagonist << "." << endl;
    cout << "40 damage!" << endl;
    cout << " " << endl;
    health = health - 40;
    cout << " " << endl;
    attack = rand() % 2;
    dodge = rand() % 2;
    if(attack != dodge)
    {
        cout << killer << " goes for another stab at " << Protagonist << " and causes some serious damage." << endl;
        cout << "55 damage!" << endl;
        cout << " " << endl;
        health = health - 55;
    }
    else if(dodge == attack)
    {
        item++;
        points  = points + 10;
        exp = exp + 8;
    }
}


}





            if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp;
    exp = 1;
    health = 20 * lvl/2;
    strength = strength + 8;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
cout << "\n\n\n\n\n" << endl;
enemyteam = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4];
if(enemyteam == 0)
{
    victor = 1;
}
else if(health == 0)
{
    victor = 2;
}
system("pause");
cout << "\n\n\n\n\n" << endl;
    }while (victor == 0);

   if(victor == 1)
   {
       lvl = States(strength, Protagonist, lvl, armor, points);
   }
  return victor;


}


int Fight3(int& item,int& armor, int& points, int& strength, int& lvl, int& team, string Protagonist, string Diary[13])
{
    int teamally, attack, dodge, going,going2, killers = 1, Takeo = 0, Ally = 2500, AI = 11, allypoints = 950, health = armor * (lvl/2), enemyteam, enemies[3]={1500, 1000, 2500}, superattack, enemy, increase = 1, timer = 0, exp = 0, maxexp = 100, allystrength = 15 + bat;
    string attacked, killer;
    do
    {
        cout << "\n\n\n\n\n" << endl;
