            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << Gods[1] << " misses her attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                exp = exp + 10;
                points = points + 15;
                allypoints = allypoints + 30;
                health++;
                Minene++;
            }
            else if(attack != dodge)
            {
                cout << Gods[1] << " landed a destructive blow to " << target << "." << endl;
                cout << "500 damage!" << endl;
                cout << " " << endl;
                ally = ally - 500;
            }
        }
        else if(charge == 2)
        {
            cout << " " << endl;
            cout << Gods[1] << " fires death spheres at " << target << "." << endl;
            cout << " " << endl;
            attack = rand()% 3;
            dodge = rand() % 3;
            if(dodge == attack)
            {
                cout << target << " dodges the death spheres." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                exp = exp + 5;
                points = points + 50;
                allypoints = allypoints + 100;
                health = health + 25;
                Minene = Minene + 50;
            }
            else if(dodge != attack)
            {
                cout << Gods[1] << " landed multiple hits at " << target << "." << endl;
                cout << "750 damage!" << endl;
                cout << " " << endl;
                ally = ally - 750;
            }
        }
                if(going3 == 0)
        {
            health = ally;
        }
        else if(going3 == 1)
        {
            Minene = ally;
        }
        }




         if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp;
    exp = 1;
    points = points + 200;
    health = 600 * lvl;
    strength = strength + 15;

    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
cout << "\n\n\n" << endl;
system("pause");
turn++;
}while(turn <= 30 && Minene >= 1);


}

int FinalFight(int& strength, int& lvl, int& item, int& points, int& armor, string Protagonist, string Gods[2])
{
          int health = armor * lvl, enemy = 10000000, superattack, increase, timer = 0, attack, dodge, going, turn = 0, exp = 1, maxexp = 100;
    string target = Protagonist, attacked = Gods[1];

    do
    {
        going = rand()%2;
        if(going == 0)
        {
        int choice, attacking;
