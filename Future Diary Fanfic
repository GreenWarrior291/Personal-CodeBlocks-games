        exp = exp + 10;
    }
}

}

else if (choice == 3 && chosen2 == choice)
{
    cout << Protagonist << " chooses to use an item." << endl;
    cout << " " << endl;
    {
        cout << Protagonist << " heals himself." << endl;
        cout << "Health was " << health;
        health = health + 25;
        cout << ", now it is " << health << endl;
        cout << " "  << endl;
        item = item - 1;
    }
}

