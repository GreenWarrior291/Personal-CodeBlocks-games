    }
    else if(AI >= 6)
    {
        object++;
        cout << object << ". Strength of love\t6" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> direction;
    }while(direction > object || direction < 1);
    cout << " " << endl;
    cout << " " << endl;
    if(direction == 1)
    {
        AI--;
        cout << "Who do you wish to heal? " << endl;
        cout << "\n\n\n" << endl;
        cout << "1. " << Protagonist << "." << endl;
        cout << "2. " << Diary[5] << "." << endl;
        cout << "\n\n\n" << endl;
        do
        {
            cin >> heal;
        } while(heal > 3 || heal < 1);
        cout << "\n\n" << endl;
        if(heal == 1)
        {
            cout << Diary[5] << " chooses to heal " << Protagonist << "." << endl;
            cout << "His health was " << health;
            health = health + 85;
            cout << ", now it is " << health << "." << endl;
            cout << " " << endl;
        }
        else if(heal == 2)
        {
        cout << Diary[5] << " chooses to heal herself." << endl;
        cout << "Her health was " << Ally;
        Ally = Ally + 78;
        cout << ", now it is " << Ally << "." << endl;
        cout << " " << endl;
        }
    }
    else if(direction == 3)
    {
        AI = AI - 6;
        cout << " " << endl;
