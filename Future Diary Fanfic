int Chapter2(int Trust,std::string& load, int enemyH,int& chapter, int& friends, int& love, int& item, int& points, int allyH, string Gods[2], string Diary[13], string Supportive[5], string Protagonist,int C1[4], int C2[5], int C3[5], int C4[5], int C5[5], int C6[2], int DT[13], int ST[5], int& strength, int& armor, int& lvl, int path)
{
       friends = 0;
       love = 0;
        love2 = 0;
        saved = 0;
        saved1 = 0;
        saved2 = 2;
    Checkpoint = loading;
    int help;
    int sixth;
    int Yuno_fate;
    int training = 0, discovery = 0;
 if(Checkpoint == 0)
 {
do
  {
              cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;
        cout << " " << endl;
        cout << Gods[0] << ": Let us see what you've done so far, " << Protagonist << endl;
        cout << " " << endl;
        cout << " " << endl;
        system("pause");
        cout << " " << endl;
        cout << " " << endl;
        cout << Gods[0] << ": " << "Regarding your confrontation with the Third..." << endl;
        cout << " " << endl;
        cout << " " << endl;
        if (C1[0]==1)
        {
        cout << Gods[0] << ": Having just been there to record The First's miracle of avoiding the DEAD END was impressive." << endl;
        }
        else if(C1[0]==2)
        {
        cout << Gods[0] << ": Having defeated the Third was unexpected but impressive as it did created a miracle for the First." << endl;
        }
        cout << " " << endl;
        cout << " " << endl;
        system("pause");
        cout << " " << endl;
        cout << " " << endl;
        cout << Gods[0] << ": " << "Regarding your first confrontation with " << Diary[9] << ", the ninth..." << endl;
        cout << "\n\n" << endl;
        if(C1[1]==1)
        {
        cout << Gods[0] << ": " << "Having attacked the ninth was rather interesting, something not even the ninth herself expected." << endl;
        }
        else if(C1[1]==2)
        {
        cout << Gods[0] << ": " << "Protecting the First was a wise choice, showing your dedication towards your best friend." << endl;
        }
        cout << " " << endl;
        cout << " " << endl;
        system("pause");
        cout << " " << endl;
        cout << Gods[0] << ": " << "During your point in avoiding the ninth's attacks," << endl;
        if(C1[2]==1)
        {
        cout << Gods[0] << ": " << "Having been captured together is something I expected to have gone yet your bravery in confronting the ninth" << endl;
        cout << "gave you great credit to the Fourth, seeing a possible candidate for his police force.  You also earned my respect." << endl;
        }
        else if(C1[2]==2)
        {
        cout << Gods[0] << ": " << "Having been captured on your own to protect the first did shifted how I expected things to have gone, you not" << endl;
        cout << "only earned the respect of the second for protecting the first but you earned my respect for protecting your best friend." << endl;
        }
        cout << " " << endl;
        cout << " " << endl;
        system("pause");
        cout << "\n\n" << endl;
        cout << Gods[0] << ": " << "Lastly, having listened to the fourth, " << Diary[3] << "..." << endl;
        if(C1[3]==1)
        {
        cout << Gods[0] << ": " << "Having listened to the Fourth might have kept you your strength but you will be missing some information on what happened with the first." << endl;
        armor = 100;
        }
