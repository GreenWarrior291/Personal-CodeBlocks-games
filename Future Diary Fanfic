            {
                cout << "Cult person " << persons << " lands a deadly hit on both " << Diary[1] << " and " << Protagonist << endl;
                cout << " " << endl;
                cout << "10 damage each." << endl;
                ally2 = ally2 - 10;
                health = health - 10;
