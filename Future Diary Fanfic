        if (exp >= 100)
{
    exp = 1;
    lvl++;
    health = 100;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
}
    system("pause");
    if (teamhealth < 0)
    {
cout << " " << endl;
cout << " " << endl;
armor = health;
system("pause");
return armor;
    }
    else if (enemyteam < 0 || turn == 25)
    {
cout << " " << endl;
cout << " " << endl;
armor = health;
system("pause");
return armor;
    }

    }while (battle > 0);



    }

int DiariesFight(int& points, int& strength, string Protagonist, string Diary[13], int& item, int& armor, int& victor, int& lvl)
{
   int health = armor * (lvl/4);
   victor = 0;
   int superattack, exp, maxexp = 100*lvl, enemy1 = 3500, Ally = 1000, allies, enemy = 4000, enemies, allypoints = 1000, AI = 10, attack, dodge, going, team1 = 0, team2 = 1;
   do
   {
       cout << "\n\n\n\n\n\n\n\n\n\n\n\n" << endl;
       cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titems:\t" << item << endl;
       cout << Diary[5] << "'s health:\t" << health << "\tpoints:\t" << allypoints << "\titems:\t" << AI << endl;
       cout << "\n\n" << endl;
       cout << Diary[6] << "'s health:\t" << enemy << endl;
       cout << Diary[7] << "'s health:\t" << enemy1 << endl;
       cout << "\n\n\n\n\n" << endl;
       going = rand() % 2;
       if(going == team1)
       {
           int going2, Takeo = 0, Tsubaki = 1;
           if(health > 0 && Ally > 0)
           {
               going2 = rand() % 2;
           }
           else if(health <= 0)
           {
               going2 = 1;
           }
           else if(Ally <=0)
           {
               going2 = 0;
           }
        cout << " " << endl;
if(going2 == Takeo)
{
int choice, chosen, chosen1, chosen2, chosen3,chosen4, attacking, diary1 = 1, diary = 0;
if(enemy1 > 0 && enemy > 0)
{
    cout << " " << endl;
    cout << "Who do you choose to attack?" << endl;
    cout << "\n\n\n" << endl;
    cout << "1. " << Diary[6] << "." << endl;
    cout << "2. " << Diary[7] << "." << endl;
    cout << "\n\n" << endl;
    do
    {
        cin >> attacking;
    }while(attacking < 1 || attacking > 2);
cout << " " << endl;
cout << " " << endl;
}
else if(enemy1 <= 0)
{
    attacking = 1;
}
else if(enemy <=0)
{
    attacking = 0;
}
if(attacking == diary)
{
 cout << Protagonist << " turn." << endl;
 cout << "\n\n\n" << endl;
 int maxi = 2, x = 1;
 cout << x << "." << "  Attack." << endl;
 x++;
 cout << x << "." << " Skip turn" << endl;
 if (points >= 5)
 {
     x++;
     chosen = x;
     maxi++;
     cout << x << "." << " Super Attack \t5" << endl;
 }
 else if (lvl >= 6)
 {
     x++;
     chosen1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    chosen2 = x;
     cout << x << "." << " Items\t1" << endl;
 }
 cout << " " << endl;
 cout << " " << endl;
 do
 {
     cin >> choice;
 } while(choice > maxi || choice < 1);
int superattack;
cout << "\n\n\n" << endl;

 if (choice == 1)
 {
     superattack = 2;
     cout << Protagonist << " chose to attack " << Diary[6] << "." << endl;
     cout << " " << endl;
     attack = rand() % 5;
     dodge = rand() % 5;
     if (attack == dodge)
     {
         cout << "Sadly " << Diary[6] << " dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[6] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 exp = exp + lvl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << Diary[6] << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
     }
     else if (attack != dodge)
     {
         superattack = superattack*(strength/10);
         cout << Protagonist << " lands an attack on " << Diary[6] << "." << endl;
         cout << superattack << " damage!" << endl;
         cout << " " << endl;
         enemy = enemy - superattack;
                 exp = exp + lvl;
                 points++;
     }
      }
else if (choice == 2)
{
    cout << Protagonist << " chooses to skip his turn." << endl;
    cout << " " << endl;
}

else if (choice == 3 && chosen==choice)
{
superattack = 4;
cout << Protagonist << " chooses to do a very powerful attack at " << Diary[6] << endl;
cout << " " << endl;
attack = rand() % 4;
dodge = rand() % 4;
if(attack == dodge)
{
 cout << "Lucky for " << Diary[6] << ", he managed to dodge the attack, barely." << endl;
    cout << "0 damage." << endl;
    points = points - 5;
}
else if (attack != dodge)
{
    superattack = superattack*(strength/5);
    cout << Protagonist << " landed a strong blow at " << Diary[6] << "." << endl;
    cout << superattack << " damage!" << endl;
    cout << " " << endl;
    enemy = enemy - superattack;
    exp = exp + (lvl*2);
    points = points - 5;
}

}
else if (choice == 3 && chosen1 == choice)
{
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5;
        cout << Protagonist << " goes to strike down " << Diary[6] << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << Diary[6] << ", he dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[6] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 exp = exp + lvl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << Diary[6] << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << Diary[6] << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on " << Diary[6] << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7;
        cout << Protagonist << " goes to give a strong kick to " << Diary[6] << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << Diary[6] << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
        cout << Protagonist << " misses the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(attack != dodge)
    {
        superattack = superattack * (strength/2);
        cout << Protagonist << " lands a very deadly blow to " << Diary[6] << "." << endl;
        cout << superattack << " damage" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
        exp = exp + 10;
    }
}
}

else if (choice == 4 && chosen1 == choice)
{
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
            else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5;
        cout << Protagonist << " goes to strike down " << Diary[6] << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << Diary[6] << ", he dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << Diary[6] << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 exp = exp + lvl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << Diary[6] << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << Diary[6] << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on " << Diary[6] << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7;
        cout << Protagonist << " goes to give a strong kick to " << Diary[6] << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
