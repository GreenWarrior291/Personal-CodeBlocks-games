int savefile(int& Checkpoint,int Trust,std::string& load, int enemyH,int& chapter, int& friends, int& love, int& item, int& points, int allyH, string Gods[2], string Diary[13], string Supportive[5], string Protagonist,int C1[4], int C2[5], int C3[5], int C4[5], int C5[5], int C6[2], int DT[13], int ST[5], int& strength, int& armor, int& lvl, int path)
{

ofstream fout;
fout.open("Future.txt");
if (!fout)
{
cout << " " << endl;
cout << "..." << endl;
system("pause");
}
else
{
    loading = Checkpoint;
    fout << Checkpoint << endl;
    fout << strength << endl;
    fout << item << endl;
    fout << lvl << endl;
    fout << points << endl;
    fout << armor << endl;
    fout << DT[0] << endl;
    fout << DT[1] << endl;
    fout << DT[2] << endl;
    fout << DT[3] << endl;
    fout << DT[4] << endl;
    fout << DT[5] << endl;
    fout << DT[6] << endl;
        fout << DT[7] << endl;
    fout << DT[8] << endl;
    fout << DT[9] << endl;
        fout << DT[10] << endl;
    fout << DT[11] << endl;
    fout << DT[12] << endl;
            fout << ST[0] << endl;
                    fout << ST[1] << endl;
                            fout << ST[2] << endl;
                                    fout << ST[3] << endl;
                                            fout << ST[4] << endl;
    fout << chapter << endl;
    fout << friends << endl;
    fout << love << endl;
        for(int x = 0; x < 4; x++)
    {
        fout << C1[x] << endl;
    }
        for(int x = 0; x < 5; x++)
    {
        fout << C2[x] << endl;
    }
            for(int x = 0; x < 5; x++)
    {
        fout << C3[x] << endl;
    }
            for(int x = 0; x < 5; x++)
    {
        fout << C4[x] << endl;
    }
        for(int x = 0; x < 5; x++)
    {
        fout << C5[x] << endl;
    }
    fout << love2 << endl;
    fout << saved << endl;
    fout << saved1 << endl;
    fout << saved2 << endl;
}
cout << " " << endl;
fout.close();

}

int loadfile(int& Checkpoint, int Trust,std::string& load, int enemyH,int& chapter, int& friends, int& love, int& item, int& points, int allyH, string Gods[2], string Diary[13], string Supportive[5], string Protagonist,int C1[4], int C2[5], int C3[5], int C4[5], int C5[5], int C6[2], int DT[13], int ST[5], int& strength, int& armor, int& lvl, int path)
{
cout << " " << endl;
cout << "Make sure Future.txt is there." << endl;
cout << " " << endl;
system("pause");
ifstream fin;
fin.open("Future.txt");
if (!fin)
{
    cout << " " << endl;
    cout << "File not found." << endl;
    cout << " " << endl;
    cout << "please assure that Future.txt is there." << endl;
    cout << " " <<endl;
    return -1;
}
else
{
    fin >> Checkpoint;
    fin >> strength;
    fin >> item;
    fin >> lvl;
    fin >> points;
    fin >> armor;
    fin >> DT[0];
    fin >> DT[1];
    fin >> DT[2];
    fin >> DT[3];
    fin >> DT[4];
    fin >> DT[5];
    fin >> DT[6];
        fin >> DT[7];
    fin >> DT[8];
    fin >> DT[9];
        fin >> DT[10];
    fin >> DT[11];
    fin >> DT[12];
            fin >> ST[0];
                    fin >> ST[1];
                            fin >> ST[2];
                                    fin >> ST[3];
                                            fin >> ST[4];
    fin >> chapter;
    fin >> friends;
    fin >> love;
        for(int x = 0; x < 4; x++)
    {
        fin >> C1[x];
    }
        for(int x = 0; x < 5; x++)
    {
        fin >> C2[x];
    }
            for(int x = 0; x < 5; x++)
    {
        fin >> C3[x];
    }
            for(int x = 0; x < 5; x++)
    {
        fin >> C4[x];
    }
        for(int x = 0; x < 5; x++)
    {
        fin >> C5[x];
    }

}
fin.close();
cout << " " << endl;
loading = Checkpoint;
cout << "File save found." << endl;
cout << " " << endl;
system("pause");
    if (chapter == 0)
    {
     Checkpoint = Chapter0(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
    }
    else if (chapter == 1)
    {
    Checkpoint = Chapter1(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
    }
        else if (chapter == 2)
    {
    Checkpoint = Chapter2(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
    }
        else if (chapter == 3)
    {
    Checkpoint = Chapter3(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
    }
        else if (chapter == 4)
    {
    Checkpoint = Chapter4(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
    }
        else if (chapter == 5)
    {
    Checkpoint = Chapter5(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
    }
    else if(chapter == 6)
    {
        Checkpoint = Chapter6(Trust, load, enemyH, chapter, friends, love, item, points, allyH, Gods, Diary, Supportive, Protagonist, C1, C2, C3, C4, C5, C6, DT, ST, strength, armor, lvl, path);
    }



}

int Chapter0(int Trust,std::string& load, int enemyH,int& chapter, int& friends, int& love, int& item, int& points, int allyH, string Gods[2], string Diary[13], string Supportive[5], string Protagonist,int C1[4], int C2[5], int C3[5], int C4[5], int C5[5], int C6[2], int DT[13], int ST[5], int& strength, int& armor, int& lvl, int path)
{
cout << " " << endl;
cout << "Note: Before starting the game, please have Future.txt in the folder to save progress." << endl;
cout << " " << endl;
system("pause");
cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;
cout << "Chapter 0: The beginning" << endl;
cout << "\n\n\n\n\n\n\n\n\n" << endl;
cout << "This story takes place a year before the games." << endl;
cout << "Deus is just observing and yet he sensed something was off." << endl;
system("pause");
cout << "\n\n\n";
cout << Gods[0] << ": " << "Hmm, I sense a change." << endl;
cout << " " << endl;
cout << Gods[1] << ": " << "Is something wrong?" << endl;
cout << " " << endl;
cout << Gods[0] << ": " << "It's nothing to worry about MurMur, you can return to your things." << endl;
cout << "\n\n\n";
cout << "Deus goes into his sleeping quarters and begins to create something on his own," << endl;
cout << "assuring that things go how he hopes they do." << endl;
cout << " " << endl;
system("pause");
cout << " " << endl;
cout << " " << endl;
cout << Gods[0] << ": " << "Let us create something in the case my sense were correct that things are off." << endl;
cout << " " << endl;
cout << Gods[0] << ": Hmm, alright I know who to create." << endl;
cout << " " << endl;
cout << Gods[0] << ": " << Protagonist << ", 5'10 on height, 14 year old boy, brown hair, brave, and someone who can make choices with much thought (like a strategist)." << endl;
cout << " " << endl;
cout << " " << endl;
cout << Gods[0] << ": " << "Yes, you will serve me well," << Protagonist << "." << endl;
cout << " " << endl;
cout << Gods[0] << ": " << "Now go my creating and assure the survival of this world with your choices. \n\n" << endl;
system("pause");
cout << "\n\n\n" << endl;
cout << "Depending on your choice, will depending how your abilities are and how the others will treat you." << endl;
    system("pause");
        strength = 46;
