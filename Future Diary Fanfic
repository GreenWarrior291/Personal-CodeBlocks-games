    {
        reload = bullets;
        gun = gun + reload;
        if(gun>6)
        {
            gun = 6;
        }
    }
    else if(bullets >= 6)
    {
        bullets = bullets - reload;
        gun = 6;
    }
 reload = 0;
}
else if(choice == 4 && chosen4 == choice)
{
    cout << Protagonist << " decides to reload his gun." << endl;
    cout << " " << endl;
    if(bullets < 6)
    {
        reload = bullets;
        gun = gun + reload;
        if(gun>6)
        {
            gun = 6;
        }
    }
