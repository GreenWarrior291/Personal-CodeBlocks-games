enemies[choices] = enemy;
}
    else if(going == cop)
    {
        int copper, cont;
        cont = 0;
        do
        {
         copper = rand() % 5;
         if(enemies[copper] > 0)
         {
             cont = 1;
         }
        } while (cont == 0);
        cout << " " << endl;
        cout << "Cop " << copper+1 << " turn." << endl;
        cout << " " << endl;
        attack = rand() % 10;
        dodge = rand() % 10;
        if(attack != dodge)
        {
            cout << " " << endl;
            cout << "Cop " << copper+1 << " attacks " << Protagonist << " and does damage to him." << endl;
            cout << "20 damage." << endl;
            cout << " " << endl;
            health = health - 20;
        }
        else if (attack == dodge)
        {
            cout << " " << endl;
            cout << "Cop " << copper+1 << " missed." << endl;
            cout << "0 damage" << endl;
            cout << " " << endl;
            health = health + 10;
            exp = exp + 2;
            points++;
        }
    }
    if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp * lvl;
    exp = 1 * lvl;
    health = 100;
    strength = strength + 5;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
team = enemies[0] + enemies[1] + enemies[2] + enemies[3] + enemies[4];
cout << " " << endl;
system("pause");
    if(health < 1)
    {
        armor = health;
        return armor;
    }
    } while(team > 1);

armor = health;
return armor;
}

int minigame3 (int& strength, int& points, int& lvl, string Protagonist, string Diary[13])
{
    int continue1;
    cout << "\n\n\n\n" << endl;
    cout << Diary[9] << ": " << "This is how it'll work.  Takeo will choose between one of you two who to do a combo attack.  As Takeo makes his choice," << endl;
    cout << "you two will do a combo attack together." << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << "Choose who you want to team up with." << endl;
    cout << " " << endl;
    cout << " " << endl;
    cout << "1. Tsubaki." << endl;
    cout << "2. Yuno" << endl;
    cout << " " << endl;
    cout << " " << endl;
    int choose, attacking, random, attack, dodge;
    do
    {
        cin >> choose;
    } while (choose > 2 || choose < 1);
    cout << " " << endl;
    cout << " " << endl;
    if(choose == 1)
    {
        cout << Protagonist << ": " << "I want Tsubaki to join me for this." << endl;
        cout << " " << endl;
        cout << Diary[5] << ": " << "Alright, cool!  I won't let you down." << endl;
        cout << " " << endl;
    }
    else if(choose == 2)
    {
        cout << Protagonist << ": " << "I choose Yuno to assist me with this." << endl;
        cout << " " << endl;
