    cout << "Team 2: " << endl;
    for (int x = 0; x < 10; x++)
    {
    cout << "Cult person " << x << "'s health:\t" << enemies[x] << endl;
    }
    cout << " " << endl;
    cout << " " << endl;
    system("pause");
    cout << "\n\n\n\n\n\n\n\n\n\n\n\n\n" << endl;

