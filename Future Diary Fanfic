int DogFight (int& lvl, int& strength, int& armor, int& points, int& item, string Protagonist)
{
    int health = armor;
    int attack, dodge, going, SuperAttack, choose, choice, attackers, turns = 0, turn = 0;
    int students[20] = {100,100,100,100,100,100,100,100,100,100,100,100,100,100,100,50,75,83,99,25};
    int exp = 1, expmax = 100;
    int nextturn = 1;
    do
        {
            int team1 = 0, team2 = 1;
            going = rand() % 2;

        if (team1 == going)
        {
            cout << " " << endl;
    cout << Protagonist <<"'s turn." << endl;
            cout << " " << endl;
            cout << "\n\n" << endl;
            cout << "Choose which dog to attack" << endl;
            cout << " " << endl;
                for (int y = 0; y < 20; y++)
        {
        if (students[y]> 0)
        {
            cout << "Dog " << y+1 << endl;
        }

        }
        cout << " " << endl;
        do
        {
            cin >> choose;
            choose = choose - 1;
            if (students[choose] > 0 )
            {
                attackers++;
            }
            else if (students[choose] <= 0)
            {
                cout << "choose another opponent." << endl;
                cout << " " << endl;
            }
        }while (attackers == 1);


        attackers = 1;


        cout << " " << endl;
        cout << " " << endl;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n\n" << endl;
        int options,path,path2, maximumchoice = 2, x = 1;
          cout << x << ".\t Attack." << endl;
          x++;
          cout << x << ".\tskip." << endl;
          path = 0;
          path2 = 0;
          if (points >= 5)
          {
              x++;
              maximumchoice++;
              path = x;
              cout << x << ".\tSuper Attack." << endl;
          }


          if (item > 0)
          {
              maximumchoice++;
              x++;
              path2 = x;
              cout << x << ".\t Healing." << endl;
          }
        cout << "\n\n\n\n" << endl;
        do
        {
            cin >> options;
        } while (options < 0 || options > maximumchoice);

        if (options == 1)
        {
            SuperAttack = 2;
            cout << " " << endl;
            cout << Protagonist << " gives an attack at ";
            cout << "Dog " << choose+1 << "." << endl;
            cout << " " << endl;
            cout << " " << endl;
            attack = rand() % 15;
            dodge = rand() % 15;
            if (attack == dodge)
            {
                cout << Protagonist << " misses." << endl;
                cout << "No damage!" << endl;
                cout << " " << endl;
            }
            else if (attack != dodge)
            {
                SuperAttack = SuperAttack*(strength/10);
                students[choose] = students[choose] - SuperAttack;
                cout << Protagonist << " manages to land a blow at ";
            cout << "Dog " << choose+1 << "." << endl;
                cout << SuperAttack << " damage!" << endl;
                points++;
                exp++;
            }
        }
        else if (options == 2 )
        {
    cout << " " << endl;
    cout << Protagonist << " skips his turn." << endl;
    cout << " " << endl;
        }
                else if (options == 3 && path == 3)
        {
            SuperAttack = 4;
            cout << " " << endl;
            cout << Protagonist << " gives a strong attack at ";
            cout << "Dog " << choose+1 << "." << endl;
            cout << " " << endl;
            cout << " " << endl;
            attack = rand() % 10 + 1;
            dodge = rand() % 10 + 1;
            if (attack == dodge)
            {
                cout << Protagonist << " misses." << endl;
                cout << "No damage!" << endl;
                cout << " " << endl;
            }
        else if (options == 3 && path2 == 3)
        {
            SuperAttack = 4;
            cout << " " << endl;
            cout << Protagonist << " gives a strong attack at ";
            cout << "Dog " << choose+1 << "." << endl;
            cout << " " << endl;
            cout << " " << endl;
            attack = rand() % 10 + 1;
            dodge = rand() % 10 + 1;
            if (attack == dodge)
            {
                cout << Protagonist << " misses." << endl;
                cout << "No damage!" << endl;
                cout << " " << endl;
            }
            else if (attack != dodge)
            {
                cout << Protagonist << " manages to land a deadly blow at ";
                SuperAttack = SuperAttack*(strength/5);
                students[choose] = students[choose] - SuperAttack;

            cout << "Dog " << choose+1 << "." << endl;

                cout << SuperAttack << " damage!" << endl;
                points = points - 5;
                exp = exp + lvl;
            }
        }
        else if (options == path2)
        {
            cout << " " << endl;
            cout << " " << endl;
            cout << "Who do you wish to heal?" << endl;
            cout << "1. " << Protagonist << endl;
            cout << " " << endl;
            int heal;
            do
            {
                cin >> heal;
            } while (heal != 1);
            cout << " " << endl;
            cout << " " << endl;
            cout << "Takeo heal himself." << endl;
            health =+ 50;
            if (health > 100)
            {
                health = 100;
            }
            cout << "Health now: " << health << endl;
            cout << " " << endl;
        }
        turns++;
        }


        cout << " " << endl;
        if (exp >= expmax)
{
    exp = 1;
    lvl++;
    health = 100;
    strength = strength + 5;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
    cout << "\n\n\n" << endl;
}
if (points == itemachieve)
{
    itemachieve =+ 200;
    item =+ 1;
}
        }
        else if (team2 == going)
        {
        do
        {
        int person;
        person = rand() % 20;
        if (students[person] > 0)
        {
            cout << " " << endl;
            cout << "Team 2's turn." << endl;
            cout << " " << endl;
            cout << " " << endl;
            cout << "Dog " << person+1 << " goes off to attack " << Protagonist << "." << endl;
            cout << " " << endl;
        system("pause");
        cout << "\n\n\n" << endl;
                attack = rand() % 10;
        dodge = rand() % 10;

        if (attack == dodge)
        {
            cout << "Dog " << person+1 << " land a hit on " << Protagonist << "." << endl;
            cout << "5 damage!" << endl;
            cout << " " << endl;
            health = health - 5;
        }
        else if (attack != dodge)
        {
            cout << "Dog " << person+1 << " missed." << endl;
            cout << "No damage." << endl;
            cout << " " << endl;
            points++;
            exp++;
        }
        turn++;
        }

        }while (turn < nextturn);
        turns++;
        }
nextturn++;

} while (turns <= 55);
if(health <= 0)
{
    armor = health;
    return armor;
}
else if (health >= 1)
{
    armor = health;
    lvl = States(strength, Protagonist, lvl, armor, points);
    armor = 75;
    return armor;
}
}

int DiaryFight(int& lvl, int& strength, int& armor, int& points, int& item, int& victor, string Protagonist, string Diary[13])
{
    int exp = 1;
    int enemy = 1000;
    int ally1 = 250;
    int ally2 = 700;
    int health = armor;
    int protection;
    int battle = 1;
    int DiaryFinisher = 980;
    int teamhealth;
    int allypoints = 150;
    int YI = 5;
    int YI2 = 1;
    int attack, dodge, action, action2, SuperAttack;
    int enemypoints = 500;
    int going, going2, going3, team1, team2;
    const int MIN_VALUE = 1,
              MAX_VALUE = 5;

    unsigned seed = time(0);


    do
    {
    cout << "Team 1: " << endl;
    cout << Protagonist << "'s health:\t\t" << health << "\tPoints:\t" << points << endl;
    cout << Diary[0] << "'s health:\t\t" << ally1 << "\tPoints:\t" << allypoints << "\tItems:\t" << YI << endl;
    cout << Diary[1] << "'s health:\t\t" << ally2 << "\tPoints:\t" << allypoints << "\tItems:\t" << YI2 << endl;
    cout << "Team 2: " << endl;
    cout << Diary[2] << "'s health:\t" << enemy << endl;
    cout << "\n\n\n" << endl;
            team1 = 0;
            team2 = 1;
            int contin;
        int goingoff = rand() % 2;

if (team1 == goingoff)
{
    contin = 1;
  cout <<  " " << endl;
  cout << Diary[2] << " turn. " << endl;
  cout << " " << endl;
  int choosing, yuno, you, yukki;
              yukki = 1;
                      yuno = 2;
                      you = 0;
do
{
        choosing = rand() % 3;

  if(choosing == yuno && ally2 > 0)
  {
    if (enemypoints > 75)
    {
        action2 = rand() % 2;

        if (action2 == 0)
        {
        cout << Diary[2] << " chooses to attack with his knife." << endl;
        cout << " " << endl;
                          dodge = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
                      attack = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
            if (attack != dodge)
            {
                cout << Diary[2] << " lands a deadly hit on " << Diary[1] << endl;
                cout << " " << endl;
                cout << "25 damage." << endl;
                ally2 = ally2 - 25;
                enemypoints = enemypoints + 5;
            }
            else if (attack == dodge)
            {
                cout << Diary[2] << " misses." << endl;
                cout << " " << endl;
                cout << "No damage." << endl;
            }
        }
        else if (action2 == 1)
        {
            cout << Diary[2] << " chooses to give a lethal blow." << endl;
            cout << " " << endl;
                                      dodge = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
                      attack = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
            if (attack != dodge)
            {
                cout << Diary[2] << " manages to give a lethal blow to " << Diary[1] << endl;
                cout << " " << endl;
                cout << "50 damage." << endl;
                ally2 = ally2 - 50;
                enemypoints = enemypoints - 75;
            }
            else if (attack == dodge)
            {
                cout << Diary[2] << " misses and ends up getting countered by " << Diary[1] << endl;
                cout << " " << endl;
                cout << "Took 25 damage." << endl;
                enemy = enemy - 25;
                enemypoints = enemypoints - 75;
            }
        }
    }
    else if (enemypoints < 75)
    {

        cout << Diary[2] << " chooses to attack with his knife." << endl;
        cout << " " << endl;
                          dodge = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
                      attack = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
            if (attack != dodge)
            {
                cout << Diary[2] << " lands a deadly hit on " << Diary[1] << endl;
                cout << " " << endl;
                cout << "25 damage." << endl;
                ally2 = ally2 - 25;
                enemypoints = enemypoints + 5;
            }
            else if (attack == dodge)
            {
                cout << Diary[2] << " misses." << endl;
                cout << "No damage." << endl;
                cout << " " << endl;
            }
    }
    contin++;
  }
  else if(yukki == choosing && ally1 > 0)
  {
    if (enemypoints > 75)
    {
        action2 = rand() % 2;
        if (action2 == 0)
        {
            if (protection < 1)
            {
        cout << Diary[2] << " chooses to attack with his knife." << endl;
        cout << " " << endl;
                          dodge = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
                      attack = (rand() %
                 (MAX_VALUE - MIN_VALUE )) +
                  MIN_VALUE;
            if (attack != dodge)
            {
                cout << Diary[2] << " lands a deadly hit on " << Diary[0] << endl;
                cout << " " << endl;
                cout << "25 damage." << endl;
                ally1 = ally1 - 25;
                enemypoints = enemypoints + 5;
            }
                else if (attack == dodge)
            {
                cout << Diary[2] << " misses." << endl;
                cout << " " << endl;
                cout << "No damage." << endl;
            }
        }
        else if (protection > 0)
        {
            cout << Diary[2] << " tries to attack " << Diary[0] << " but he has a defense preventing him." << endl;
            cout << " " << endl;
            cout << "No damage." << endl;
            protection = protection - 1;
        }
        }
        else if (action2 == 1)
        {
