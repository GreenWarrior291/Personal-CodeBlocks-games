 else if(attack != dodge)
 {
     cout << Supportive[0] << " manages to damage " << Protagonist << "." << endl;
     cout << "250 damage." << endl;
     cout << " " << endl;
     health = health - 250;
 }
}
         if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp;
    exp = 1;
    health = 100 * lvl/2;
    strength = strength + 8;

    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
    system("pause");
}
if(health <= 0)
{
    victor = 1;
    lvl = States(strength, Protagonist, lvl, armor, points);
}
else if(enemy <= 0)
{
    victor = 2;
    armor = 0;
}
}while(victor == 0);

return armor;
}

int DiaryFight8(int& item, int& strength, int& points, int& armor, int& lvl, int& victor, string Protagonist, string Diary[13])
{
    int health = armor * (lvl/2), Yuno = 100000, superattack, enemy, increase, timer = 0, attack, dodge, going, turn = 0, exp = 1, maxexp = 100;
    string target = Protagonist, attacked = Diary[1];

victor = 0;
do
{
    cout << "\n\n\n\n\n" << endl;
    cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titem:\t" << item << "." << endl;
    cout << "\n\n" << endl;
    cout << Diary[1] << "'s health:\t" << Yuno << "." << endl;
    cout << "\n\n" << endl;
    going = rand() % 2;
              if(going == 0)
        {
        enemy = Yuno;
        int choice, attacking, continue1 = 1;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    pat2 = x;
     cout << x << "." << " Items" << endl;
 }
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> choice;
        } while(choice > y || choice < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(choice == 1)
        {
            superattack = 2;
            cout << Protagonist << " goes to attack " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 4;
            dodge = rand() % 4;
            if(attack == dodge)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/4);
                cout << Protagonist << " lands a hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp++;
                points++;
            }
        }
        else if(choice == 2)
        {
            cout << Protagonist << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(choice == 3 && pat == choice)
        {
            superattack = 6;
            cout << Protagonist << " charges for a super strike on " <<  attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << attacked << " dodged the super attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                points = points - 5;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/2);
                cout << Protagonist << " lands a deadly hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp = exp + 5;
                points = points - 5;
            }
        }
        else if(choice == 3 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", she dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
