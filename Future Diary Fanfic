        cout << " " << endl;
        attack = rand() % 3;
        dodge = rand() % 3;
        if(dodge == attack)
        {
            cout << Diary[6] << " managed to dodge the attack" << endl;
            cout << "0 damage." << endl;
            cout << " " << endl;
        }
        else if(dodge != attack)
        {
            cout << Diary[5] << " landed a hit on " << Diary[6] << "." << endl;
            superattack = superattack * believe;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            allypoints = allypoints + 5;
        }
        cout << " " << endl;
        if(power > 1)
        {
            power--;
        }
        else if(power == 1)
        {
            power = 0;
            believe = 1;
        }
    }
    else if(choice == 2)
    {
        cout << " " << endl;
        cout << Diary[5] << " skips her turn." << endl;
    }
    else if(choice == 3 && choice == chosen)
    {
        cout << " " << endl;
        int p = 1, pi = 1, ability, loving, powering;
        cout << "Choose an ability." << endl;
        cout << " " << endl;
        cout << p << ". Strength of self \t50" << endl;
        cout << " " << endl;
        if(health > 0)
        {
            p++;
            loving = p;
            pi++;
            cout << p << ". Strength in love \t100" << endl;
            cout << " " << endl;
        }
        else if(power == 0)
        {
            p++;
            powering = p;
            pi++;
            cout << p << ". Belief of strength \t250" << endl;
            cout << " " << endl;
        }
        cout << " " << endl;
        do
        {
            cin >> ability;
            cout << " " << endl;
        }while (ability > pi || ability < 1);
        cout << " " << endl;
        if(ability == 1)
        {
            cout << " " << endl;
            cout << Diary[5] << " uses all of her strength to attack " << Diary[6] << "." << endl;
            cout << " " << endl;
            superattack = 5;
            attack = rand() % 5;
            dodge = rand() % 5;
            if(attack == dodge)
            {
                cout << Diary[6] << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                cout << Diary[5] << " did a ton of damage." << endl;
                superattack = superattack * (10*believe);
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
            }
            allypoints = allypoints - 50;
        }
        else if(ability == 2 && ability == loving)
        {
            cout << " " << endl;
            cout << Diary[5] << " asks Takeo to join forces in attacking " << Diary[6] << "." << endl;
            cout << " " << endl;
            superattack = 3;
            attack = rand() % 6;
            dodge = rand() % 6;
            if(dodge == attack)
            {
