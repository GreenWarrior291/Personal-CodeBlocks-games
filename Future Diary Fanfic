             cout << " " << endl;
             cout << "1. Heal.       1" << endl;
             cout << "2. Strength.   5" << endl;
             cout << "\n\n" << endl;
             do
             {
                 cin >> let;
             } while(let > 2 || let < 1);
         }
         else if(timer != 0 || item < 5)
         {
             let = 1;
         }
         cout << "\n\n" << endl;
         if(let == 1)
         {
             item--;
             cout << Protagonist << " heals himself." << endl;
             cout << "His health was " << health;
             health = health + 95;
             cout << ", now it is " << health << "." << endl;
             cout << " " << endl;
         }
         else if(let == 2)
         {
             item = item - 5;
             cout << Protagonist << " increased his strength!" << endl;
             cout << " " << endl;
             increase = 5;
             timer = 21;
         }
     }


     cout << " " << endl;
     if(timer <= 5 || timer >= 1)
     {
         timer--;
         increase = 1;

     }
     else if(timer > 5)
     {
         increase = 5;
     }


Minene = enemy;
    }
   else if(going == 1)
   {
       cout << " " << endl;
       cout << Diary[9] << " turn." << endl;
       cout << " " << endl;
       cout << Diary[9] << " goes to strike down " << target << "." << endl;
       cout << " " << endl;
       attack = rand() % 4;
       dodge = rand() % 4;
       if(attack == dodge)
       {
           cout << target << " dodge the attack." << endl;
           cout << "0 damage." << endl;
           cout << " " << endl;
           exp++;
       }
       else if(attack != dodge)
       {
           cout << Diary[9] << " damaged " << target << " with a powerful attack." << endl;
           cout << "55 damage." << endl;
           cout << " " << endl;
           health = health - 55;
           cout << Diary[9] << " goes for a second attack." << endl;
           cout << " " << endl;
           attack = rand() % 3;
           dodge = rand() % 3;
           if(dodge == attack)
           {
               cout << target << " managed to block off the attack." << endl;
               cout << "0 damage." << endl;
               cout << " " << endl;
           }
           else if(attack != dodge)
           {
               cout << Diary[9] << " landed the second blow to " << target << "." << endl;
               cout << "75 damage!" << endl;
               cout << " " << endl;
               health = health - 75;
               cout << Diary[9] << " goes for the finishing touch." << endl;
               cout << " " << endl;
               attack = rand() % 2;
               dodge = rand() % 2;
               if(attack == dodge)
               {
                   cout << target << " deflects off the final blow." << endl;
                   cout << "0 damage." << endl;
                   cout << " " << endl;
               }
               else if(attack != dodge)
               {
                   cout << Diary[9] << " put the finishing blow towards " << target << "." << endl;
                   cout << "105 damage!" << endl;
                   cout << " " << endl;
                   health = health - 105;
               }
           }
       }
   }

cout << "\n\n\n" << endl;


    if (exp >= maxexp)
{
    lvl++;
    maxexp = maxexp;
    exp = 1;
    health = 100 * lvl/2;
    strength = strength + 8;
    cout << "\n\n\n" << endl;
    cout << Protagonist << " leveled up to level " << lvl;
    cout << "\n\n" << endl;
}

cout << "\n\n\n" << endl;
system("pause");
cout << "\n\n\n\n\n\n\n" << endl;

}while(enemy > 0 && health > 0);
lvl = States(strength, Protagonist, lvl, armor, points);
return armor;
}

int minigame5(int& codebreaker, int& lvl, string Protagonist)
{
    int code, chance = 2;
    string code1, code2, code3, input, user;
    code1 = "Yuno";
    code2 = "Gasai";
    code3 = "Family";
    codebreaker = 0;
    cout << "\n\n" << endl;
    system("pause");
    cout << " " << endl;
    cout << " " << endl;
    code = rand() % 3;
    if(code == 0)
    {
        input = code1;
    }
    else if(code == 1)
    {
        input = code2;
    }
    else if(code == 2)
    {
        input = code3;
    }
    do
    {
        cout << Protagonist << ": " << "(Okay, this is it.  I have to do this.)" << endl;
        cout << " " << endl;
        cout << " " << endl;
        cout << "Note: You will have two chances to make this write, input it word for word on which you think that is the password." << endl;
        cout << "\n\n" << endl;
        cout << code1 << ", " << code2 << ", " << code3 << "." << endl;
        cout << " " << endl;
        cout << " " << endl;
            cin >> user;
        cout << " " << endl;
        cout << " " << endl;
        if(user == input)
        {
            codebreaker = 1;
        }
        else if(user != input)
        {
            chance--;
            cout << " " << endl;
            cout << Protagonist << ": " << "Shit!  I can do this!" << endl;
            cout << "\n\n\n" << endl;
        cout << code1 << ", " << code2 << ", " << code3 << "." << endl;
        cout << " " << endl;
        cout << " " << endl;
            cin >> user;
        cout << " " << endl;
        if(user == input)
        {
            codebreaker = 1;
        }
        else if(user != input)
        {
            chance--;
        }
        }
    }while(chance > 0 && codebreaker == 0);
    return codebreaker;
}


int AkiseFight(int& armor, int& strength, int& lvl, int& points, int& item, int& victor, string Protagonist, string Supportive[5])
{

    int health = armor * (lvl/2), Akise = 10000, superattack, enemy, increase, timer = 0, attack, dodge, going, exp = 1, maxexp = 100;
    string target = Protagonist, attacked = Supportive[5];

victor = 0;
do
{
              if(going == 0)
        {
        enemy = Akise;
        int choice, attacking, continue1 = 1;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    pat2 = x;
     cout << x << "." << " Items" << endl;
 }
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> choice;
        } while(choice > y || choice < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(choice == 1)
        {
            superattack = 2;
            cout << Protagonist << " goes to attack " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 4;
            dodge = rand() % 4;
            if(attack == dodge)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/4);
                cout << Protagonist << " lands a hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp++;
                points++;
            }
        }
        else if(choice == 2)
        {
            cout << Protagonist << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(choice == 3 && pat == choice)
        {
            superattack = 6;
            cout << Protagonist << " charges for a super strike on " <<  attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << attacked << " dodged the super attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                points = points - 5;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/2);
                cout << Protagonist << " lands a deadly hit on " << attacked << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp = exp + 5;
                points = points - 5;
            }
        }
        else if(choice == 3 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", she dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on  " << attacked << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7 * increase;
        cout << Protagonist << " goes to give a strong kick to " << attacked << "." << endl;
        cout << " " << endl;
