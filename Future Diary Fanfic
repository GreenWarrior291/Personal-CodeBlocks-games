    {
        cout << " " << endl;
        cout << Diary[1] << " lands a strong kick." << endl;
        cout << "55 damage!" << endl;
        enemy = enemy - 55;
    }
    cout << " " << endl;
    cout << " " << endl;
    cout << Protagonist << " goes off to take the next attack." << endl;
    cout << "\n\n\n" << endl;
cout << "Choose a number from 1 to 4." << endl;
cout << "\n\n\n" << endl;
do
{
    cin >> attack;
} while(attack > 4 || attack < 1);
cout << "\n\n" << endl;
    dodge = rand() % 4;
    if(attack == dodge)
    {
        cout << " " << endl;
        cout << Protagonist << " misses his attack." << endl;
        cout << "0 damage" << endl;
        points = points/2;
        allypoints = allypoints/2;
    }
    else if(attack != dodge)
    {
        cout << " " << endl;
        cout << Protagonist << " lands a strong attack." << endl;
        cout << "40 damage!" << endl;
        enemy = enemy - 40;
    }
    cout << " " << endl;
    cout << " " << endl;
    cout << Protagonist << " and " << Diary[1] << " together go off to deliver the finishing blow." << endl;
cout << "\n\n" << endl;
cout << "Choose a number from 1 to 3." << endl;
cout << "\n\n\n" << endl;
do
{
    cin >> attack;
} while(attack > 3 || attack < 1);
cout << "\n\n" << endl;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << " " << endl;
        cout << Protagonist << " and " << Diary[1] << " misses their attack." << endl;
        cout << "0 damage" << endl;
        points = 0;
        allypoints = 5;
    }
    else if(attack != dodge)
    {
        cout << " " << endl;
        cout << Diary[1] << " and " << Protagonist << " land a powerful finishing blow." << endl;
        cout << "125 damage!" << endl;
        enemy = enemy - 125;
    }
    cout << " " << endl;
    cout << " " << endl;
    }
    if(choose == 1)
    {
        enemy = enemy2;
    }
    else if(choose == 2)
    {
        enemy = enemy1;
    }
}

        }

            }
            else if(choice == 3 && choice == pat1)
                {

            cout << " " << endl;
            cout << " " << endl;
            cout << Protagonist << " charges towards " << attacked << "." << endl;
            cout << " " << endl;
            superattack = 5;
            attack = rand() % 4;
            dodge = rand()% 4;
            if(attack == dodge)
            {
            cout << " " << endl;
            cout << attacked << " dodged the attack." << endl;
            cout << "0 damage." << endl;
            cout << " " << endl;
            }
            else if(attack != dodge)
            {
            cout << Protagonist << " lands a strong hit at " << attacked << "." << endl;
            superattack = superattack * (strength/4);
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy =  enemy - superattack;
            points = points - 5;
            }
                }
            else if(choice == 4 && choice == pat1)
                {

            cout << " " << endl;
            cout << " " << endl;
            cout << Protagonist << " charges towards " << attacked << "." << endl;
            cout << " " << endl;
            superattack = 5;
            attack = rand() % 4;
            dodge = rand()% 4;
