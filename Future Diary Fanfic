            }
            else if(dodge != attack)
            {
                cout << attacked << " landed a hit on " << attacked << "." << endl;
                cout << "25 damage." << endl;
                strike = strike - 25;
            }
        }
        else if(striking == 1)
        {
            cout << Supportive[4] << " goes to combo attack " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 5;
            dodge = rand() % 5;
            if(dodge == attack)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                allypoints = allypoints + 5;
                points = points + 5;
            }
            else if(dodge != attack)
            {
                cout << Supportive[4] << " took a swing and hit " << attacked << "." << endl;
                cout << "20 damage!" << endl;
                cout << " " << endl;
                strike = strike - 20;
                cout << Supportive[4] << " goes for another swing." << endl;
                cout << " " << endl;
                attack = rand() % 4;
                dodge = rand() %4;
                if(dodge == attack)
                {
                    cout << attacked << " blocked the attack." << endl;
                    cout << "0 damage." << endl;
                }
                else if(attack != dodge)
                {
                    cout << Supportive[4] << " landed another blow towards " << attacked << "." << endl;
                    cout << "20 damage" << endl;
                    strike = strike - 20;
                    cout << " " << endl;
                    cout << Supportive[4] << " goes for the finishing blow." << endl;
                    cout << " " << endl;
                    attack = rand() % 3;
                    dodge = rand() % 3;
                    if(attack == dodge)
                    {
                        cout << attacked << " luckily dodged " << Supportive[4] << "'s attack." << endl;
                        cout << "0 damage." << endl;
                    }
                    else if(attack != dodge)
                    {
                        cout << Supportive[4] << " landed the finishing blow to his combo towards " << attacked << "." << endl;
                        cout << "30 damage." << endl;
                        strike = strike - 30;
                    }
                }
            }
        }

        cout << " " << endl;
        if(player == Takeo)
        {
            health = strike;
        }
        else if(player == Yuno)
        {
            Ally = strike;
        }
        else if(player == Tsubaki)
        {
            Ally2 = strike;
        }
    }
    else if(going1 == 1)
    {
        cout << " " << endl;
        cout << Supportive[0] << " turn." << endl;
        if(packs == 1)
        {
            player = rand() % 2;
        }
        else if(packs == 2)
        {
            player = rand() % 3;
        }
        cout << " " << endl;
        if(player == Takeo)
        {
            attacked = Protagonist;
            strike = health;
        }
        else if(player == Yuno)
        {
            attacked = Diary[1];
            strike = Ally;
        }
        else if(player == Tsubaki)
        {
            attacked = Diary[5];
            strike = Ally2;
        }
        cout << " " << endl;
        striking = rand() % 3;
        if(striking == 0)
        {
            cout << " " << endl;
            cout << Supportive[0] << " charges at " << attacked << " to give a strong kick." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << attacked << " dodged " << Supportive[0] << "'s kick." << endl;
                cout << "0 damage." << endl;
                points++;
                allypoints++;
            }
            else if(attack != dodge)
            {
                cout << Supportive[0] << " landed a strong kick towards " << attacked << "." << endl;
                cout << "45 damage." << endl;
                strike = strike - 45;
            }
        }
        else if(striking == 1)
        {
            cout << " " << endl;
            cout << Supportive[0] << " charges at " << attacked << "." << endl;
            cout << " " << endl;
            attack = rand() % 5;
            dodge = rand() % 5;
            if(dodge == attack)
            {
                cout << attacked << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                points++;
                allypoints = allypoints + 2;
            }
            else if(dodge != attack)
            {
            cout << Supportive[0] << " landed a strong punch towards " << attacked << "." << endl;
            cout << "25 damage!" << endl;
            cout << " " << endl;
            strike = strike - 25;
            cout << Supportive[0] << " attempts to land a powerful kick." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
            cout << attacked << " blocks the attack and manages to land a counter attack on " << Supportive[0] << "." << endl;
            cout << "15 damage to " << Supportive[0] << "." << endl;
            cout << " " << endl;
            enemy2 = enemy2 - 15;
            points++;
            allypoints++;
            }
            else if(attack != dodge)
            {
                cout << Supportive[0] << " landed a strong kick to the gut towards " << attacked << "." << endl;
                cout << "25 damage!" << endl;
                cout << " " << endl;
                strike = strike - 25;
                cout << Supportive[0] << " takes the opportunity to go for one final finishing blow." << endl;
                cout << " " << endl;
                attack = rand() % 3;
                dodge = rand() % 3;
                if(dodge == attack)
                {
                    cout << attacked << " dodge the attack and without hesitation counter attacked" << Supportive[0] << "." << endl;
                    cout << "40 damage to " << Supportive[0] << "." << endl;
                    cout << " " << endl;
                    enemy2 = enemy2 - 40;
                    points = points + 20;
                    allypoints = allypoints + 35;
                }
                else if(attack != dodge)
                {
                    cout << Supportive[0] << " lands a powerful finishing blow to " << attacked << "." << endl;
                    cout << "25 damage!" << endl;
                    cout << " " << endl;
                    strike = strike - 25;
                }
            }
            }
        }
        else if(striking == 2)
        {
        cout << Supportive[0] << " attempts to steal " << attacked << "'s health." << endl;
        cout << " " << endl;
        attack = rand() % 2;
        dodge = rand ()%2;
        if(dodge == attack)
        {
        cout << attacked << " dodge the attack and managed to cause damage to " << Supportive[0] << "." << endl;
        cout << "50 damage!" << endl;
        cout << " " << endl;
        enemy2 = enemy2 - 50;
        }
        else if(attack != dodge)
        {
            cout << Supportive[0] << " stole 50 of health from " << attacked << "." << endl;
            cout << "50 damage!" << endl;
            strike = strike - 50;
            enemy2 = enemy2 + 50;
        }

        }
        cout << " " << endl;
        if(player == Takeo)
        {
            health = strike;
        }
        else if(player == Yuno)
        {
            Ally = strike;
        }
        else if(player == Tsubaki)
        {
            Ally2 = strike;
        }
    }

        }
        cout << " " << endl;
        cout << " " << endl;
        if(packs == 1)
        {
            allyteam = health + Ally;
        }
        else if(packs == 2)
        {
            allyteam = health + Ally + Ally2;
        }
        enemyteam = enemy1 + enemy2;
        cout << " " << endl;
        cout << " " << endl;
        if(allyteam <= 0)
        {
            turns = 100;
        }
        else if(enemyteam <= 0)
        {
            turns = 100;
        }
        system("pause");
        turns++;
        cout << "\n\n\n\n\n\n\n" << endl;
    } while (turns < 20);

        if(allyteam < enemyteam)
        {
            victory = 2;
            lvl++;
            strength = strength + 8;
        }
        else if(enemyteam <= allyteam)
        {
            victory = 1;
            lvl = lvl + 2;
            strength = strength + 25;
        }
        lvl = States(strength, Protagonist, lvl, armor, points);
        cout << " " << endl;
        return victory;
}

int DiaryFight6(int& points, int& armor, int& item, int& strength, int& lvl, int& victor, string Protagonist, string Diary[13])
{
int enemy = 2500, health = armor *(lvl/2), superattack, attack, increase = 1, timer = 0, dodge, going, Takeo = 0, Marco = 1, turn = 0, exp = 1, maxexp = 100;
do
{
    cout << "\n\n\n\n" << endl;
    cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titem:\t" << item << "." << endl;
    cout << "\n\n" << endl;
    cout << Diary[6] << "'s health:\t" << enemy << endl;
    cout << "\n\n\n" << endl;
    system("pause");
    cout << "\n\n\n\n" << endl;
    going = rand() % 2;
    if(going == Takeo)
    {
        int choice;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    pat2 = x;
     cout << x << "." << " Items" << endl;
 }
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> choice;
        } while(choice > y || choice < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(choice == 1)
        {
            superattack = 2;
            cout << Protagonist << " goes to attack " << Diary[6] << "." << endl;
            cout << " " << endl;
            attack = rand() % 4;
            dodge = rand() % 4;
            if(attack == dodge)
            {
                cout << Diary[6] << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/4);
                cout << Protagonist << " lands a hit on " << Diary[6] << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp++;
                points++;
            }
        }
        else if(choice == 2)
        {
            cout << Protagonist << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(choice == 3 && pat == choice)
        {
            superattack = 6;
            cout << Protagonist << " charges for a super strike on " << Diary[6] << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << Diary[6] << " dodged the super attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                points = points - 5;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/2);
                cout << Protagonist << " lands a deadly hit on " << Diary[6] << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp = exp + 5;
                points = points - 5;
            }
        }
        else if(choice == 3 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    string attacked = Diary[6];
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", he dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on  " << attacked << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7 * increase;
        cout << Protagonist << " goes to give a strong kick to " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << attacked << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20 * increase;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
        cout << Protagonist << " misses the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(attack != dodge)
    {
        superattack = superattack * (strength/2);
        cout << Protagonist << " lands a very deadly blow to " << attacked << "." << endl;
        cout << superattack << " damage" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 4)
{
    cout << Protagonist << " gains his ancestors strength before charging directly at " << attacked << "." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses and ends up getting fatigue." << endl;
        cout << " " << endl;
        health = health - 50;
        if(health <= 0)
        {
            health = 10;
        }
        points = points - 100;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " landed one hell of an attack." << endl;
        superattack = 10 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 5)
{
    cout << Protagonist << " goes and charges " << attacked << " with everything he got." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses his attack and gets countered by " << attacked << "." << endl;
        cout << "50 damage to " << Protagonist << "." << endl;
        cout << " " << endl;
        health = health - 50;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " land one deadly attack." << endl;
        superattack = 15 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " "<< endl;
        enemy = enemy - superattack;
    }
}
}
        else if(choice == 4 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    string attacked = Diary[6];
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", he dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on  " << attacked << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7 * increase;
        cout << Protagonist << " goes to give a strong kick to " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << attacked << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20 * increase;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
        cout << Protagonist << " misses the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(attack != dodge)
    {
        superattack = superattack * (strength/2);
        cout << Protagonist << " lands a very deadly blow to " << attacked << "." << endl;
        cout << superattack << " damage" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 4)
{
    cout << Protagonist << " gains his ancestors strength before charging directly at " << attacked << "." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses and ends up getting fatigue." << endl;
        cout << " " << endl;
        health = health - 50;
        if(health <= 0)
        {
            health = 10;
        }
        points = points - 100;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " landed one hell of an attack." << endl;
        superattack = 10 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 5)
{
    cout << Protagonist << " goes and charges " << attacked << " with everything he got." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses his attack and gets countered by " << attacked << "." << endl;
        cout << "50 damage to " << Protagonist << "." << endl;
        cout << " " << endl;
        health = health - 50;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " land one deadly attack." << endl;
        superattack = 15 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " "<< endl;
        enemy = enemy - superattack;
    }
}
}
     else if(choice == 3 && choice == pat2)
     {
         int let;
         if(timer == 0 && item >= 5)
         {
             cout << " " << endl;
             cout << "What would you like to do?" << endl;
             cout << " " << endl;
             cout << "1. Heal.       1" << endl;
             cout << "2. Strength.   5" << endl;
             cout << "\n\n" << endl;
             do
             {
                 cin >> let;
             } while(let > 2 || let < 1);
         }
         else if(timer != 0 || item < 5)
         {
             let = 1;
         }
         cout << "\n\n" << endl;
         if(let == 1)
         {
             item--;
             cout << Protagonist << " heals himself." << endl;
             cout << "His health was " << health;
             health = health + 95;
             cout << ", now it is " << health << "." << endl;
             cout << " " << endl;
         }
         else if(let == 2)
         {
             item = item - 5;
             cout << Protagonist << " increased his strength!" << endl;
             cout << " " << endl;
             increase = 5;
             timer = 21;
         }
     }
           else if(choice == 4 && choice == pat2)
     {
         int let;
         if(timer == 0 && item >= 5)
         {
             cout << " " << endl;
             cout << "What would you like to do?" << endl;
             cout << " " << endl;
             cout << "1. Heal.       1" << endl;
             cout << "2. Strength.   5" << endl;
             cout << "\n\n" << endl;
             do
             {
                 cin >> let;
             } while(let > 2 || let < 1);
         }
         else if(timer != 0 || item < 5)
         {
             let = 1;
         }
         cout << "\n\n" << endl;
         if(let == 1)
         {
             item--;
             cout << Protagonist << " heals himself." << endl;
             cout << "His health was " << health;
             health = health + 95;
             cout << ", now it is " << health << "." << endl;
             cout << " " << endl;
         }
         else if(let == 2)
         {
             item = item - 5;
             cout << Protagonist << " increased his strength!" << endl;
             cout << " " << endl;
             increase = 5;
             timer = 21;
         }
     }
          else if(choice == 5 && choice == pat2)
     {
         int let;
         if(timer == 0 && item >= 5)
         {
             cout << " " << endl;
             cout << "What would you like to do?" << endl;
             cout << " " << endl;
             cout << "1. Heal.       1" << endl;
             cout << "2. Strength.   5" << endl;
             cout << "\n\n" << endl;
             do
             {
                 cin >> let;
             } while(let > 2 || let < 1);
         }
         else if(timer != 0 || item < 5)
         {
             let = 1;
         }
         cout << "\n\n" << endl;
         if(let == 1)
         {
             item--;
             cout << Protagonist << " heals himself." << endl;
             cout << "His health was " << health;
             health = health + 95;
             cout << ", now it is " << health << "." << endl;
             cout << " " << endl;
         }
         else if(let == 2)
         {
             item = item - 5;
             cout << Protagonist << " increased his strength!" << endl;
             cout << " " << endl;
             increase = 5;
             timer = 21;
         }
     }


     cout << " " << endl;
     if(timer <= 5 || timer >= 1)
     {
         timer--;
         increase = 1;

     }
     else if(timer > 5)
     {
         increase = 5;
     }
    }
    else if(going == Marco)
    {
        cout << " " << endl;
        cout << Diary[6] << " turn." << endl;
        cout << " " << endl;
        cout << Diary[6] << " goes to strike down " << Protagonist << "." << endl;
        cout << " " << endl;
        attack = rand() % 3;
        dodge = rand() % 3;
        if(dodge == attack)
        {
            cout << Diary[6] << " misses his attack on " << Protagonist << "." << endl;
            cout << "0 damage." << endl;
            cout << " " << endl;
            exp = exp + 10;
            points = points + 5;
        }
        else if(dodge != attack)
        {
            cout << Diary[6] << " lands a heavy blow to " << Protagonist << "." << endl;
            cout << "75 damage!" << endl;
            cout << " " << endl;
            health = health - 75;
            attack = rand() % 2;
            dodge = rand() % 2;
            if(dodge == attack)
            {
                cout << " " << endl;
            }
            else if(dodge != attack)
            {
                cout << Diary[6] << " manages to land an extra hit." << endl;
                cout << "25 damage!" << endl;
                cout << " " << endl;
                health = health - 25;
            }
        }
    }
    cout << " " << endl;
    system("pause");
    cout << " " << endl;
    cout << " " << endl;
    turn++;
if(health <= 0)
{
    armor = 0;
    return armor;
}
else if(enemy <= 0)
{
    turn = 30;
}
}while(turn < 20);
if(health >= enemy)
{
    victor = 1;
}
else if(health < enemy)
{
    victor = 2;
}
lvl = States(strength, Protagonist, lvl, armor, points);

}

int DiaryFight6A(int& points, int& armor, int& item, int& strength, int& lvl, int& victor, string Protagonist, string Diary[13])
{
int enemy = 2000, health = armor *(lvl/2), superattack, attack, increase = 1, timer = 0, dodge, going, Takeo = 0, Ai = 1, turn = 0, exp = 1, maxexp = 100;
do
{
    cout << "\n\n\n\n" << endl;
    cout << Protagonist << "'s health:\t" << health << "\tpoints:\t" << points << "\titem:\t" << item << "." << endl;
    cout << "\n\n" << endl;
    cout << Diary[7] << "'s health:\t" << enemy << endl;
    cout << "\n\n\n" << endl;
    system("pause");
    cout << "\n\n\n\n" << endl;
    going = rand() % 2;
    if(going == Takeo)
    {
        int choice;
        cout << Protagonist << " turn." << endl;
        cout << "\n\n\n" << endl;
        int x = 1, y = 1, pat, pat1, pat2, maxi = 1;
        cout << x << "." << " Attack" << endl;
        x++;
        y++;
        cout << x << "." << " Dodge" << endl;
        if(points >= 5)
        {
            x++;
            pat = x;
            y++;
            cout << x << "." << " Super Attack\t5" << endl;
        }
 else if (lvl >= 6)
 {
     x++;
     pat1 = x;
     maxi++;
     cout << x << "." << " Combos" << endl;
 }
 else if (item >= 1)
 {
     x++;
     maxi++;
    pat2 = x;
     cout << x << "." << " Items" << endl;
 }
        cout << " " << endl;
        cout << " " << endl;
        do
        {
            cin >> choice;
        } while(choice > y || choice < 1);
        cout << " " << endl;
        cout << " " << endl;
        if(choice == 1)
        {
            superattack = 2;
            cout << Protagonist << " goes to attack " << Diary[7] << "." << endl;
            cout << " " << endl;
            attack = rand() % 4;
            dodge = rand() % 4;
            if(attack == dodge)
            {
                cout << Diary[7] << " dodged the attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/4);
                cout << Protagonist << " lands a hit on " << Diary[7] << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp++;
                points++;
            }
        }
        else if(choice == 2)
        {
            cout << Protagonist << " skips his turn." << endl;
            cout << " " << endl;
        }
        else if(choice == 3 && pat == choice)
        {
            superattack = 6;
            cout << Protagonist << " charges for a super strike on " <<  Diary[7] << "." << endl;
            cout << " " << endl;
            attack = rand() % 3;
            dodge = rand() % 3;
            if(attack == dodge)
            {
                cout << Diary[7] << " dodged the super attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
                points = points - 5;
            }
            else if(attack != dodge)
            {
                superattack = superattack*(increase*strength/2);
                cout << Protagonist << " lands a deadly hit on " << Diary[7] << "." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
                enemy = enemy - superattack;
                exp = exp + 5;
                points = points - 5;
            }
        }
        else if(choice == 3 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
    string attacked = Diary[7];
    if (lvl == 6)
    {
    cout << y << "." << " Breaking guard!" << endl;
    }
    else if (lvl >= 7 || lvl <= 10)
    {
    y++;
    maxi++;
    cout << y << "." << " Forceful kick!" << endl;
    }
        else if (lvl >= 11 || lvl <= 15)
    {
    y++;
    maxi++;
    cout << y << "." << " Deadly blow!" << endl;
    }
    else if (lvl >= 16 || lvl <= 20)
    {
        y++;
        maxi++;
        cout << y << "." << " Power of the Aiwaka family!" << endl;
    }
    else if(lvl >= 21 || lvl <= 25)
    {
        y++;
        maxi++;
        cout << y << "." << " Strong will!" << endl;
    }
    cout << "\n\n" << endl;
    do
    {
        cin >> combo;
    } while (combo > maxi || combo < 1);
    cout << " " << endl;
    cout << " " << endl;
    if (combo == 1)
    {
        superattack = 5 * increase;
        cout << Protagonist << " goes to strike down " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 4;
        dodge = rand() % 4;
        if (attack == dodge)
        {
         cout << "Lucky for " << attacked << ", she dodges the attack." << endl;
         cout << "0 damage." << endl;
         cout << " " << endl;
         int counter;
         counter = rand() % 5;
         if (counter >= attack)
         {
             cout << attacked << " takes the opportunity to attack " << Protagonist << "." << endl;
             cout << " " << endl;
             system("pause");
             cout << "\n\n" << endl;
             attack = rand() % 3;
             dodge = rand() % 3;
             if (attack == dodge)
             {
                 cout << Protagonist << " manages to dodge the attack." << endl;
                 cout << "0 damage." << endl;
                 cout << " " << endl;
                 points++;
             }
             else if (attack != dodge)
             {
                 cout << attacked << " lands a hit on " << Protagonist << "." << endl;
                 cout << "5 damage!" << endl;
                 cout << " " << endl;
                 health = health - 5;
             }
         }
        }
        else if (dodge != attack)
        {
            superattack = superattack * (strength/4);
            cout << Protagonist << "'s first attack manages to land a hit on " << attacked << "." << endl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
            int attack2, dodge2;
            attack2 = rand() % 2;
            dodge2 = rand() % 2;
            if (attack2 == dodge2)
            {
                cout << Protagonist << " missed his second attack." << endl;
                cout << "0 damage." << endl;
                cout << " " << endl;
            }
            else if (attack2 != dodge2)
            {
                enemy = enemy - superattack;
                cout << Protagonist << "'s second attack manages to hit on  " << attacked << " as well." << endl;
                cout << superattack << " damage!" << endl;
                cout << " " << endl;
            }
        }
    }
    else if (combo == 2)
    {
        superattack = 7 * increase;
        cout << Protagonist << " goes to give a strong kick to " << attacked << "." << endl;
        cout << " " << endl;
        attack = rand() % 5;
        dodge = rand() % 5;
        if (attack == dodge)
        {
            cout << Protagonist << " missed!" << endl;
            cout << "0 damage!" << endl;
            cout << " " << endl;
        }
        else if (attack != dodge)
        {
            cout << Protagonist << " kicked " << attacked << "." << endl;
            superattack = superattack*lvl;
            cout << superattack << " damage!" << endl;
            cout << " " << endl;
            enemy = enemy - superattack;
        }
    }
else if(combo == 3)
{
    cout << Protagonist << " goes to give a deadly blow." << endl;
    cout << " " << endl;
    superattack = 20 * increase;
    attack = rand() % 10;
    dodge = rand() % 10;
    if(attack == dodge)
    {
        cout << Protagonist << " misses the attack." << endl;
        cout << "0 damage." << endl;
        cout << " " << endl;
    }
    else if(attack != dodge)
    {
        superattack = superattack * (strength/2);
        cout << Protagonist << " lands a very deadly blow to " << attacked << "." << endl;
        cout << superattack << " damage" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 4)
{
    cout << Protagonist << " gains his ancestors strength before charging directly at " << attacked << "." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses and ends up getting fatigue." << endl;
        cout << " " << endl;
        health = health - 50;
        if(health <= 0)
        {
            health = 10;
        }
        points = points - 100;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " landed one hell of an attack." << endl;
        superattack = 10 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " " << endl;
        enemy = enemy - superattack;
    }
}
else if(combo == 5)
{
    cout << Protagonist << " goes and charges " << attacked << " with everything he got." << endl;
    cout << " " << endl;
    attack = rand() % 3;
    dodge = rand() % 3;
    if(attack == dodge)
    {
        cout << Protagonist << " misses his attack and gets countered by " << attacked << "." << endl;
        cout << "50 damage to " << Protagonist << "." << endl;
        cout << " " << endl;
        health = health - 50;
    }
    else if(attack != dodge)
    {
        cout << Protagonist << " land one deadly attack." << endl;
        superattack = 15 * increase;
        superattack = superattack * (strength/lvl);
        cout << superattack << " damage!" << endl;
        cout << " "<< endl;
        enemy = enemy - superattack;
    }
}
}
        else if(choice == 4 && choice == pat1)
    {
    cout << "\n\n\n" << endl;
    int combo, y = 1, maxi = 1;
